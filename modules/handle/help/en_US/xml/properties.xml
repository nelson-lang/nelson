  <language>en_US</language>
  <keyword>properties</keyword>
  <short_description
  >Returns the properties name of an object handle.</short_description>

  <syntax>
    <syntax_item>properties(h)</syntax_item>
    <syntax_item>c = properties(h)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>h</param_name>
      <param_description>a handle object</param_description>
    </param_input_item>
  </param_input>

  <param_output>
    <param_output_item>
      <param_name>c</param_name>
      <param_description>a cell of strings</param_description>
    </param_output_item>
  </param_output>

  <description><b
    >properties</b> returns a cell of strings with properties name.</description>

