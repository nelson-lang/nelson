  <language>en_US</language>
  <keyword>poheader</keyword>
  <short_description>Generates po file header.</short_description>

  <syntax>
    <syntax_item>ce = poheader(domain, language)</syntax_item>
  </syntax>
  
  <param_input>

    <param_input_item>
      <param_name>domain</param_name>
      <param_description>a string: domain message.</param_description>
    </param_input_item>

    <param_input_item>
      <param_name>language</param_name>
      <param_description
      >a string: language, examples 'en_US' or 'fr_FR'.</param_description>
    </param_input_item>
    
  </param_input>

  <param_output>

    <param_output_item>
      <param_name>ce</param_name>
      <param_description>a cell of string: po file header.</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p><b>ce = poheader(domain, language)</b> generates po file header.</p>
</description>


  <used_function />
  <bibliography />

<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data><![CDATA[poheader('nelson', 'en_US')]]>
    </example_item_data>
  </example_item>

</examples>

