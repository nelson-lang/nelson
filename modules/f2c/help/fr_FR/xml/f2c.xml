  <language>fr_FR</language>
  <keyword>f2c</keyword>
  <short_description>Convertisseur Fortran vers C.</short_description>
  <syntax>
    <syntax_item>f2c(src, dest)</syntax_item>
    <syntax_item>r = f2c(src, dest)</syntax_item>
    <syntax_item>[r, msg] = f2c(src, dest)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>src</param_name>
      <param_description
      >une chaîne : fichier source Fortran.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>dest</param_name>
      <param_description
      >une chaîne : répertoire de destination.</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>r</param_name>
      <param_description>un booléen : true si succès.</param_description>
    </param_output_item>
    <param_output_item>
      <param_name>msg</param_name>
      <param_description
      >une chaîne : message d'erreur ou ''.</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p><b>f2c</b> convertit les fichiers Fortran 66 et Fortran 77 en code C.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[f2c([modulepath(nelsonroot(),'f2c','root'), '/tests/dgemm.f'], tempdir());
fileread([tempdir(), 'dgemm.c'])]]></example_item_data>
    </example_item>
  </examples>
