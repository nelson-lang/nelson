  <language>en_US</language>
  <keyword>cd</keyword>
  <short_description>Changes Nelson current directory.</short_description>
  <syntax>
    <syntax_item>cd(dirname)</syntax_item>
    <syntax_item>cd dirname</syntax_item>
    <syntax_item>previous_path = cd(dirname)</syntax_item>
    <syntax_item>cd ..</syntax_item>
    <syntax_item>cd</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>dirname</param_name>
      <param_description>a string: directory name to move.</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>previous_path</param_name>
      <param_description>a string: previous directory.</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p>Changes the current working directory to dirname.</p>
    <p><b
      >a = cd()</b> without input argument returns the current working directory.</p>
    <p><b
      >cd()</b> without input argument displays the current working directory.</p>
    <p />
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[previous = cd(tempdir())
cd
cd ..
]]></example_item_data>
    </example_item>
  </examples>
