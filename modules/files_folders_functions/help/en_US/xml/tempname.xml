	<language>en_US</language>
	<keyword>tempname</keyword>
	<short_description>Returns an unique temporary filename.</short_description>

	<syntax>
		<syntax_item>f = tempname()</syntax_item>
		<syntax_item>f = tempdir(path)</syntax_item>
	</syntax>
	
	<param_input>
    	<param_input_item>
      	<param_name>path</param_name>
      	<param_description>a string: an existing directory used instead of tempdir().</param_description>
    	</param_input_item>
  	</param_input>


	<param_output>
		<param_output_item>
			<param_name>f</param_name>
			<param_description>a string: an unique temporary filename.</param_description>
		</param_output_item>
	</param_output>

	<description>
		<p>Returns the name of an unique temporary filename.</p>
	</description>

	<used_function></used_function>
