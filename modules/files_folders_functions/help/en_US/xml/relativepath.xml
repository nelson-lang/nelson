	<language>en_US</language>
	<keyword>relativepath</keyword>
	<short_description>Returns the relative path from an actual path to the target path.</short_description>

	<syntax>
		<syntax_item>r = relativepath(path_1, path_2)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>path_1</param_name>
			<param_description>a string: file or directory.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>path_2</param_name>
			<param_description>a string: file or directory.</param_description>
		</param_input_item>
	</param_input>
	
	<param_output>
		<param_output_item>
			<param_name>r</param_name>
			<param_description>a string: relative path.</param_description>
		</param_output_item>

	</param_output>

	<description>
<p>Returns the relative path from an actual path to the target path.</p>
</description>


	<used_function></used_function>
	<bibliography></bibliography>

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[relativepath(nelsonroot(), [nelsonroot(), '/COPYING.LGPLv2.1'])
relativepath(nelsonroot(), [nelsonroot(), '/etc/finish.m'])
relativepath([nelsonroot(),'/bin'], [nelsonroot(), '/COPYING.LGPLv2.1'])
relativepath('.', '.')
relativepath('.', '..')
relativepath('..', '.')]]>
		</example_item_data>
	
	</example_item>

</examples>

