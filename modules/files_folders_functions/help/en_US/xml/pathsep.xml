	<language>en_US</language>
	<keyword>pathsep</keyword>
	<short_description
  >Return the search path separator character for the current platform.</short_description>

	<syntax>
		<syntax_item>res = pathsep()</syntax_item>
	</syntax>
	

	<param_output>
		<param_output_item>
			<param_name>res</param_name>
			<param_description>a string: ';' or ':'</param_description>
		</param_output_item>
	</param_output>

	<description><b
    >pathsep</b> returns ';' on Windows and ':' on others platforms.</description>

