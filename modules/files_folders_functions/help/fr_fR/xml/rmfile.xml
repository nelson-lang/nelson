  <language>fr_FR</language>
  <keyword>rmfile</keyword>
  <short_description>Supprime un fichier.</short_description>

  <syntax>
    <syntax_item>rmfile(filename)</syntax_item>
    <syntax_item>res = rmfile(filename)</syntax_item>
    <syntax_item>[res, msg] = rmfile(filename)</syntax_item>
    <syntax_item>[res, msg] = rmfile(filename)</syntax_item>

  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>filename</param_name>
      <param_description>a string: nom du fichier.</param_description>
    </param_input_item>
  </param_input>

  <param_output>

    <param_output_item>
  <param_name>res</param_name>
  <param_description>un bool√©en: true ou false.</param_description>
    </param_output_item>
    <param_output_item>
  <param_name>msg</param_name>
  <param_description>a string: message d'erreur ou ''.</param_description>
    </param_output_item>

  </param_output>

  <description>
  <p><b>res = rmfile(filename)</b> supprime le fichier <b>filename</b>.</p>
  </description>


  <used_function />
  <bibliography />

<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data
        runnable="cli"
      ><![CDATA[fd = fopen([tempdir(), 'test_rmfile.txt'], 'wt')
fclose(fd)
isfile([tempdir(), 'test_rmfile.txt'])
rmfile([tempdir(), 'test_rmfile.txt'])
isfile([tempdir(), 'test_rmfile.txt'])
]]>
    </example_item_data>
  
  </example_item>

</examples>

