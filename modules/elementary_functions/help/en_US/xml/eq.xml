	<language>en_US</language>
	<keyword>eq</keyword>
	<short_description>equality, == operator.</short_description>

	<syntax>
		<syntax_item>C = eq(A, B)</syntax_item>
		<syntax_item>C = (A == B)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>A</param_name>
			<param_description>a variable</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>B</param_name>
			<param_description>a variable</param_description>
		</param_input_item>
	</param_input>

	<param_output>

		<param_output_item>
			<param_name>C</param_name>
			<param_description>result of A == B</param_description>
		</param_output_item>
	</param_output>

	<description>
		<p><b>C = eq(A, B)</b> returns a logical array with elements set to logical <b>true</b> where arrays A and B are equals.</p>
		<p></p>
	</description>


	<used_function></used_function>
	<bibliography></bibliography>

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[eye(2,2) == ones(2, 2)]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[0 == i]]>
		</example_item_data>
	</example_item>


	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA['Nelson' == 'Noslen']]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA["Nelson" == "Noslen"]]>
		</example_item_data>
	</example_item>


	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA['Nelson' == 'l']]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[eq(0.8-0.6-0.2, 0)]]>
		</example_item_data>
	</example_item>

</examples>

