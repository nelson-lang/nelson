  <language>en_US</language>
  <keyword>hypot</keyword>
  <short_description>Square root of sum of squares</short_description>

  <syntax>
    <syntax_item>C = hypot(A, B)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>A</param_name>
      <param_description
      >a variable: scalars, vectors, matrices, multidimensional arrays single or double</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>B</param_name>
      <param_description
      >a variable: scalars, vectors, matrices, multidimensional arrays single or double</param_description>
    </param_input_item>

  </param_input>

  <param_output>

    <param_output_item>
      <param_name>R</param_name>
      <param_description>result of hypot: hypothenuse.</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p><b>hypot</b> computes the hypothenuse.</p>
    <p>If one or both inputs is NaN, then <b>hypot</b> returns <b>NaN</b>.</p>
  </description>


  <used_function />
  <bibliography />

