  <language>en_US</language>
  <keyword>bin2dec</keyword>
  <short_description>Convert number in base 2 to decimal.</short_description>

  <syntax>
    <syntax_item>D = bin2dec(TXT)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>TXT</param_name>
      <param_description>a char array.</param_description>
    </param_input_item>
  </param_input>

  <param_output>

    <param_output_item>
      <param_name>D</param_name>
      <param_description
      >result of bin2dec: an integer value.</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p><b>bin2dec</b> converts number in base 2 to decimal.</p>
    <p>Note:</p>
    <p> - <b>bin2dec</b> and <b>dec2bin</b> are inverses of one another.</p> 
  </description>

  <used_function />
  <bibliography />

<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data>
      <![CDATA[bin2dec('11')]]>
    </example_item_data>
  </example_item>

</examples>

