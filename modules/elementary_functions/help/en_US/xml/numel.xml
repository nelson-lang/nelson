    <language>en_US</language>
    <keyword>numel</keyword>
    <short_description>Number of elements.</short_description>
    <syntax>
        <syntax_item>nbel = numel(M)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>M</param_name>
            <param_description>a variable</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>nbel</param_name>
            <param_description>the number of elements.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>Return the number of elements in the object M.</p>
    </description>
    <used_function />
    <bibliography />
    <examples>
        <example_item>
            <example_item_type>nelson</example_item_type>
            <example_item_description />
            <example_item_data
      ><![CDATA[numel(ones(3, 0))
numel(ones(3,4))
numel(ones(3,4,5))]]></example_item_data>
        </example_item>
    </examples>
