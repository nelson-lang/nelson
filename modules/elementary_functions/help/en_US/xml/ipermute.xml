    <language>en_US</language>
    <keyword>ipermute</keyword>
    <short_description>Inverse permute array dimensions.</short_description>
    <syntax>
        <syntax_item>R = ipermute(A, order)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>A</param_name>
            <param_description>an array.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>order</param_name>
            <param_description>Dimension order: row vector</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>R</param_name>
            <param_description
      >result array rearranged with new dimension order.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <b
    >ipermute</b> permutes the dimensions of an array (in inverse order of <b
    >permute</b>).</description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[x = [1 2 3; 4 5 6]
y = permute(x,[3 1 2])
x2 = ipermute(y,[3 1 2])]]></example_item_data>
            </example_item>
        </examples>
