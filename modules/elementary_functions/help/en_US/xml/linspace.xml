  <language>en_US</language>
  <keyword>linspace</keyword>
  <short_description>linearly spaced vector constructor.</short_description>

  <syntax>
    <syntax_item>V = linspace(s, e)</syntax_item>
    <syntax_item>V = linspace(s, e, n)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>s</param_name>
      <param_description
      >first value: a scalar, single or double.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>e</param_name>
      <param_description
      >last value: a scalar, single or double.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>n</param_name>
      <param_description
      >Number of points: a scalar, single or double (by default 100).</param_description>
    </param_input_item>

  </param_input>

  <param_output>

    <param_output_item>
      <param_name>V</param_name>
      <param_description
      >result of linspace: an linearly spaced vector.</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p><b>linspace</b> generates an linearly spaced vector.</p>
  </description>

  <used_function />
  <bibliography />

<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data>
      <![CDATA[V = linspace(1+2i, 10+10i, 4)]]>
    </example_item_data>
  </example_item>

</examples>

