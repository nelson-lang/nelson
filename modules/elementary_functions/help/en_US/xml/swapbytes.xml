  <language>en_US</language>
  <keyword>swapbytes</keyword>
  <short_description>Swap byte ordering.</short_description>

  <syntax>
    <syntax_item>R = swapbytes(M)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>M</param_name>
      <param_description
      >a variable: integer, single or double real full matrix.</param_description>
    </param_input_item>
  </param_input>

  <param_output>

    <param_output_item>
      <param_name>R</param_name>
      <param_description
      >result of swapbytes: reversed byte order of M.</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p><b>swapbytes</b> Swap byte ordering.</p>
    <p>endian (little - big) converter</p>
  </description>

  <used_function />
  <bibliography />

<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data>
      <![CDATA[X = uint16([65535 128; 1 0])
Y = swapbytes(X)]]>
    </example_item_data>
  </example_item>


</examples>

