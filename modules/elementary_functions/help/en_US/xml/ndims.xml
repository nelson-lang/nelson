  <language>en_US</language>
  <keyword>ndims</keyword>
  <short_description>Number of dimensions of an array.</short_description>

  <syntax>
    <syntax_item>n = ndims(M)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>M</param_name>
      <param_description>a variable</param_description>
    </param_input_item>
  </param_input>

  <param_output>

    <param_output_item>
      <param_name>n</param_name>
      <param_description
      >a integer value: Number of dimensions of M.</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p><b>n = ndims(M)</b> return the number of dimension of the array <b
      >M</b>.</p>
    <p><b>M</b> is greater than or equal to 2.</p>
  </description>


  <used_function />
  <bibliography />

<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data>
      <![CDATA[ndims(ones(3, 0))
ndims(3)
ndims([1 2 3 4 5])
ndims(ones(3, 4, 5))]]>
    </example_item_data>
  </example_item>


</examples>

