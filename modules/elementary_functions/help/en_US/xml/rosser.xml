  <language>en_US</language>
  <keyword>rosser</keyword>
  <short_description
  >Classic symmetric eigenvalue test problem.</short_description>

  <syntax>
    <syntax_item>R = rosser()</syntax_item>
    <syntax_item>R = rosser(classname)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>classname</param_name>
      <param_description
      >row character vector or scalar string: class name desired ('double' by default).</param_description>
    </param_input_item>
  </param_input>

  <param_output>

    <param_output_item>
      <param_name>R</param_name>
      <param_description>Rosser Matrix.</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p><b>R = rosser()</b> returns the Rosser Matrix.</p>
  </description>

  <used_function />
  <bibliography>https://archive.org/details/jresv47n4p291</bibliography>

