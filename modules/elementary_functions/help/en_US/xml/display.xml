	<language>en_US</language>
	<keyword>display</keyword>
	<short_description>Show information about variable or result of expression.</short_description>

	<syntax>
		<syntax_item>display(V)</syntax_item>
		<syntax_item>display(V, name)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>V</param_name>
			<param_description>Result of executing a statement or expression</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>name</param_name>
			<param_description>a character vector: variable name displayed.</param_description>
		</param_input_item>
	</param_input>

	<description>
		<p><b>display(V)</b> displays information about the variable <b>V</b>.</p>
		<p>Nelson calls <b>display</b> function whenever an object is referred to in a statement that is not terminated by a semicolon.</p>
	</description>

	<used_function></used_function>
	<bibliography></bibliography>

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[display(33, 'Hello')]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[display('Hello Nelson')]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[display(pi)]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[A = eye(3, 3); disp(A)]]>
		</example_item_data>
	</example_item>


</examples>

