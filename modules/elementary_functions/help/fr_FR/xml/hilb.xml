    <keyword>hilb</keyword>
    <short_description>Matrice de Hilbert</short_description>
    <syntax>
        <syntax_item>h = invhilb(n)</syntax_item>
        <syntax_item>h = invhilb(n, className)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
        <param_name>n</param_name>
        <param_description>un scalaire, entier non négatif.</param_description>
        </param_input_item>
        <param_input_item>
        <param_name>className</param_name>
        <param_description
      >'single' ou 'double' (par défaut).</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>h</param_name>
            <param_description>Matrice de Hilbert.</param_description>
        </param_output_item>
    </param_output>
        <description>
        <p>
            <b>hilb</b> calcule la matrice de Hilbert.</p>
        </description>
        <used_function />
        <bibliography
  >https://en.wikipedia.org/wiki/David_Hilbert, and Thanks to https://nhigham.com/2020/06/30/what-is-the-hilbert-matrix/</bibliography>
