  <language>fr_FR</language>
  <keyword>istril</keyword>
  <short_description
  >Tester si une matrice est triangulaire inférieure</short_description>
  <syntax>
    <syntax_item>tf = istril(M)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>M</param_name>
      <param_description>un tableau numérique</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>tf</param_name>
      <param_description>booléen : résultat de 'istril'.</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p><b
      >istril</b> renvoie un scalaire booléen si la matrice est triangulaire inférieure.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[A = eye(3, 3);
R = istril(A)
R = istril(A(:,1))]]></example_item_data>
    </example_item>
  </examples>
