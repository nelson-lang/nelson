    <language>en_US</language>
    <keyword>wilkinson</keyword>
    <short_description>Wilkinson's eigenvalue test matrix</short_description>
    <syntax>
        <syntax_item>W = wilkinson(n)</syntax_item>
        <syntax_item>W = wilkinson(n, classname)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>n</param_name>
            <param_description>scalar integer value: order.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>classname</param_name>
            <param_description
      >row character vector or scalar string: class name desired ('double' by default).</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>W</param_name>
            <param_description
      >Wilkinson's eigenvalue test matrix.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>W = wilkinson(n)</b> returns the wilkinson Matrix of order <b
      >n</b>.</p>
        </description>
        <used_function />
        <bibliography
  >https://en.wikipedia.org/wiki/Wilkinson_matrix</bibliography>
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[W = wilkinson(4)]]></example_item_data>
            </example_item>
        </examples>
        <see_also>
            <see_also_item>
                <link linkend="${constructors_functions}diag">diag</link>
            </see_also_item>
        </see_also>
