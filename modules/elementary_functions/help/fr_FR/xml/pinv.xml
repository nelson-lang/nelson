    <language>en_US</language>
    <keyword>pinv</keyword>
    <short_description>Moore-Penrose pseudoinverse</short_description>
    <syntax>
        <syntax_item>y = pinv(A)</syntax_item>
        <syntax_item>y = pinv(A, tol)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>A</param_name>
            <param_description>matrix: input matrix</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>tol</param_name>
            <param_description
      >scalar: singular value tolerance</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>y</param_name>
            <param_description
      >Moore-Penrose Pseudoinverse of matrix A.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>pinv</b> returns Moore-Penrose Pseudoinverse of matrix A.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[A = [1, 2, 3; 4, 5, 6];
R = pinv(A)
R = pinv(A, 2)]]></example_item_data>
            </example_item>
        </examples>
        <see_also>
