    <language>fr_FR</language>
    <keyword>isdiag</keyword>
    <short_description>Vérifie si une matrice est diagonale.</short_description>
    <syntax>
        <syntax_item>tf = isdiag(M)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>M</param_name>
            <param_description>un tableau numérique</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>tf</param_name>
            <param_description
      >booléen : résultat de 'isdiag'.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b
      >isdiag</b> renvoie un scalaire booléen si la matrice est diagonale.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[A = eye(3, 3);
R = isdiag(A)
R = isdiag(A(:,1))]]></example_item_data>
            </example_item>
        </examples>
