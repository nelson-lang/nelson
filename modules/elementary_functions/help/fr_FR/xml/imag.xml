    <language>fr_FR</language>
    <keyword>imag</keyword>
    <short_description
  >Partie imaginaire d'un nombre complexe.</short_description>
    <syntax>
        <syntax_item>im = imag(M)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>M</param_name>
            <param_description>une variable</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>R</param_name>
            <param_description
      >partie imaginaire des éléments du tableau complexe M.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>R = imag(M)</b> Renvoie la partie imaginaire de M.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[cplx = 22+34*i;
r = imag(cplx)]]></example_item_data>
            </example_item>
        </examples>
