  <language>fr_FR</language>
  <keyword>factorial</keyword>
  <short_description>Fonction factorielle</short_description>
  <syntax>
    <syntax_item>R = factorial(M)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>M</param_name>
      <param_description
      >un entier, matrice réelle single ou double.</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>R</param_name>
      <param_description
      >résultat de la fonction factorielle.</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p><b
      >factorial</b> calcule la fonction factorielle : le produit des entiers 1 * 2 * ... * M.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[R = factorial([1:10])
R = factorial(int8(4))]]></example_item_data>
    </example_item>
  </examples>
