	<language>en_US</language>
	<keyword>siogetvariable</keyword>
	<short_description>Emit an value of variable to web browser.</short_description>

	<syntax>
		<syntax_item>siogetvariable(variable_name)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>variable_name</param_name>
			<param_description>a string: name of variable</param_description>
		</param_input_item>
	</param_input>


	<description><p><b
      >siogetvariable</b> send value of a variable to web browser.</p></description>


	<used_function />
	<bibliography />
	
<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[A = eye(3, 3); siogetvariable('A')]]>
		</example_item_data>
	</example_item>

</examples>

