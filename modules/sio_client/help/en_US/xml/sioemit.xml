	<language>en_US</language>
	<keyword>sioemit</keyword>
	<short_description>Emit an event to web client.</short_description>

	<syntax>
		<syntax_item>sioemit(name, message)</syntax_item>
		<syntax_item>sioemit(name)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>name</param_name>
			<param_description>a string: event name</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>message</param_name>
			<param_description>a string: message to emit</param_description>
		</param_input_item>

	</param_input>


	<description><p><b>sioemit</b> emits an event to client.</p></description>


	<used_function />
	<bibliography />
	
<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[sioemit('event_demo', jsonencode(eye(3,3)))]]>
		</example_item_data>
	</example_item>

</examples>

