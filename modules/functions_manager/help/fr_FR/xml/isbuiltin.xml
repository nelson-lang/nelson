    <language>fr_FR</language>
    <keyword>isbuiltin</keyword>
    <short_description
  >Vérifie l'existence d'une fonction intégrée.</short_description>
    <syntax>
        <syntax_item>tf = isbuiltin(name)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>name</param_name>
            <param_description
      >une chaîne : nom de fonction intégrée.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>tf</param_name>
            <param_description
      >un booléen : vrai si la fonction intégrée existe.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>isbuiltin</b> vérifie l'existence d'une fonction intégrée.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[isbuiltin('isbuiltin')
isbuiltin('exist')
ismacro('exist')]]></example_item_data>
            </example_item>
        </examples>
