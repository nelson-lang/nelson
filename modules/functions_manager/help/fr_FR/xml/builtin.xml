    <language>fr_FR</language>
    <keyword>builtin</keyword>
    <short_description>Exécute une fonction intégrée.</short_description>
    <syntax>
        <syntax_item>builtin(function_name; x1, ..., xn)</syntax_item>
        <syntax_item>builtin(function_handle; x1, ..., xn)</syntax_item>
        <syntax_item
    >[r1, ..., rn] = builtin(function_name, x1, ..., xn)</syntax_item>
        <syntax_item
    >[r1, ..., rn] = builtin(function_handle, x1, ..., xn)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>function_name</param_name>
            <param_description>une chaîne : nom de fonction.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>function_handle</param_name>
            <param_description>un handle de fonction.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>x1, ..., xn</param_name>
            <param_description
      >arguments d'entrée de la fonction intégrée.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>r1, ..., rn</param_name>
            <param_description
      >arguments de sortie retournés par la fonction intégrée</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b
      >builtin</b> appelle la fonction intégrée de base décrite par son nom ou handle de fonction et arguments d'entrée.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[a = builtin('cos', 0)
b = builtin(str2func('cos'), 0)]]></example_item_data>
            </example_item>
        </examples>
