  <language>en_US</language>
  <keyword>ismex</keyword>
  <short_description
  >Check for the existence of a mex function.</short_description>
  <syntax>
    <syntax_item>tf = ismex(name)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>name</param_name>
      <param_description>a string: mex function name.</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>tf</param_name>
      <param_description>a logical: true if mex exists.</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p><b>ismex</b> checks for the existence of a mex function.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[ismex('isbuiltin')
ismex('exist')
ismex('exist')]]></example_item_data>
    </example_item>
  </examples>
