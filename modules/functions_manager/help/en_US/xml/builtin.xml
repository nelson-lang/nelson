	<language>en_US</language>
	<keyword>builtin</keyword>
	<short_description>Executes built-in function.</short_description>

	<syntax>
		<syntax_item>builtin(function_name; x1, ..., xn)</syntax_item>
		<syntax_item>builtin(function_handle; x1, ..., xn)</syntax_item>
		<syntax_item>[r1, ..., rn] = builtin(function_name, x1, ..., xn)</syntax_item>
		<syntax_item>[r1, ..., rn] = builtin(function_handle, x1, ..., xn)</syntax_item>

	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>function_name</param_name>
			<param_description>a string: function name.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>function_handle</param_name>
			<param_description>a function handle.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>x1, ..., xn</param_name>
			<param_description>input arguments of the builtin.</param_description>
		</param_input_item>

	</param_input>

	<param_output>
		<param_output_item>
			<param_name>r1, ..., rn</param_name>
			<param_description>output arguments returned by the builtin</param_description>
		</param_output_item>
	</param_output>

	<description><p><b
      >builtin</b> calls the base built-in described by its name or function handle and input arguments.</p>
</description>
	<used_function />
	<bibliography />

<examples>
