  <language>en_US</language>
  <keyword>ismacro</keyword>
  <short_description>Check for the existence of a macro (function).</short_description>

  <syntax>
    <syntax_item>tf = ismacro(name)</syntax_item>
  </syntax>
  
  <param_input>

    <param_input_item>
      <param_name>name</param_name>
      <param_description>a string: macro name.</param_description>
    </param_input_item>
  </param_input>

  <param_output>
    <param_output_item>
      <param_name>tf</param_name>
      <param_description>a logical: true if macro exists.</param_description>
    </param_output_item>
  </param_output>

  <description><p><b>isbuiltin</b> checks for the existence of a macro.</p>
</description>
  <used_function></used_function>
  <bibliography></bibliography>

<examples>
  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description></example_item_description>
    <example_item_data><![CDATA[ismacro('isbuiltin')
ismacro('exist')]]>
    </example_item_data>
  </example_item>


</examples>

