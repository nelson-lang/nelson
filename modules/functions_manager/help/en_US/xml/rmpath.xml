	<language>en_US</language>
	<keyword>rmpath</keyword>
	<short_description>Remove directory from search path.</short_description>

	<syntax>
		<syntax_item>rmpath(dirname)</syntax_item>
		<syntax_item>previouspaths = rmpath(dirname)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>dirname</param_name>
			<param_description>name of directory to remove</param_description>
		</param_input_item>
	</param_input>

	<param_output>
		<param_output_item>
			<param_name>previouspaths</param_name>
			<param_description
      >a string: path prior to removing the specified paths</param_description>
		</param_output_item>
	</param_output>

	<description><p><b
      >rmpath</b> removes directory from search path.</p></description>
	<used_function />
	<bibliography />

<examples>
	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[path
addpath(tempdir())
path			
rmpath(tempdir())
path]]>
		</example_item_data>
	</example_item>


</examples>

