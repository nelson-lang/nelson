  <language>en_US</language>
  <keyword>clearfun</keyword>
  <short_description>Clear an built-in function.</short_description>
  <syntax>
    <syntax_item>l = clearfun(function_name)</syntax_item>
    <syntax_item>l = clearfun(function_handle)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>function_name</param_name>
      <param_description>a string: function name.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>function_handle</param_name>
      <param_description>a function handle.</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>l</param_name>
      <param_description>a logical</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p><b>clearfun</b> clears built-in.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[cos(3)
a = clearfun('cos')
cos(3)

sin(3)
b = clearfun(str2func('sin'))
sin(3)
]]></example_item_data>
    </example_item>
  </examples>
