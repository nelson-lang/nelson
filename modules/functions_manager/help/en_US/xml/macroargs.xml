	<language>en_US</language>
	<keyword>macroargs</keyword>
	<short_description>Returns variables names of a function.</short_description>

	<syntax>
		<syntax_item>[argOut, argIn] = macroarg(function_name)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>function_name</param_name>
			<param_description>a string: function name.</param_description>
		</param_input_item>
	</param_input>

	<param_output>
		<param_output_item>
			<param_name>argOut</param_name>
			<param_description>a cell with output arguments.</param_description>
		</param_output_item>
		<param_output_item>
			<param_name>argIn</param_name>
			<param_description>a cell with input arguments.</param_description>
		</param_output_item>

	</param_output>

	<description><p><b
      >macroargs</b> returns input and output variables used by the function.</p>
</description>
	<used_function />
	<bibliography />

<examples>
	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data
      ><![CDATA[[out_args, in_args] = macroarg('getfield')
[out_args, in_args] = macroarg('deal')]]>
		</example_item_data>
	</example_item>


</examples>

