    <language>fr_FR</language>
    <keyword>COM_methods</keyword>
    <short_description
  >Retourne les noms des méthodes d'un objet COM.</short_description>
    <syntax>
        <syntax_item>l = COM_methods(h)</syntax_item>
        <syntax_item>l = methods(h)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>h</param_name>
            <param_description>un objet COM.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>l</param_name>
            <param_description>une cellule de chaînes.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <b
    >methods</b> retourne une cellule de chaînes avec les noms des méthodes.</description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[e = actxserver('Excel.Application');
methods(e)
delete(e)
clear e]]></example_item_data>
            </example_item>
        </examples>
