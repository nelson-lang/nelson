    <language>fr_FR</language>
    <keyword>COM_xlsfinfo</keyword>
    <short_description
  >Détermine si le fichier contient une feuille de calcul Microsoft Excel.</short_description>
    <syntax>
        <syntax_item>status = xlsfinfo(filename)</syntax_item>
        <syntax_item>[status, sheets] = xlsfinfo(filename)</syntax_item>
        <syntax_item
    >[status, sheets, xlsformat] = xlsfinfo(filename)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>filename</param_name>
            <param_description
      >une chaîne : un nom de fichier.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>status</param_name>
            <param_description>une chaîne : type de fichier</param_description>
        </param_output_item>
        <param_output_item>
            <param_name>sheets</param_name>
            <param_description
      >un vecteur de chaînes : noms des feuilles</param_description>
        </param_output_item>
        <param_output_item>
            <param_name>xlsformat</param_name>
            <param_description
      >une chaîne : format de fichier Excel</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p
    >Interroge le fichier de feuille de calcul Excel filename pour obtenir des informations sur son contenu.</p>
    </description>
    <used_function />
    <bibliography />
    <examples>
        <example_item>
            <example_item_type>nelson</example_item_type>
            <example_item_description />
            <example_item_data
        runnable="cli"
      ><![CDATA[[status, sheets, xlsformat] =COM_xlsfinfo([modulepath('com_engine'), '/examples/sample_xslx.xlsx'])]]></example_item_data>
        </example_item>
    </examples>
