    <language>fr_FR</language>
    <keyword>COM_xlsread</keyword>
    <short_description
  >Lire un fichier de feuille de calcul Microsoft Excel en utilisant COM.</short_description>
    <syntax>
        <syntax_item>numeric_data = COM_xlsread(filename)</syntax_item>
        <syntax_item>numeric_data = COM_xlsread(filename, sheet)</syntax_item>
        <syntax_item>numeric_data = COM_xlsread(filename, range)</syntax_item>
        <syntax_item
    >numeric_data = COM_xlsread(filename, sheet, range)</syntax_item>
        <syntax_item
    >numeric_data = COM_xlsread(filename, sheet, range)</syntax_item>
        <syntax_item
    >[numeric_data, txt_data, raw_data] = COM_xlsread(filename)</syntax_item>
        <syntax_item
    >[numeric_data, txt_data, raw_data] = COM_xlsread(filename, sheet)</syntax_item>
        <syntax_item
    >[numeric_data, txt_data, raw_data] = COM_xlsread(filename, range)</syntax_item>
        <syntax_item
    >[numeric_data, txt_data, raw_data] = COM_xlsread(filename, sheet, range)</syntax_item>
        <syntax_item
    >[numeric_data, txt_data, raw_data] = COM_xlsread(filename, sheet, range)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>filename</param_name>
            <param_description
      >une chaîne : un nom de fichier existant.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>sheet</param_name>
            <param_description
      >un entier ou une chaîne : id de feuille ou nom de feuille</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>range</param_name>
            <param_description>une chaîne : une plage xx:xx</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>numeric_data</param_name>
            <param_description
      >une matrice ou un vecteur : données de chaîne converties en double.</param_description>
        </param_output_item>
        <param_output_item>
            <param_name>txt_data</param_name>
            <param_description
      >une cellule de chaînes avec seulement des chaînes.</param_description>
        </param_output_item>
        <param_output_item>
            <param_name>raw_data</param_name>
            <param_description
      >une cellule de chaînes : données brutes sans conversion.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <b
    >COM_xlsread</b> lit un fichier de feuille de calcul Microsoft Excel en utilisant COM.</description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[data = {'Time', 'Temp'; 12 98; 13 99; Inf 97};
s = COM_xlswrite([tempdir(), 'example_xlswrite_2.xlsx'], data, 'Temperatures');
[numeric_data, txt_data, raw_data] = COM_xlsread([tempdir(), 'example_xlswrite_2.xlsx'])]]></example_item_data>
            </example_item>
        </examples>
