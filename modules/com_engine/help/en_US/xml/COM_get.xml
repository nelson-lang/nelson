  <language>en_US</language>
  <keyword>COM_get</keyword>
  <short_description>Get property value from COM interface.</short_description>
  <syntax>
    <syntax_item>v = get(h, propertyname)</syntax_item>
    <syntax_item>v = COM_get(h, propertyname)</syntax_item>
    <syntax_item>v = h.propertyname</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>h</param_name>
      <param_description>a COM object.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>propertyname</param_name>
      <param_description
      >a string: the property's name of COM object.</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>v</param_name>
      <param_description>a nelson variable.</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p
    >The function returns the value of the property specified in the string, propertyname.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[e = actxserver('Excel.Application');
get(e, 'Path')
e.Path]]></example_item_data>
    </example_item>
  </examples>
