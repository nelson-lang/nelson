    <language>en_US</language>
    <keyword>actxserver</keyword>
    <short_description>Creates COM server.</short_description>
    <syntax>
        <syntax_item>h = actxserver(progid)</syntax_item>
        <syntax_item
    >h = actxserver(progid, 'machine', machineName)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>progid</param_name>
            <param_description
      >a string:  the name of a COM server.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>machineName</param_name>
            <param_description
      >a string: the name of the machine on which to start the server.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>h</param_name>
            <param_description>a COM object.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b
      >h = actxserver(progid)</b> creates a COM server using the progid identifier.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[h = actxserver('Excel.application')]]></example_item_data>
            </example_item>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[pTextToSpeech = actxserver('Sapi.SpVoice')
for i = 0:5
  invoke(pTextToSpeech, 'Speak', int2str(5 - i));
end
invoke(pTextToSpeech, 'Speak', _('Welcome to COM Interface for Nelson !'));
delete(pTextToSpeech)
clear pTextToSpeech]]></example_item_data>
            </example_item>
        </examples>
        <see_also>
            <see_also_item>
                <link
        linkend="${com_engine}actxGetRunningSrv"
      >actxGetRunningSrv</link>
            </see_also_item>
        </see_also>
