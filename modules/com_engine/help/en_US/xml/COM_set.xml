  <language>en_US</language>
  <keyword>COM_set</keyword>
  <short_description
  >Set object or interface property to specified value.</short_description>

  <syntax>
    <syntax_item>set(h, propertyname, value)</syntax_item>
    <syntax_item>COM_set(h, propertyname, value)</syntax_item>
    <syntax_item>h.propertyname = value</syntax_item>

  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>h</param_name>
      <param_description>a COM object.</param_description>
    </param_input_item>

    <param_input_item>
      <param_name>propertyname</param_name>
      <param_description
      >a string: the property's name of COM object.</param_description>
    </param_input_item>

    <param_input_item>
      <param_name>value</param_name>
      <param_description>a string, boolean, double ...</param_description>
    </param_input_item>
    
  </param_input>



  <description>
    <p
    >The function sets the property specified in the string propertyname to the given value.</p>
  </description>

  <used_function />
  <bibliography />
  

<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data
      ><![CDATA[pWord = actxserver('Word.Application')
pWord.Visible = true
invoke(pWord, 'Quit')
delete(pWord)
clear pWord]]>
    </example_item_data>
  </example_item>

</examples>

