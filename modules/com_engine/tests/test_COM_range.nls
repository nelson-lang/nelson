assert_isequal(nargin('COM_range'), -1);
assert_isequal(nargout('COM_range'), 1);
//=============================================================================
s = COM_range('A1:B2');
assert_istrue(s);
//=============================================================================
s = COM_range('Nel:Son');
assert_isfalse(s);
