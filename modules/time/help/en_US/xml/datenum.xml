                                                                        <examples
  >
                                                                            <example_item
    >
                                                                                <example_item_type
      >nelson</example_item_type>
                                                                                <example_item_description
      />
                                                                                <example_item_data
      ><![CDATA[
d = datenum([1973,8,4,12,1,18])
datevec(d)
d = datenum('04–Aug-1973 12:01:18')
d = datenum(["04–Aug-1973 12:01:18"; "04–Aug-1974 11:01:18"])
]]></example_item_data>
                                                                            </example_item>
