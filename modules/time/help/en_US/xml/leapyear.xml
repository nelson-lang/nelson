    <language>en_US</language>
    <keyword>leapyear</keyword>
    <short_description>Determine leap year.</short_description>
    <syntax>
        <syntax_item>tf = leapyear(year)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>year</param_name>
            <param_description
      >year: scalar or array numeric value.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>tf</param_name>
            <param_description
      >Leap year determination result: scalar or array logical value.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>leapyear</b> determines leap years.</p>
            <p>Leap years is done by Gregorian calendar rules.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[tf = leapyear([2020 2021 2022])]]></example_item_data>
            </example_item>
        </examples>
