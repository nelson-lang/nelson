    <language>en_US</language>
    <keyword>time</keyword>
    <short_description
  >Return the current time as the number of seconds or nanoseconds since the epoch.</short_description>
    <syntax>
        <syntax_item>t_s = time()</syntax_item>
        <syntax_item>t_s = time('s')</syntax_item>
        <syntax_item>t_ns = time('ns')</syntax_item>
    </syntax>
    <param_output>
        <param_output_item>
            <param_name>t_s</param_name>
            <param_description
      >a double: value of current time as the number of seconds since the epoch.</param_description>
        </param_output_item>
        <param_output_item>
            <param_name>t_ns</param_name>
            <param_description
      >a unsigned integer 64 bit: value of current time as the number of nanoseconds since the epoch.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b
      >time</b> returns the current time as the number of seconds or nanoseconds since the epoch.</p>
            <p
    >The epoch is referenced to 00:00:00 UTC (Coordinated Universal Time) 1 Jan 1970.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[t1=time()
sleep(10)
t2 = time()
t2 - t1
]]></example_item_data>
            </example_item>
        </examples>
