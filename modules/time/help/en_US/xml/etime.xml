  <language>en_US</language>
  <keyword>etime</keyword>
  <short_description>Time elapsed between date vectors.</short_description>
  <syntax>
    <syntax_item>e = etime(t2, t1)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>t2</param_name>
      <param_description
      >Date vectors: 1-by-6 vector or m-by-6 matrix.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>t1</param_name>
      <param_description
      >Date vectors: 1-by-6 vector or m-by-6 matrix.</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>e</param_name>
      <param_description
      >a scalar or a vector: time elapsed (seconds).</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p><b
      >e = etime(t2, t1)</b> returns the number of seconds between two date vectors or matrices of date vectors,<b
      >t1</b> and <b>t2</b>.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[t1 = clock()
sleep(6)
t2 = clock()
etime(t2, t1)]]></example_item_data>
    </example_item>
  </examples>
