  <language>fr_FR</language>
  <keyword>eomday</keyword>
  <short_description>Returns last day of month.</short_description>
  <syntax>
    <syntax_item>E = eomday(Y, M)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>Y</param_name>
      <param_description>année : réel.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>M</param_name>
      <param_description>mois : réel.</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>E</param_name>
      <param_description>dernier jour du mois : réel.</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p><b>E = eomday(Y, M)</b> returns the last day of the month<b
      >M</b> for the year <b>Y</b>.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[eomday(1900, 1:12)]]></example_item_data>
    </example_item>
  </examples>
