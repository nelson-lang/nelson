    <language>fr_FR</language>
    <keyword>calendar</keyword>
    <short_description>Calendar.</short_description>
    <syntax>
        <syntax_item>calendar()</syntax_item>
        <syntax_item>c = calendar()</syntax_item>
        <syntax_item>c = calendar(d)</syntax_item>
        <syntax_item>c = calendar(y, m)</syntax_item>
    </syntax>
    <param_input>
                <param_input_item>
                        <param_name>d</param_name>
                        <param_description
      >un entier : numéro de date série.</param_description>
                </param_input_item>
                <param_input_item>
                        <param_name>y</param_name>
                        <param_description
      >un entier : année souhaitée [1400, 9999].</param_description>
                </param_input_item>
                <param_input_item>
                        <param_name>m</param_name>
                        <param_description
      >un entier : mois souhaité [1, 12].</param_description>
                </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>c</param_name>
            <param_description>une matrice 6x7.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>calendar()</b> returns the currently monthly calendar.</p>
            <p
    >If no output arguments are specified,the calendar is displayed on the screen instead of returning a matrix 6x7.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[calendar()
c = calendar(1973, 8)
c = calendar(datenum(1973, 8, 4))]]></example_item_data>
            </example_item>
        </examples>
        <see_also>
            <see_also_item>
                <link linkend="${time}datenum">datenum</link>
            </see_also_item>
        </see_also>
