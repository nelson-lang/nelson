assert_isequal(nargin('clock'), 0);
assert_isequal(nargout('clock'), 1);
//=============================================================================
c = clock();
assert_isequal(size(c), [1 6]);
//=============================================================================
n = now();
d = datevec(n);
assert_isequal(c(1:5), d(1:5));
