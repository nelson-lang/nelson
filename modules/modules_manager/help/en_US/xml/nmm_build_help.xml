  <language>en_US</language>
  <keyword>nmm_build_help</keyword>
  <short_description>helper's function to build help of an external module</short_description>

  <syntax>
    <syntax_item>nmm_build_help(module_short_name, module_root_path)</syntax_item>
  </syntax>


  <param_input>
    <param_input_item>
      <param_name>module_short_name</param_name>
      <param_description>a string: short module's name.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>module_root_path</param_name>
      <param_description>a string: path of the module named 'module_short_name'.</param_description>
    </param_input_item>

  </param_input>

  <description>
  <p><b>nmm_build_help</b> generates help of an external module.</p>
  </description>

  <used_function></used_function>
  <bibliography></bibliography>

<examples>
  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description>See module skeleton for example</example_item_description>
