  <language>en_US</language>
  <keyword>removemodule</keyword>
  <short_description>remove a module from Nelson.</short_description>
  <syntax>
    <syntax_item>removemodule(module_short_name)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>module_short_name</param_name>
      <param_description>a string: short module's name.</param_description>
    </param_input_item>
  </param_input>
  <description>
    <p><b>removemodule</b> remove a module designed by his short name.</p>
    <p
    >all core's modules are protected and cannot removed during an nelson's session.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description
      >See module skeleton for example</example_item_description>
      <example_item_data
        runnable="cli"
      ><![CDATA[ismodule('module_skeleton')
addmodule([nelsonroot(), '/module_skeleton'], 'module_skeleton')
ismodule('module_skeleton')
removemodule('module_skeleton')
ismodule('module_skeleton')]]></example_item_data>
    </example_item>
  </examples>
