	<language>en_US</language>
	<keyword>str2func</keyword>
	<short_description>Returns a function handle from a string.</short_description>

	<syntax>
		<syntax_item>func_handle = str2func(str)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>str</param_name>
			<param_description>a string</param_description>
		</param_input_item>
	</param_input>

	<param_output>

		<param_output_item>
			<param_name>func_handle</param_name>
			<param_description>a function handle.</param_description>
		</param_output_item>

	</param_output>

	<description>
		<p><b>function_handle = str2func(str)</b> returns a function handle <b
      >function_handle</b> for the function named in the string <b>str</b></p>
		<p><b>str</b> function name or representation of anonymous function.</p>
	</description>


	<used_function />
	<bibliography />

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[fh = str2func('cos')
str = func2str(fh)]]>
		</example_item_data>
	
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data
      ><![CDATA[myFind = str2func('@(x, y) find(x > y)')
M = rand(4, 3, 5);
[R, C] = myFind(M, 0.9)]]>
		</example_item_data>
	
	</example_item>

</examples>

