	<language>en_US</language>
	<keyword>func2str</keyword>
	<short_description
  >Return a function handle constructed from a string.</short_description>

	<syntax>
		<syntax_item>func_handle = str2func(str)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>str</param_name>
			<param_description>a string.</param_description>
		</param_input_item>
	</param_input>

	<param_output>

		<param_output_item>
			<param_name>func_handle</param_name>
			<param_description>a function handle</param_description>
		</param_output_item>

	</param_output>

	<description>
<p><b
      >func_handle = str2func(str)</b> returns a function handle constructed from the string <b
      >str</b>.</p>
</description>


	<used_function />
	<bibliography />

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[fh = str2func('cos')
class(fh)]]>
		</example_item_data>
	
	</example_item>

</examples>

