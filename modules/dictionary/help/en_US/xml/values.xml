    <language>en_US</language>
    <keyword>values</keyword>
    <short_description>Values of dictionary.</short_description>
    <syntax>
        <syntax_item>v = values(d)</syntax_item>
        <syntax_item>v = values(d, 'cell')</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>d</param_name>
            <param_description>scalar: dictionary object.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>v</param_name>
            <param_description>values.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b
      >v = values(d)</b> retrieves an array containing the values of the specified dictionary, <b
      >d</b>.</p>
            <p>
                <b
      >v = values(d, 'cell')</b> optionally returns the values as a cell array.</p>
            </description>
            <used_function />
            <bibliography />
            <examples>
                <example_item>
                    <example_item_type>nelson</example_item_type>
                    <example_item_description />
                    <example_item_data
      ><![CDATA[names = ["Biil" "John" "Yann"];
wheels = [1 2 3];
d = dictionary(wheels, names)
v = values(d)
v = values(d, 'cell')
]]></example_item_data>
                </example_item>
            </examples>
            <see_also>
                <see_also_item>
                    <link linkend="${dictionary}dictionary">dictionary</link>
                </see_also_item>
                <see_also_item>
                    <link linkend="${dictionary}keys">keys</link>
                </see_also_item>
            </see_also>
