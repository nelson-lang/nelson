	<language>en_US</language>
	<keyword>remove</keyword>
	<short_description>Remove dictionary entries.</short_description>

	<syntax>
		<syntax_item>db = remove(da, key)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>da</param_name>
			<param_description>scalar: a dictionary object.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>key</param_name>
			<param_description>scalar or array: key</param_description>
		</param_input_item>
	</param_input>

	<param_output>
		<param_output_item>
			<param_name>db</param_name>
			<param_description>scalar: a dictionary object.</param_description>
		</param_output_item>
	</param_output>

	<description>
		<p><b
      >db = remove(da, key)</b> deletes the entry associated with the key from dictionary da.</p>
		<p><b>d = remove(d, key)</b> is equivalent to <b>d[key] = []</b>.</p>
	</description>
	<used_function />
	<bibliography />

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data
      ><![CDATA[names = ["Apple" "Banana" "Kiwi"];
wheels = [1 2 3];
d = dictionary(wheels, names)
d = remove(d, 2)
]]>
		</example_item_data>
	</example_item>

</examples>

