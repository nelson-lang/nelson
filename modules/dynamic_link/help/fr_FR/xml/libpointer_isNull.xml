        <language>fr_FR</language>
        <keyword>libpointer_isNull</keyword>
        <short_description
  >Vérifie si un handle libpointer pointe vers NULL</short_description>
    <syntax>
        <syntax_item>tf = isNull(h)</syntax_item>
        <syntax_item>tf = h.isNull()</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>h</param_name>
            <param_description>a libpointer handle.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>tf</param_name>
            <param_description>a logical.</param_description>
        </param_output_item>
    </param_output>
    <description>
    <p>Vérifie si un handle libpointer pointe vers un pointeur NULL.</p>
    </description>
    <used_function />
    <bibliography />
    <examples>
        <example_item>
            <example_item_type>nelson</example_item_type>
            <example_item_description />
            <example_item_data
        runnable="cli"
      ><![CDATA[p = libpointer('int8Ptr', int8([3 4]));
p.isNull()
p2 = libpointer()
p2.isNull()
isNull(p2)]]></example_item_data>
        </example_item>
    </examples>
