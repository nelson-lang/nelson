	<language>en_US</language>
	<keyword>dlgeneratecleaner</keyword>
	<short_description>Generates cleaner.nls file for C++ gateway.</short_description>

	<syntax>
      <syntax_item>dlgeneratecleaner(destinationdir)</syntax_item>
      <syntax_item>dlgeneratecleaner(destinationdir, files)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>destinationdir</param_name>
			<param_description>a string: destination directory where is generated the cleaner.nls file.</param_description>
        </param_input_item>            
		<param_input_item>
			<param_name>files</param_name>
			<param_description>a string or a cell of string: list of files to delete.</param_description>
		</param_input_item>

	</param_input>

	<description>
		<p><b>dlgeneratecleaner</b> generates a 'cleaner.nls' to remove files.</p>
	</description>


	<used_function></used_function>
	<bibliography></bibliography>

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description>See module skeleton for example</example_item_description>
		<example_item_data><![CDATA[
dlgeneratecleaner(tempdir());
text = fileread([tempdir(), 'cleaner.nls'])]]>
	</example_item_data>
	
	</example_item>


</examples>

