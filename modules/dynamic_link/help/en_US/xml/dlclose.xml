  <language>en_US</language>
  <keyword>dlclose</keyword>
  <short_description>Removes dllib object.</short_description>

  <syntax>
    <syntax_item>dllib_delete(h)</syntax_item>
    <syntax_item>delete(h)</syntax_item>
    <syntax_item>dlclose(h)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>h</param_name>
      <param_description>a handle: an dllib object.</param_description>
    </param_input_item>
  </param_input>

  <description>
  <p><b>dlclose(h)</b> or <b>delete(h)</b> releases dllib object.</p>
  <p>Do not forget to clear h afterward.</p> 
 </description>

  <used_function></used_function>
  <bibliography></bibliography>

<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description></example_item_description>
    <example_item_data><![CDATA[path_ref = [modulepath(nelsonroot(),'dynamic_link','bin'), '/libnlsDynamic_link', getdynlibext()];
lib = dlopen(path_ref)
isvalid(lib)
dlclose(lib); // or delete(lib)
isvalid(lib)]]>
    </example_item_data>
  </example_item>

</examples>

