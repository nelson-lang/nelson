  <language>en_US</language>
  <keyword>libpointer_reshape</keyword>
  <short_description>Reshapes libpointer dimensions.</short_description>

  <syntax>
    <syntax_item>tf = h.reshape(X, Y)</syntax_item>
  </syntax>

  <param_input>
    <param_input_item>
      <param_name>h</param_name>
      <param_description>a libpointer handle.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>X</param_name>
      <param_description>a scalar double: new X dimension.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>Y</param_name>
      <param_description>a scalar double: new Y dimension.</param_description>
    </param_input_item>

  </param_input>

  <description>
    <p>Set dimensions from libpointer object.</p>
</description>

  <used_function />
  <bibliography />

  <see_also>
    <see_also_item>
      <link linkend="${dynamic_link}libpointer">libpointer</link>
    </see_also_item>
  </see_also>


<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data
      ><![CDATA[a = libpointer('doublePtr', eye(2, 2));
a.reshape(3, 3);
a.Value]]>
    </example_item_data>
  </example_item>

</examples>


