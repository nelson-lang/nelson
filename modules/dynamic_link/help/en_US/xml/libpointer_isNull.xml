  <language>en_US</language>
  <keyword>libpointer_isNull</keyword>
  <short_description>Checks if libpointer handle points on NULL pointer.</short_description>

  <syntax>
    <syntax_item>tf = isNull(h)</syntax_item>
    <syntax_item>tf = h.isNull()</syntax_item>
  </syntax>

  <param_input>
    <param_input_item>
      <param_name>h</param_name>
      <param_description>a libpointer handle.</param_description>
    </param_input_item>
  </param_input>

  <param_output>
    <param_output_item>
      <param_name>tf</param_name>
      <param_description>a logical.</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p>Checks if libpointer handle points on NULL pointer.</p>
</description>

  <used_function></used_function>
  <bibliography></bibliography>

  <see_also>
    <see_also_item>
      <link linkend="${dynamic_link}libpointer">libpointer</link>
    </see_also_item>

  </see_also>


<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description></example_item_description>
    <example_item_data><![CDATA[p = libpointer('int8Ptr', int8([3 4]));
p.isNull()
p2 = libpointer()
p2.isNull()
isNull(p2)]]>
    </example_item_data>
  </example_item>

</examples>


