  <language>en_US</language>
  <keyword>libpointer_delete</keyword>
  <short_description>Removes libpointer object.</short_description>

  <syntax>
    <syntax_item>libpointer_delete(h)</syntax_item>
    <syntax_item>delete(h)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>h</param_name>
      <param_description>a handle: an libpointer object.</param_description>
    </param_input_item>
  </param_input>

  <description>
  <p><b>delete(h)</b> releases libpointer object.</p>
  <p>Do not forget to clear h afterward.</p> 
 </description>

  <used_function></used_function>
  <bibliography></bibliography>

<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description></example_item_description>
    <example_item_data><![CDATA[libpointer_used(),delete(libpointer_used())]]>
    </example_item_data>
  </example_item>

</examples>

