  <language>en_US</language>
  <keyword>cosm</keyword>
  <short_description>Computes the matrix cosine of a square matrix.</short_description>

  <syntax>
    <syntax_item>res = cosm(x)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>x</param_name>
      <param_description>a numeric value: scalar or square matrix</param_description>
    </param_input_item>
  </param_input>

  <param_output>
    <param_output_item>
      <param_name>res</param_name>
      <param_description>a numeric value: a square matrix</param_description>
    </param_output_item>
  </param_output>

  <description><b>cosm(x)</b> computes the matrix cosine of <b>x</b>.</description>

  <used_function></used_function>
  <bibliography></bibliography>

<examples>
  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description></example_item_description>
    <example_item_data><![CDATA[A = eye(3, 3);
res = cosm(A)
A = [1, 2; 3, 4];
res = cosm(A)]]>
    </example_item_data>
  </example_item>

</examples>

