	<language>en_US</language>
	<keyword>sin</keyword>
	<short_description
  >Computes the sine in radians for each element of x.</short_description>

	<syntax>
		<syntax_item>res = sin(x)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>x</param_name>
			<param_description>a numeric value</param_description>
		</param_input_item>
	</param_input>

	<param_output>
		<param_output_item>
			<param_name>res</param_name>
			<param_description>a numeric value</param_description>
		</param_output_item>
	</param_output>

	<description><b>sin</b> computes the sine in radians for each element of <b
    >x</b>.</description>

	<used_function />
	<bibliography />

<examples>
	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[A = eye(3, 3);
res = sin(A)]]>
		</example_item_data>
	</example_item>

</examples>

