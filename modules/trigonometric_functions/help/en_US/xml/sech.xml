  <language>en_US</language>
  <keyword>sech</keyword>
  <short_description>Hyperbolic secant.</short_description>
  <syntax>
    <syntax_item>res = sech(x)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>x</param_name>
      <param_description>a numeric value</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>res</param_name>
      <param_description>a numeric value</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p><b>sech</b> computes the Hyperbolic secant for each element of<b
      >x</b>.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[X = [3*pi, 2*pi, pi, 0];
R = sech(X)]]></example_item_data>
    </example_item>
  </examples>
