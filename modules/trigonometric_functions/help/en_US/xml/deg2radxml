<?xml version="1.0" encoding="UTF-8"?>
<xmldoc>
	<copyright>SAME AS NELSON SOFTWARE</copyright>

	<language>en_US</language>
	<keyword>deg2rad</keyword>
	<short_description>Convert angle from degrees to radians.</short_description>

	<syntax>
		<syntax_item>r = deg2rad(d)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>d</param_name>
			<param_description>a numeric value (double or single)</param_description>
		</param_input_item>
	</param_input>

	<param_output>
		<param_output_item>
			<param_name>r</param_name>
			<param_description>a numeric value</param_description>
		</param_output_item>
	</param_output>

	<description><b>d = deg2rad(r)</b> converts angle units from degrees to radians for each element of <b>r</b>.</description>

	<used_function></used_function>
	<bibliography></bibliography>

<examples>
	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[D = 64.7;
R = deg2rad(D);
radEarth = 6371;
dist = radEarth * R]]>
		</example_item_data>
	</example_item>

</examples>

	<see_also>
		<see_also_item>
			<link linkend="${trigonometric_functions}rad2deg">rad2deg</link>
		</see_also_item>
	</see_also>

	<history>
		<history_item>
			<history_version>1.0.0</history_version>
			<history_description>initial version</history_description>
		</history_item>
	</history>

	<authors>
		<author_item>Allan CORNET</author_item>
	</authors>
</xmldoc>
