  <language>en_US</language>
  <keyword>cosd</keyword>
  <short_description
  >Computes the cosine in degree for each element of x.</short_description>
  <syntax>
    <syntax_item>res = cosd(x)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>x</param_name>
      <param_description>a numeric value</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>res</param_name>
      <param_description>a numeric value</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p><b>cosd</b> computes the cosine in degree for each element of <b
      >x</b>.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[A = [0 30 45 60 90 360];;
res = cosd(A)]]></example_item_data>
    </example_item>
  </examples>
