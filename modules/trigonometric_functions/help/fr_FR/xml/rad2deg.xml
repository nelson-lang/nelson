    <language>fr_FR</language>
    <keyword>rad2deg</keyword>
        <short_description
  >Convertit un angle de radians en degrés.</short_description>
    <syntax>
        <syntax_item>d = rad2deg(r)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>r</param_name>
        <param_description
      >une valeur numérique (double ou simple)</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>d</param_name>
            <param_description>une valeur numérique</param_description>
        </param_output_item>
    </param_output>
    <description>
        <b
    >d = rad2deg(r)</b> convertit les unités d'angle de radians en degrés pour chaque élément de <b
    >r</b>.</description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[dist = 7194;
radEarth = 6371;
D = rad2deg(dist / radEarth)]]></example_item_data>
            </example_item>
        </examples>
