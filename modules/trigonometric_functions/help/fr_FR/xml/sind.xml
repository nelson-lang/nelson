    <language>fr_FR</language>
    <keyword>sind</keyword>
        <short_description
  >Calcule le sinus en degrés pour chaque élément de x.</short_description>
    <syntax>
        <syntax_item>res = sind(x)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>x</param_name>
            <param_description>une valeur numérique</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>res</param_name>
            <param_description>une valeur numérique</param_description>
        </param_output_item>
    </param_output>
    <description>
        <b>sind</b> calcule le sinus en degrés pour chaque élément de <b
    >x</b>.</description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[A = [0 30 45 60 90 360];
sind(A)]]></example_item_data>
            </example_item>
        </examples>
