    <language>en_US</language>
    <keyword>asech</keyword>
    <short_description
  >Inverse hyperbolic secant of angle in radians.</short_description>
    <syntax>
        <syntax_item>res = asech(x)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>x</param_name>
            <param_description>a numeric value</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>res</param_name>
            <param_description>a numeric value</param_description>
        </param_output_item>
    </param_output>
    <description>
        <b
    >asech</b> computes the inverse hyperbolic secant of argument in radians for each element of <b
    >x</b>.</description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[x = -pi:0.75:pi;
R = asech(x)]]></example_item_data>
            </example_item>
        </examples>
