    <language>fr_FR</language>
    <keyword>h5read</keyword>
    <short_description>Lit un jeu de données HDF5.</short_description>
    <syntax>
        <syntax_item>val = h5read(filename, location)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>filename</param_name>
            <param_description
      >une chaîne : nom de fichier HDF5.</param_description>
        </param_input_item>
        <param_input_item>
                        <param_name>location</param_name>
                        <param_description
      >une chaîne : chemin complet identifiant un jeu de données.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>val</param_name>
            <param_description>une variable Nelson.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p><b>h5read</b> lit le jeu de données situé à <b
      >location</b> dans le fichier HDF5.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[h5_directory = [modulepath('hdf5','tests'), '/h5'];
double_data = [h5_directory, '/h5ex_t_float.h5'];
R = h5read(double_data,'/DS1')]]></example_item_data>
            </example_item>
        </examples>
