        <language>fr_FR</language>
        <keyword>whosnh5</keyword>
        <short_description
  >Liste les variables d'un fichier .nh5 valide avec tailles et types.</short_description>
    <syntax>
        <syntax_item>whosnh5(filename)</syntax_item>
        <syntax_item>st = whosnh5(filename)</syntax_item>
        <syntax_item>whosnh5(filename, var1, ..., varN)</syntax_item>
        <syntax_item>st = whosnh5(filename, var1, ..., varN)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>filename</param_name>
            <param_description
      >une chaîne : nom de fichier .nh5.</param_description>
        </param_input_item>
        <param_input_item>
                        <param_name>var1, ..., varN</param_name>
                        <param_description
      >chaînes : noms des variables à inspecter.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
                        <param_name>st</param_name>
                        <param_description
      >contient des informations sur les variables dans le tableau de structures st.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p><b>whosnh5</b> liste les variables d'un fichier .nh5 valide.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[A = ones(3, 4);
B = 'Nelson';
C = sparse(true);
D = sparse(3i);		
savenh5([tempdir(), 'example_whosnh5.nh5'], 'A', 'B', 'C', 'D')
whosnh5([tempdir(), 'example_whosnh5.nh5'])
st = whosnh5([tempdir(), 'example_whosnh5.nh5'])]]></example_item_data>
            </example_item>
        </examples>
