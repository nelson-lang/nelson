    <language>fr_FR</language>
    <keyword>h5write</keyword>
    <short_description>Écrit un jeu de données HDF5.</short_description>
    <syntax>
        <syntax_item>h5write(filename, location, value)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>filename</param_name>
            <param_description>a string: hdf5 filename.</param_description>
        </param_input_item>
        <param_input_item>
                        <param_name>location</param_name>
                        <param_description
      >une chaîne : chemin complet identifiant un jeu de données.</param_description>
        </param_input_item>
        <param_input_item>
        <param_name>value</param_name>
        <param_description
      >une valeur : types supportés : double, uint64, uint32, uint16, uint8, single, int64, int32, int16, int8 ou tableau de caractères.</param_description>
        </param_input_item>
    </param_input>
    <description>
        <p><b>h5write</b> écrit des données dans l'ensemble du jeu de données <b
      >location</b> dans le fichier HDF5.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[h5filename = [tempdir(), 'doc_h5write.h5'];
R = rand(3, 4)
h5write(h5filename,'/rand', R);
h5write(h5filename,'/str', 'Hello');
h5dump(h5filename)]]></example_item_data>
            </example_item>
        </examples>
