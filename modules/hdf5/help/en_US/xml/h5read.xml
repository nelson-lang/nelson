    <language>en_US</language>
    <keyword>h5read</keyword>
    <short_description>Read HDF5 data set.</short_description>
    <syntax>
        <syntax_item>val = h5read(filename, location)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>filename</param_name>
            <param_description>a string: hdf5 filename.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>location</param_name>
            <param_description
      >a string: full path identifying a data set.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>val</param_name>
            <param_description>a nelson's variable.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>h5read</b> reads data set in <b
      >location</b> from the HDF5 file.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[h5_directory = [modulepath('hdf5','tests'), '/h5'];
double_data = [h5_directory, '/h5ex_t_float.h5'];
R = h5read(double_data,'/DS1')]]></example_item_data>
            </example_item>
        </examples>
        <see_also>
            <see_also_item>
                <link linkend="${hdf5}h5write">h5write</link>
            </see_also_item>
        </see_also>
        <history>
            <history_item>
                <history_version>1.0.0</history_version>
                <history_description>initial version</history_description>
            </history_item>
        </history>
        <authors>
            <author_item>Allan CORNET</author_item>
        </authors>
    </xmldoc>
