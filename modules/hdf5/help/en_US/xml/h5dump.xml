	<language>en_US</language>
	<keyword>h5dump</keyword>
	<short_description>dump the content of hdf5 file as text.</short_description>

	<syntax>
		<syntax_item>h5dump(filename)</syntax_item>
		<syntax_item>R = h5dump(filename)</syntax_item>
		<syntax_item>h5dump(filename, location)</syntax_item>
		<syntax_item>R = h5dump(filename, location)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>filename</param_name>
			<param_description>a string: hdf5 filename.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>location</param_name>
			<param_description>a string: name of the path to dump.</param_description>
		</param_input_item>
	</param_input>
	<param_output>
		<param_output_item>
			<param_name>R</param_name>
			<param_description>a string: dump of hdf5 file as text.</param_description>
		</param_output_item>
	</param_output>


	<description>
		<p><b>h5dump</b> dump the content of hdf5 file as text.</p>
	</description>


	<used_function></used_function>
	<bibliography></bibliography>

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[h5create([tempdir(), 'myfile.h5'],'/myDataset2',[10 20]);
h5dump([tempdir(), 'myfile.h5'])
R = h5dump([tempdir(), 'myfile.h5'])]]>
		</example_item_data>
	</example_item>

</examples>

