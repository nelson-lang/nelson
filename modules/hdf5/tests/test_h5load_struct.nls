test_h5save_file = [tempdir(), '/test_h5save.nh5'];
if isfile(test_h5save_file)
  rmfile(test_h5save_file);
end
//=============================================================================
A.a = 1;
A.b = uint8(2);
B.c = 4;
B.d = 5;
B.e = A;
A.f = B;
h5save(test_h5save_file, 'A');
A_REF = A;
clear A;
st = h5load(test_h5save_file);
assert_isequal(A_REF, st.A);
//=============================================================================
