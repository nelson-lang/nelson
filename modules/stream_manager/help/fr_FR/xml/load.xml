    <language>fr_FR</language>
    <keyword>load</keyword>
        <short_description
  >charger des données depuis un fichier .nh5 ou .mat dans l'espace de travail de Nelson.</short_description>
    <syntax>
        <syntax_item>load(filename)</syntax_item>
        <syntax_item>st = load(filename)</syntax_item>
        <syntax_item>load(filename, var1, ..., varN)</syntax_item>
        <syntax_item>st = load(filename, var1, ..., varN)</syntax_item>
        <syntax_item>load(filename, '-mat')</syntax_item>
        <syntax_item>load(filename, '-nh5')</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>filename</param_name>
        <param_description
      >une chaîne : nom de fichier .nh5 ou .mat.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>'-mat' or '-nh5'</param_name>
        <param_description
      >force la lecture du fichier en tant que nh5 ou mat.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>var1, ..., varN</param_name>
        <param_description
      >chaîne : noms des variables à charger dans l'espace de travail de Nelson.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>st</param_name>
        <param_description
      >une structure avec les noms de variables comme champs.</param_description>
        </param_output_item>
    </param_output>
                <description>
                <p>
                        <b
      >load</b> charge des données depuis un fichier .nh5 ou .mat vers l'espace de travail de Nelson.</p>
                </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[A = ones(3, 4);
B = 'hello for open mat users';
save([tempdir(), 'example_load.mat'], 'A', 'B')
clear;
st = load([tempdir(), 'example_load.mat']);
who
st.A
st.B
clear
who
load([tempdir(), 'example_load.mat']);
who
A
B
]]></example_item_data>
            </example_item>
        </examples>
