    <keyword>frewind</keyword>
        <short_description
  >Positionne le flux au début du fichier.</short_description>
    <syntax>
        <syntax_item>frewind(fid)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>fid</param_name>
        <param_description
      >une valeur entière : descripteur de fichier</param_description>
        </param_input_item>
    </param_input>
        <description>
        <p>
            <b>frewind</b> positionne le pointeur au début du fichier</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[
fileID = fopen([tempdir(), 'frewind.txt'],'wt');
fprintf(fileID, 'son is beautiful.');
frewind(fileID);
fprintf(fileID, 'sun');
fclose(fileID);
R = fileread([tempdir(), 'frewind.txt'])]]></example_item_data>
            </example_item>
        </examples>
