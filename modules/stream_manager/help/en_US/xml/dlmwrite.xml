  <language>en_US</language>
  <keyword>dlmwrite</keyword>
  <short_description>Write an numeric matrix to a text file file using a delimiter.</short_description>

  <syntax>
  <syntax_item>dlmwrite(filename, M)</syntax_item>
  <syntax_item>dlmwrite(filename, M, delimiter)</syntax_item>
  <syntax_item>dlmwrite(filename, M, '-append')</syntax_item>
  <syntax_item>dlmwrite(filename, M, '-append', delimiter)</syntax_item>
  <syntax_item>dlmwrite(filename, M, delimiter, r, c)</syntax_item>
  <syntax_item>dlmwrite(filename, M, '-append', delimiter, r, c)</syntax_item>
  <syntax_item>dlmwrite(filename, M, delimiter, r, c, eol)</syntax_item>
  <syntax_item>dlmwrite(filename, M, '-append', delimiter, r, c, eol)</syntax_item>
  <syntax_item>dlmwrite(filename, M, delimiter, r, c, eol, precision)</syntax_item>
  <syntax_item>dlmwrite(filename, M, '-append', delimiter, r, c, eol, precision)</syntax_item> 
  </syntax>
  
  <param_input>

    <param_input_item>
      <param_name>filename</param_name>
      <param_description>a string: filename destination.</param_description>
    </param_input_item>
    
   
    <param_input_item>
      <param_name>M</param_name>
      <param_description>an numeric or logical matrix.</param_description>
    </param_input_item>
     
    <param_input_item>
      <param_name>delimiter</param_name>
      <param_description>a string: ',' , '\t', ';' delimiter. default ','</param_description>
    </param_input_item>

    <param_input_item>
      <param_name>r, c</param_name>
      <param_description>integer: offset. default : 0, 0</param_description>
    </param_input_item>

    <param_input_item>
      <param_name>eol</param_name>
      <param_description>a string: 'pc' or 'unix'.</param_description>
    </param_input_item>


    <param_input_item>
      <param_name>precision</param_name>
      <param_description>a integer or C format string. (default: 5)</param_description>
    </param_input_item>

  </param_input>

  <description>
   <p><b>dlmwrite</b> writes an numeric matrix to an ASCII format file.</p>
  </description>

  <used_function></used_function>
  <bibliography></bibliography>

<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description></example_item_description>
    <example_item_data><![CDATA[A = [Inf, -Inf, NaN, 3];
filename = [tempdir(), 'dlwrite_example.csv'];
dlmwrite(filename, A);
R = fileread(filename)
A = eye(3, 2);
dlmwrite(filename, A, ';', 4, 5);
R = fileread(filename)]]>
    </example_item_data>
  
  </example_item>

</examples>

