	<language>en_US</language>
	<keyword>disp</keyword>
	<short_description>Display a variable.</short_description>

	<syntax>
		<syntax_item>disp(V)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>V</param_name>
			<param_description>a variable</param_description>
		</param_input_item>
	</param_input>


	<description>
		<p><b>disp(V)</b> displays the value of the variable <b>V</b>.</p>
		<p><b>disp</b> uses current <b>format</b> setting to display numeric values.</p>
	</description>


	<used_function></used_function>
	<bibliography></bibliography>

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[disp('Hello Nelson')]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[disp(pi)]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[disp(eye(3, 3))]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description>disp always ends with a newline.</example_item_description>
		<example_item_data><![CDATA[disp('')]]>
		</example_item_data>
	</example_item>

</examples>

