        <language>fr_FR</language>
        <keyword>mustBeA</keyword>
        <short_description
  >Vérifie que la valeur d'entrée appartient à l'une des classes spécifiées.</short_description>
    <syntax>
        <syntax_item>mustBeA(var, classNames)</syntax_item>
        <syntax_item>mustBeA(var, classNames, argPosition)</syntax_item>
        <syntax_item
    >C++: void mustBeA(const ArrayOfVector&amp; args, const wstringVector &amp;classNames, int argPosition)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>var</param_name>
            <param_description>une variable.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>classNames</param_name>
            <param_description
      >une variable : nom du type de données ou de la classe.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>argPosition</param_name>
            <param_description
      >un entier positif : position de l'argument d'entrée.</param_description>
        </param_input_item>
    </param_input>
    <description>
        <p><b
      >mustBeA</b> vérifie que la valeur d'entrée appartient à l'une des classes spécifiées.</p>
    </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[mustBeA(1, 'double')
mustBeA([], ["double", "single"])]]></example_item_data>
            </example_item>
        </examples>
