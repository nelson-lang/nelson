  <language>en_US</language>
  <keyword>mustBeFile</keyword>
  <short_description>Checks that input path refers to file.</short_description>

  <syntax>
    <syntax_item>mustBeFile(var)</syntax_item>
    <syntax_item>mustBeFile(var, argPosition)</syntax_item>
    <syntax_item
    >C++: void mustBeFile(const ArrayOfVector&amp; args, int argPosition)</syntax_item>
  </syntax>

  <param_input>
    <param_input_item>
      <param_name>var</param_name>
      <param_description
      >a variable: a scalar string array or row vector characters array.</param_description>
    </param_input_item>
     <param_input_item>
      <param_name>argPosition</param_name>
      <param_description
      >a positive integer value: Position of input argument.</param_description>
    </param_input_item>
  </param_input>


  <description>
  <p><b
      >mustBeFile</b> checks that input path refers to file or raise an error.</p>
</description>

  <used_function />
  <bibliography />

<examples>
  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data
      ><![CDATA[mustBeFile(tempdir())
 mustBeFile([nelsonroot(), '/etc/startup.m'])]]>
    </example_item_data>
  </example_item>
</examples>

