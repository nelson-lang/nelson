    <language>en_US</language>
    <keyword>mustBeFolder</keyword>
    <short_description
  >Checks that input path refers to folder.</short_description>
    <syntax>
        <syntax_item>mustBeFolder(var)</syntax_item>
        <syntax_item>mustBeFolder(var, argPosition)</syntax_item>
        <syntax_item
    >C++: void mustBeFolder(const ArrayOfVector&amp; args, int argPosition)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>var</param_name>
            <param_description
      >a variable: a scalar string array or row vector characters array.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>argPosition</param_name>
            <param_description
      >a positive integer value: Position of input argument.</param_description>
        </param_input_item>
    </param_input>
    <description>
        <p>
            <b
      >mustBeFolder</b> checks that input path refers to folder or raise an error.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[mustBeFolder(tempdir())
mustBeFolder('hello_nelson')]]></example_item_data>
            </example_item>
        </examples>
