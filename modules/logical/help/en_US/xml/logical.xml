    <language>en_US</language>
    <keyword>logical</keyword>
    <short_description
  >Converts a numeric value to logical type.</short_description>
    <syntax>
        <syntax_item>Y = logical(X)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>X</param_name>
            <param_description>a numeric value.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>Y</param_name>
            <param_description>a logical value.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>logical</b> converts a numeric value to logical type.</p>
            <p
    >Nonzero value converted to true and zeros values converted to false.</p>
            <p>Complex numbers returns an error.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[A = eye(2, 2)
B = logical(A)
islogical(B)]]></example_item_data>
            </example_item>
        </examples>
        <see_also>
            <see_also_item>
                <link linkend="${types}islogical">islogical</link>
            </see_also_item>
        </see_also>
