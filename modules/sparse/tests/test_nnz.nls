//=============================================================================
S = sparse(eye(3,4));
assert_isequal(nnz(S), 3);
//=============================================================================
S = sparse([]);
assert_isequal(nnz(S), 0);
//=============================================================================
S = sparse(complex([]));
S = sparse(complex([]));
assert_isequal(nnz(S), 0);
//=============================================================================
A = [    0     1     2     3;
1     0     0     0;
5     0     1     0];
S = sparse(A);
assert_isequal(nnz(S), 6);
//=============================================================================
A = [1 1 1 1 1 1 0 0 1 0];
S = sparse(A);
assert_isequal(nnz(S), 7);
//=============================================================================
i = [  1,  1,  1,  2,  2,  3,  3,  3,  3 ];
j = [  1,  2,  5,  2,  3,  1,  3,  4,  5 ];
s = [ 11, 12, 15, 22, 23, 31, 33, 34, 35 ];
m = 3;
n = 5;
nz_max = 9;
S = sparse ( i, j, s, m, n, nz_max );
assert_isequal(nnz(S), 9);
//=============================================================================
