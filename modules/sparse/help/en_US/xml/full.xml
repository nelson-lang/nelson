  <language>en_US</language>
  <keyword>full</keyword>
  <short_description>Sparse to full matrix conversion.</short_description>
  <syntax>
    <syntax_item>M = full(sp)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>sp</param_name>
      <param_description
      >a matrix: double or logical, sparse.</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>M</param_name>
      <param_description>a matrix.</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p><b>full</b> converts a sparse matrix into its full representation.</p>
    <p
    >If input argument is already full then output argument will be equal to input argument.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[sp = sparse(eye(3,3))
F = full(sp)]]></example_item_data>
    </example_item>
  </examples>
