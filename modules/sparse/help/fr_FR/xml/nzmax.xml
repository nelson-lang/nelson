    <language>fr_FR</language>
    <keyword>nzmax</keyword>
    <short_description
  >Taille réservée pour les éléments non nuls.</short_description>
    <syntax>
        <syntax_item>v = nzmax(M)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>M</param_name>
            <param_description
      >une matrice : double ou logique, sparse ou non.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>v</param_name>
            <param_description>une valeur entière.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b
      >nzmax</b> retourne la quantité de stockage allouée pour les éléments non nuls.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[I = [1 2 3];
J = [3 1 2];
V = [32 42 53];
sp = sparse(I, J, V, 5, 4, 10)
size(sp)
nnz(sp)
nzmax(sp)]]></example_item_data>
            </example_item>
        </examples>
