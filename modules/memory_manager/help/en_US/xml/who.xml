  <language>en_US</language>
  <keyword>who</keyword>
  <short_description
  >List variables in memory or in .nh5 or in .mat file.</short_description>
  <syntax>
    <syntax_item>who</syntax_item>
    <syntax_item>s = who()</syntax_item>
    <syntax_item>who(scope)</syntax_item>
    <syntax_item>s = who(scope)</syntax_item>
    <syntax_item>who('-file', filename)</syntax_item>
    <syntax_item>s = who('-file', filename)</syntax_item>
    <syntax_item>who(... , var1, ..., varN)</syntax_item>
    <syntax_item>s = who(... , var1, ..., varN)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>scope</param_name>
      <param_description
      >a string: 'global', 'base', 'caller', 'local' or '-file'.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>filename</param_name>
      <param_description
      >string: an existing filename .nh5 or .mat file.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>var1, ..., varN</param_name>
      <param_description>string: variable name.</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>s</param_name>
      <param_description
      >a cell of strings: list of variable's name.</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p><b>who</b> displays current variable names.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
