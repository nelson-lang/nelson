	<language>en_US</language>
	<keyword>assignin</keyword>
	<short_description
  >Assignin value to a variable in a specified variables scope.</short_description>

	<syntax>
		<syntax_item>assignin(scope, variable_name, variable_value)</syntax_item>

	</syntax>
	

	<param_input>
		<param_input_item>
			<param_name>scope</param_name>
			<param_description
      >a string: 'global', 'base', 'caller', 'local'.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>variable_name</param_name>
			<param_description
      >a string: the name of variable destination.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>variable_value</param_name>
			<param_description>a variable to assign.</param_description>
		</param_input_item>

	</param_input>

	<description><p><b
      >assignin</b> assign value to a variable in a specified variables scope.</p>
	</description>

	<used_function />
	<bibliography />

<examples>
	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data
      ><![CDATA[assignin('base', 'X', 33);
Y = acquirevar('base', 'X');]]>
		</example_item_data>
	</example_item>

</examples>

