    <language>en_US</language>
    <keyword>varunlock</keyword>
    <short_description>Unlocks a variable.</short_description>
    <syntax>
        <syntax_item>varunlock(scope, variable_name)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>scope</param_name>
            <param_description
      >a string: 'global', 'base', 'caller', 'local'.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>variable_name</param_name>
            <param_description>a string: variable name.</param_description>
        </param_input_item>
    </param_input>
    <description>
        <p>
            <b>varunlock</b> unlocks a variable.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[y = 3;
varislock('local', 'y')
varlock('local', 'y')
varislock('local', 'y')
y = 4
varunlock('local', 'y')
varislock('local', 'y')
y = 4
varlock('local', 'ans')
varislock('local', 'ans')

]]></example_item_data>
            </example_item>
        </examples>
