  <language>fr_FR</language>
  <keyword>varunlock</keyword>
  <short_description>Déroque une variable.</short_description>
  <syntax>
    <syntax_item>varunlock(scope, variable_name)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>scope</param_name>
      <param_description
      >une chaîne : 'global', 'base', 'caller', 'local'.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>variable_name</param_name>
      <param_description>une chaîne : nom de la variable.</param_description>
    </param_input_item>
  </param_input>
  <description>
    <p><b>varunlock</b> déverrouille une variable.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[y = 3;
varislock('local', 'y')
varlock('local', 'y')
varislock('local', 'y')
y = 4
varunlock('local', 'y')
varislock('local', 'y')
y = 4
varlock('local', 'ans')
varislock('local', 'ans')

]]></example_item_data>
    </example_item>
  </examples>
