    <language>en_US</language>
    <keyword>qt_verbose</keyword>
    <short_description>show/hide Qt debug message.</short_description>
    <syntax>
        <syntax_item>r = qt_verbose()</syntax_item>
        <syntax_item>p = qt_verbose(logical)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>logical</param_name>
            <param_description
      >a logical: true to show messages, false to hide.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>r</param_name>
            <param_description>logical: current value</param_description>
        </param_output_item>
        <param_output_item>
            <param_name>p</param_name>
            <param_description>logical: previous value</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>qt_verbose</b> how/hide Qt debug message.</p>
            <p>This function is usefull to debug Qt and Qml.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="adv-cli"
      ><![CDATA[h = qt_verbose()]]></example_item_data>
            </example_item>
        </examples>
