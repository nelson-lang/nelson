    <language>en_US</language>
    <keyword>msgbox</keyword>
    <short_description>Creates a message dialog box.</short_description>
    <syntax>
        <syntax_item>h = msgbox(message)</syntax_item>
        <syntax_item>h = msgbox(message, mode)</syntax_item>
        <syntax_item>h = msgbox(message, title)</syntax_item>
        <syntax_item>h = msgbox(message, title, mode)</syntax_item>
        <syntax_item>h = msgbox(message, title, icon)</syntax_item>
        <syntax_item>h = msgbox(message, title, icon, mode)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>message</param_name>
            <param_description
      >a string or a cell of string: the message to display.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>title</param_name>
            <param_description
      >a string: the title of the dialog box.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>icon</param_name>
            <param_description
      >a string: 'none', 'error', 'help', 'warn' or 'question'.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>mode</param_name>
            <param_description
      >a string: 'modal', 'on' or 'nonmodal'.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>h</param_name>
            <param_description>a QObject handle.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>msgbox</b> creates an message dialog box.</p>
            <p>
                <b
      >h = msgbox(message, title, 'on')</b> specifies whether to replace an existing dialog box having the same name.</p>
            </description>
            <used_function />
            <bibliography />
            <examples>
                <example_item>
                    <example_item_type>nelson</example_item_type>
                    <example_item_description />
                    <example_item_data
        runnable="adv-cli"
      ><![CDATA[h = msgbox('help string')]]></example_item_data>
                </example_item>
                <example_item>
                    <example_item_type>nelson</example_item_type>
                    <example_item_description />
                    <example_item_data
        runnable="adv-cli"
      ><![CDATA[h = msgbox('help string', 'dialog title')]]></example_item_data>
                </example_item>
                <example_item>
                    <example_item_type>nelson</example_item_type>
                    <example_item_description />
                    <example_item_data
        runnable="adv-cli"
      ><![CDATA[h = msgbox('help string', 'dialog title')
h = msgbox('help string', 'dialog title', 'on')]]></example_item_data>
                </example_item>
            </examples>
