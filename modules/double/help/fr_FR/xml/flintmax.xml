  <language>fr_FR</language>
  <keyword>flintmax</keyword>
  <short_description
  >Plus grand entier consécutif représentable en virgule flottante.</short_description>
  <syntax>
    <syntax_item>R = flintmax()</syntax_item>
    <syntax_item>R = flintmax('double')</syntax_item>
    <syntax_item>R = flintmax('single')</syntax_item>
    <syntax_item>R = flintmax('like', V)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>V</param_name>
      <param_description>une variable double ou single.</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>R</param_name>
      <param_description>un double ou single.</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p><b
      >flintmax</b> renvoie le plus grand entier consécutif représentable au format virgule flottante.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[flintmax
flintmax('double')
flintmax('like', pi)
flintmax('single')]]></example_item_data>
    </example_item>
  </examples>
