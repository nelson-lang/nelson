A = 1;
B = 2;
R1 = (A >= B);
R2 = (B >= A);
R3 = (A >= A);
assert_isfalse(R1);
assert_istrue(R2);
assert_istrue(R3);
%=============================================================================
R =  [1 2 3] >= 3
REF = [false, false, true];
assert_isequal(R, REF);
