    <language>en_US</language>
    <keyword>randn</keyword>
    <short_description>Normally distributed random number.</short_description>
    <syntax>
        <syntax_item>M = randn</syntax_item>
        <syntax_item>M = randn(n)</syntax_item>
        <syntax_item>M = randn(x1, x2, ... , xN)</syntax_item>
        <syntax_item>M = randn(sz)</syntax_item>
        <syntax_item>M = randn(x1, x2, ... , xN, classname)</syntax_item>
        <syntax_item>M = randn(x1, x2, ... , xN, 'like', var)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>n</param_name>
            <param_description
      >a variable: n-by-n matrix will be generated.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>x1, x2, ... , xN</param_name>
            <param_description> x1-by-...-by-xN values </param_description>
        </param_input_item>
        <param_input_item>
            <param_name>classname</param_name>
            <param_description
      >a string: 'single' or 'double'</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>var</param_name>
            <param_description>a variable: single or double</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>M</param_name>
            <param_description>a matrix of random numbers.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b
      >randn</b> returns a matrix with normally distributed random elements having zero mean and variance one.</p>
            <p>By default, <b>randn</b> uses the ziggurat algorithm.</p>
            <p>seed can be modified using <b>rng</b>.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[rng('default');
randn
rng('default');
randn
]]></example_item_data>
            </example_item>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[rng('default');
randn(6)
]]></example_item_data>
            </example_item>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[rng('default');
randn(3, 2, 3)
]]></example_item_data>
            </example_item>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[rng('default');
randn(3, 2, 'single')
]]></example_item_data>
            </example_item>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[rng('default');
v = single([3, 3]);
randn(3, 2, 'like', v)
]]></example_item_data>
            </example_item>
        </examples>
