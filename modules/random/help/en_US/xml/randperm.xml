  <language>en_US</language>
  <keyword>randperm</keyword>
  <short_description>Random permutation of integers values.</short_description>

  <syntax>
  <syntax_item>p = randperm(n)</syntax_item>
  </syntax>

  <param_input>
    <param_input_item>
      <param_name>n</param_name>
      <param_description
      >Number of integers in sample interval (positive integer).</param_description>
    </param_input_item>
  </param_input>

  <param_output>
    <param_output_item>
      <param_name>p</param_name>
      <param_description>a row vector.</param_description>
    </param_output_item>
  </param_output>


  <description>
    <p><b
      >p = randperm(n)</b> returns a row vector containing a random permutation of <b
      >1:n</b>.</p>
  </description>

  <used_function />
  <bibliography />

