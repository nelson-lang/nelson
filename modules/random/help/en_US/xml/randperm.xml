    <language>en_US</language>
    <keyword>randperm</keyword>
    <short_description
  >Random permutation of integers values.</short_description>
    <syntax>
        <syntax_item>p = randperm(n, k)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>n</param_name>
            <param_description
      >Number of integers in sample interval (positive integer).</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>k</param_name>
            <param_description
      >Number of integers to select (positive integer).</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>p</param_name>
            <param_description>a row vector.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b
      >p = randperm(n)</b> returns a row vector containing a random permutation of <b
      >1:n</b>.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[randperm(7)]]></example_item_data>
            </example_item>
        </examples>
