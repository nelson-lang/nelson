    <language>fr_FR</language>
    <keyword>contains</keyword>
        <short_description
  >Vérifie si une chaîne contient un motif.</short_description>
    <syntax>
        <syntax_item>tf = contains(str, pattern)</syntax_item>
        <syntax_item
    >tf = contains(str, pattern,'IgnoreCase', true)</syntax_item>
        <syntax_item
    >tf = contains(str, pattern,'IgnoreCase', false)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>str</param_name>
        <param_description
      >une chaîne, un tableau de chaînes ou une cellule de chaînes.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>pattern</param_name>
            <param_description>une chaîne à rechercher.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>tf</param_name>
            <param_description>une matrice de booléens.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <b>contains</b> renvoie <b>true</b> si <b>str</b> contient <b
    >pattern</b>.</description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[
str = 'To make a mountain out of a molehill';
k = contains (str, 'hill')
k = contains (str, 'molehill')
k = contains (str, 'Hill', 'IgnoreCase', true)

A = {'Nel', 'son'; 'Nelson', 'Modules'}
k = contains(A, 'son')

A = ["Nel", "son"; "Nelson", "Modules"]
k = contains(A, 'son')

]]></example_item_data>
            </example_item>
        </examples>
