  <language>en_US</language>
  <keyword>char</keyword>
  <short_description>Converts to a character array.</short_description>
  <syntax>
    <syntax_item>res = char(var)</syntax_item>
    <syntax_item>res = char(var1, var2)</syntax_item>
    <syntax_item>res = char(var1, var2, ..., varN)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>var</param_name>
      <param_description
      >a cell of strings, string array or an numeric array.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>var1, var2, ..., varN</param_name>
      <param_description>strings or an numeric arrays.</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>res</param_name>
      <param_description>a string</param_description>
    </param_output_item>
  </param_output>
  <description><b
    >char</b> converts numerical input into character data by taking the corresponding unicode character for each element.</description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[M = [ 104   101   108   108   111;
20320   22909 32    32    32];
char(M)]]></example_item_data>
    </example_item>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[R = char('these', 'are', 'test', 'strings')]]></example_item_data>
    </example_item>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[R = char(["these"; "are"; "test"; "strings"])]]></example_item_data>
    </example_item>
  </examples>
