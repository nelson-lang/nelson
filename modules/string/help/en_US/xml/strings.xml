	<language>en_US</language>
	<keyword>strings</keyword>
	<short_description>Create string array without characters.</short_description>

	<syntax>
		<syntax_item>C = strings()</syntax_item>
		<syntax_item>C = strings(m)</syntax_item>
		<syntax_item>C = strings(m, n)</syntax_item>
		<syntax_item>C = strings(m, n, ... , p)</syntax_item>
		<syntax_item>C = strings(sz)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>m, n, ... , p</param_name>
			<param_description>dimensions of the string array to create.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>sz</param_name>
			<param_description>a vector of integer values (dimensions of the cell to create).</param_description>
		</param_input_item>
	</param_input>

	<param_output>

		<param_output_item>
			<param_name>C</param_name>
			<param_description>a string array</param_description>
		</param_output_item>
	</param_output>

	<description>
		<p><b>strings</b> returns a cell array of empty matrices.</p>
	</description>


	<used_function></used_function>
	<bibliography></bibliography>

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[A = eye(2, 4);
sz = size(A)
C = strings(sz)]]>
		</example_item_data>
	
	</example_item>

</examples>

