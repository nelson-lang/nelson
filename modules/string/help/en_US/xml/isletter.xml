    <language>en_US</language>
    <keyword>isletter</keyword>
    <short_description
  >Determine which characters are letters.</short_description>
    <syntax>
        <syntax_item>res = isletter(str)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>str</param_name>
            <param_description
      >scalar, vector, matrix or multidimensional array.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>res</param_name>
            <param_description>logical array</param_description>
        </param_output_item>
    </param_output>
    <description>
       <p><b>isletter</b> determines which characters are letters.</p>
    </description>
