  <language>en_US</language>
  <keyword>isspace</keyword>
  <short_description>Determine which characters are space.</short_description>

  <syntax>
    <syntax_item>res = isspace(str)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>str</param_name>
      <param_description
      >scalar, vector, matrix or multidimensional array.</param_description>
    </param_input_item>
  </param_input>

  <param_output>
    <param_output_item>
      <param_name>res</param_name>
      <param_description>logical array</param_description>
    </param_output_item>
  </param_output>

  <description><b
    >isletter</b> determines which characters are space characters.</description>

  <used_function />
  <bibliography />

<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data><![CDATA[isspace('Nel Son')]]>
    </example_item_data>
  </example_item>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data><![CDATA[isspace("六書 six writings")]]>
    </example_item_data>
  </example_item>

</examples>

