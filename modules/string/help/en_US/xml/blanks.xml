	<language>en_US</language>
	<keyword>blanks</keyword>
	<short_description>creates an string of blank characters.</short_description>

	<syntax>
		<syntax_item>r = blanks(n)</syntax_item>
	</syntax>

	<param_input>
		<param_input_item>
			<param_name>n</param_name>
			<param_description>an value integer, number of blanks.</param_description>
		</param_input_item>
	</param_input>


	<param_output>
		<param_output_item>
			<param_name>r</param_name>
			<param_description>a character array with n blanks</param_description>
		</param_output_item>
	</param_output>

	<description><b>blanks</b> creates an string of blank characters.</description>

	<used_function />
	<bibliography />

<examples>


	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[blanks(4)]]>
		</example_item_data>
	</example_item>

</examples>

