	<language>en_US</language>
	<keyword>string</keyword>
	<short_description>string array constructor.</short_description>

	<syntax>
		<syntax_item>res = string(var)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>var</param_name>
			<param_description
      >characters, a cell of characters, or an logical or numeric array.</param_description>
		</param_input_item>
	</param_input>

	<param_output>
		<param_output_item>
			<param_name>res</param_name>
			<param_description>a string array</param_description>
		</param_output_item>
	</param_output>

	<description><b>string</b> converts input into string array.</description>

	<used_function />
	<bibliography />

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data
      ><![CDATA[R = string({'these', 'are'; 'test', 'strings'})
R2 = ["these", "are"; "test", "strings"];]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data
      ><![CDATA[M = [ 104   101   108   108   111;
20320   22909 32    32    32];
R = string(M)
D = double(R)]]>
		</example_item_data>
	</example_item>


</examples>

