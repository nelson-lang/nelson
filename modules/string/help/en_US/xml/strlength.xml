  <language>en_US</language>
  <keyword>strlength</keyword>
  <short_description>Length of strings in cell of strings or string array.</short_description>

  <syntax>
    <syntax_item>len = strlength(ce)</syntax_item>

  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>ce</param_name>
      <param_description>a string, string array or cell of strings.</param_description>
    </param_input_item>
  </param_input>

  <param_output>
    <param_output_item>
      <param_name>len</param_name>
      <param_description>a matrix of integer values: length of strings.</param_description>
    </param_output_item>
  </param_output>

  <description><b>strlength</b> returns length of strings.</description>
  <used_function></used_function>
  <bibliography></bibliography>

<examples>


  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description></example_item_description>
    <example_item_data><![CDATA[
str = 'To make a mountain out of a molehill';
k = strlength(str)

A = {'Nel', 'son'; 'Toolboxes', 'Modules'}
k = strlength(A)

B = ["Nel", NaN, "son"; "is", "open", "source"];
k = strlength(B)
]]>
    </example_item_data>
  </example_item>
</examples>

