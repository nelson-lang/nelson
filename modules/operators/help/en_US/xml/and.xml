	<language>en_US</language>
	<keyword>and</keyword>
	<short_description>logical 'AND' operator, &amp;</short_description>

	<syntax>
		<syntax_item>C = and(A, B)</syntax_item>
		<syntax_item>C = A &amp; B</syntax_item>
	</syntax>

	<param_input>
		<param_input_item>
			<param_name>A</param_name>
			<param_description>a variable</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>B</param_name>
			<param_description>a variable</param_description>
		</param_input_item>
	</param_input>

	<param_output>

		<param_output_item>
			<param_name>C</param_name>
			<param_description>result of A &amp; B</param_description>
		</param_output_item>
	</param_output>

	<description>
		<p><b>C = and(A, B)</b> performs a logical <b>AND</b> operation.</p>
	</description>


	<used_function />
	<bibliography />

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data
      ><![CDATA[A = [6 8 0; 0 3 89; 15 0 0]
B = [66 56 0; 11 33 55; -11 0 0]
C = A & B
D = and(B, A)
C == D]]>
		</example_item_data>
	</example_item>

</examples>

