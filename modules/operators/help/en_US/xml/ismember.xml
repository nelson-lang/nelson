    <language>en_US</language>
    <keyword>ismember</keyword>
    <short_description
  >Array elements that are members of another array.</short_description>
    <syntax>
        <syntax_item>T = ismember(A, B)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>A</param_name>
            <param_description>a variable</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>B</param_name>
            <param_description>a variable</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>T</param_name>
            <param_description>result of ismember.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b
      >T = ismember(A, B)</b> returns an array of logical where the data in <b
      >A</b> is found in <b>B</b>.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[A = [50 30 40 20];
B = [20 40 40 40 60 80];
T = ismember(A, B)

T = ismember(["a","b","f"], ["b", "f", "c"])

]]></example_item_data>
            </example_item>
        </examples>
