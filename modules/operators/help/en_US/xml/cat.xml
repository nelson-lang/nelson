    <language>en_US</language>
    <keyword>cat</keyword>
    <short_description>Concatenate arrays.</short_description>
    <syntax>
        <syntax_item>R = cat(dim, A, B)</syntax_item>
        <syntax_item>R = cat(dim, A1, A2, ..., An)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>dim</param_name>
            <param_description
      >Dimension to operate along: positive integer scalar.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>A</param_name>
            <param_description>a variable: first input.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>B</param_name>
            <param_description>a variable: second input.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>A1, A2, ..., An</param_name>
            <param_description>List of inputs to concatenate</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>R</param_name>
            <param_description>concatenated array</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b
      >R = cat(dim, M1, M2, ... , MN)</b> returns the concatenation of M1, M2, ... , MN along the dimension <b
      >dim</b>.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[A = eye(2, 2);
B = ones(2, 2);
C = cat(2, A, B)]]></example_item_data>
            </example_item>
        </examples>
