    <language>en_US</language>
    <keyword>mtimes</keyword>
    <short_description>Matrix multiplication, * operator</short_description>
    <syntax>
        <syntax_item>C = mtimes(A, B)</syntax_item>
        <syntax_item>C = A * B</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>A</param_name>
            <param_description>a variable</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>B</param_name>
            <param_description>a variable</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>C</param_name>
            <param_description>result of A * B</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b
      >C = mtimes(A, B)</b> performs matrix multiplication operation: A * B.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[mtimes(3, 4)
3 * 4]]></example_item_data>
            </example_item>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[M1 = [2 6 10; 4 8 70];
M2 = [-25 88 1; 23 29 41; 24 40 0];
M1 * M2]]></example_item_data>
            </example_item>
        </examples>
        <see_also>
            <see_also_item>
                <link linkend="${operators}times">times</link>
            </see_also_item>
        </see_also>
