	<language>en_US</language>
	<keyword>vertcat</keyword>
	<short_description>Vertical concatenation.</short_description>

	<syntax>
		<syntax_item>R = vertcat(M1, M2, ... , MN)</syntax_item>
		<syntax_item>R = [M1; M2; ... ; MN]</syntax_item>
	</syntax>

	<param_input>
		<param_input_item>
			<param_name>M1</param_name>
			<param_description>a variable</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>M2</param_name>
			<param_description>a variable</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>MN</param_name>
			<param_description>a variable</param_description>
		</param_input_item>

	</param_input>

	<param_output>

		<param_output_item>
			<param_name>R</param_name>
			<param_description>result of [M1; M2; ... ; MN]</param_description>
		</param_output_item>

	</param_output>

	<description>
		<p><b
      >R = vertcat(M1, M2, ... , MN)</b> returns the vertical concatenation of M1, M2, ... , MN along the dimension 1.</p>
	</description>


	<used_function />
	<bibliography />

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data
      ><![CDATA[A = eye(2, 2);
B = ones(2, 2);
C = vertcat(A, B)
D = [A; B]]]>
		</example_item_data>
	</example_item>


	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[A = 'nel';
B = 'son';
C = vertcat(A, B)]]>
		</example_item_data>
	</example_item>

</examples>

