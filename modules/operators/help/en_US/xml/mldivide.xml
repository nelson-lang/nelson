	<language>en_US</language>
	<keyword>mldivide</keyword>
	<short_description>Matrix left division, \ operator.</short_description>

	<syntax>
		<syntax_item>C = mldivide(A, B)</syntax_item>
		<syntax_item>C = A \ B</syntax_item>
	</syntax>

	<param_input>
		<param_input_item>
			<param_name>A</param_name>
			<param_description>a variable</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>B</param_name>
			<param_description>a variable</param_description>
		</param_input_item>
	</param_input>

	<param_output>

		<param_output_item>
			<param_name>C</param_name>
			<param_description>result of A \ B</param_description>
		</param_output_item>
	</param_output>

	<description>
		<p><b>C = mldivide(A, B)</b> returns the matrix left division of A and B.</p>
	</description>


	<used_function />
	<bibliography />

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[B = ones(3, 4)
A = B *2
A \ B]]>
		</example_item_data>

	</example_item>
</examples>

