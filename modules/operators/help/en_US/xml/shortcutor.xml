	<language>en_US</language>
	<keyword>shortcutor</keyword>
	<short_description>Short circuit 'OR' operator, ||</short_description>

	<syntax>
		<syntax_item>C = A || B</syntax_item>
	</syntax>

	<param_input>
		<param_input_item>
			<param_name>A</param_name>
			<param_description>a variable</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>B</param_name>
			<param_description>a variable</param_description>
		</param_input_item>
	</param_input>

	<param_output>

		<param_output_item>
			<param_name>C</param_name>
			<param_description>result of A || B</param_description>
		</param_output_item>
	</param_output>

	<description>
		<p><b>C = A || B</b> performs a logical <b
      >OR</b> operation, the second operand is evaluated only when the result is not fully determined by the first operand.</p>
	</description>


	<used_function />
	<bibliography />

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data
      ><![CDATA[A = [6 8 0; 0 3 89; 15 0 0]
B = [66 56 0; 11 33 55; -11 0 0]
C = A || B
]]>
		</example_item_data>
	</example_item>

</examples>

