  <language>fr_FR</language>
  <keyword>ctranspose</keyword>
  <short_description
  >Renvoie la transposée conjuguée complexe : opérateur '</short_description>
  <syntax>
    <syntax_item>C= ctranspose(A)</syntax_item>
    <syntax_item>C = A'</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>A</param_name>
      <param_description>une variable</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>C</param_name>
      <param_description
      >résultat : transposée conjuguée complexe de A.</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p><b
      >C = ctranspose(A)</b> renvoie la transposée conjuguée complexe de A.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[A = 3
B = A']]></example_item_data>
    </example_item>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[A = -i
B = A']]></example_item_data>
    </example_item>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[ A = sparse(eye(3, 4) * i)
B = A']]></example_item_data>
    </example_item>
  </examples>
