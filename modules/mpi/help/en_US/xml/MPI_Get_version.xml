  <language>en_US</language>
  <keyword>MPI_Get_version</keyword>
  <short_description>Return the version number of MPI.</short_description>

  <syntax>
    <syntax_item>[major, minor] = MPI_Get_version()</syntax_item>
  </syntax>



  <param_output>
    <param_output_item>
      <param_name>major</param_name>
      <param_description>an integer value.</param_description>
    </param_output_item>
    <param_output_item>
      <param_name>minor</param_name>
      <param_description>an integer value.</param_description>
    </param_output_item>
      </param_output>

  <description>
  <p>Return the version number of MPI.</p>
</description>

  <used_function></used_function>
  <bibliography></bibliography>

  <see_also>
    <see_also_item>
      <link linkend="${mpi}MPI_Init">MPI_Init</link>
    </see_also_item>
    <see_also_item>
      <link linkend="${mpi}MPI_Finalize">MPI_Finalize</link>
    </see_also_item>
  </see_also>


<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description></example_item_description>
    <example_item_data><![CDATA[if ~MPI_Initialized()
  MPI_Init();
end
[major, minor] = MPI_Get_version()
if MPI_Initialized()
  MPI_Finalize();
end
]]>
    </example_item_data>
  </example_item>

</examples>

