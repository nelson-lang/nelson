  <language>en_US</language>
  <keyword>MPI_Comm_get_name</keyword>
  <short_description>Return the print name from the communicator.</short_description>

  <syntax>
    <syntax_item>MPI_Comm_get_name(comm)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>comm</param_name>
      <param_description>a handle: a MPI_Comm object.</param_description>
    </param_input_item>
  </param_input>

  <description>
  <p><b>MPI_Comm_get_name(comm)</b> returns the print name from the communicator.</p>
 </description>

  <used_function></used_function>
  <bibliography></bibliography>

<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description>CLI required</example_item_description>
    <example_item_data><![CDATA[
if ~MPI_Initialized()
  MPI_Init();
end
comm = MPI_Comm_object();
MPI_Comm_get_name(comm)
delete(comm)
if MPI_Initialized()
  MPI_Finalize();
end
]]>
    </example_item_data>
  </example_item>

</examples>

