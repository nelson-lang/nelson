  <keyword>MPI_Comm_used</keyword>
  <short_description
  >Returns list of current used MPI_Comm handle.</short_description>

  <syntax>
    <syntax_item>r = MPI_Comm_used()</syntax_item>
  </syntax>

  <param_output>

    <param_output_item>
      <param_name>h</param_name>
      <param_description>a vector of MPI_Comm handle.</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p>Returns list of current used MPI_Comm handle.</p>
</description>

  <used_function />
  <bibliography />

  <see_also>
    <see_also_item>
      <link linkend="${mpi}MPI_Comm_delete">MPI_Comm_delete</link>
    </see_also_item>
  </see_also>


<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description>CLI required</example_item_description>
    <example_item_data
      ><![CDATA[
if ~MPI_Initialized()
  MPI_Init();
end
comm = MPI_Comm_object();
MPI_Comm_used
delete(comm)
MPI_Comm_used
if MPI_Initialized()
  MPI_Finalize();
end
]]>
    </example_item_data>
  </example_item>

</examples>


