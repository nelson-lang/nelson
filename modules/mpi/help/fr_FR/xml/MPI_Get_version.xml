    <keyword>MPI_Get_version</keyword>
    <short_description>Renvoie le numéro de version de MPI.</short_description>
    <syntax>
        <syntax_item>[major, minor] = MPI_Get_version()</syntax_item>
    </syntax>
    <param_output>
        <param_output_item>
            <param_name>major</param_name>
            <param_description>entier.</param_description>
        </param_output_item>
        <param_output_item>
            <param_name>minor</param_name>
            <param_description>an integer value.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>Renvoie le numéro de version de MPI.</p>
    </description>
    <used_function />
    <bibliography />
    <examples>
        <example_item>
            <example_item_type>nelson</example_item_type>
            <example_item_description />
            <example_item_data
        runnable="false"
      ><![CDATA[if ~MPI_Initialized()
  MPI_Init();
end
[major, minor] = MPI_Get_version()
if MPI_Initialized()
  MPI_Finalize();
end
]]></example_item_data>
        </example_item>
    </examples>
