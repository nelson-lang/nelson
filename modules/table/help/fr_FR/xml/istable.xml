    <language>fr_FR</language>
    <keyword>istable</keyword>
    <short_description>Déterminer si l'entrée est une table.</short_description>
    <syntax>
        <syntax_item>tf = istable(A)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>A</param_name>
            <param_description>Tableau d'entrée.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>tf</param_name>
        <param_description
      >un logique : vrai si c'est une table.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p><b>tf = istable(A)</b> renvoie <b>true</b> si <b
      >A</b> est une table, et <b>false</b> sinon.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[T = table();
istable(T)
M = magic(6);
istable(M)]]></example_item_data>
            </example_item>
        </examples>
