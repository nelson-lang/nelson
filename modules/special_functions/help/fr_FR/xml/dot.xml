    <language>fr_FR</language>
    <keyword>dot</keyword>
    <short_description>Produit scalaire.</short_description>
    <syntax>
        <syntax_item>R = dot(A, B)</syntax_item>
        <syntax_item>R = dot(A, B, dim)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>A, B</param_name>
            <param_description>tableaux numériques.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>dim</param_name>
            <param_description
      >scalaire entier positif : Dimension le long de laquelle opérer.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>R</param_name>
            <param_description>Produit scalaire.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>R = dot(A, B)</b> retourne le produit scalaire de <b>A</b> et <b
      >B</b>.</p>
        </description>
        <used_function />
        <bibliography>https://en.wikipedia.org/wiki/Dot_product</bibliography>
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[A = [1 2 3;4 5 6;7 8 9];
B = [9 8 7;6 5 4;3 2 1];
R = dot(A, B)
R = dot(A, B, 2)]]></example_item_data>
            </example_item>
        </examples>
