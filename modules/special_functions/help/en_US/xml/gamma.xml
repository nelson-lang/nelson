  <language>en_US</language>
  <keyword>gamma</keyword>
  <short_description>Gamma special function</short_description>

  <syntax>
    <syntax_item>R = gamma(M)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>M</param_name>
      <param_description>a real single or real double matrix.</param_description>
    </param_input_item>
  </param_input>

  <param_output>

    <param_output_item>
      <param_name>R</param_name>
      <param_description>result of gamma function.</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p><b>gamma</b> computes the gamma function.</p>
    <p><b>gamma</b> is an extension of the <b>factorial</b> function.</p>
  </description>


  <used_function></used_function>
  <bibliography></bibliography>

<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description></example_item_description>
    <example_item_data>
      <![CDATA[R = gamma([-pi:0.1:pi])]]>
    </example_item_data>
  </example_item>


</examples>

