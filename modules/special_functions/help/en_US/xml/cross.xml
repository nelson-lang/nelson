    <language>en_US</language>
    <keyword>cross</keyword>
    <short_description>Cross product.</short_description>
    <syntax>
        <syntax_item>R = cross(A, B)</syntax_item>
        <syntax_item>R = cross(A, B, dim)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>A, B</param_name>
            <param_description>numeric arrays.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>dim</param_name>
            <param_description
      >positive integer scalar: Dimension to operate along.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>R</param_name>
            <param_description>Vector cross Product.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>R = cross(A, B)</b> returns the cross product of <b>A</b> and <b
      >B</b>.</p>
        </description>
        <used_function />
        <bibliography>https://en.wikipedia.org/wiki/Cross_product</bibliography>
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[A = [1 2 3;4 5 6;7 8 9];
B = [9 8 7;6 5 4;3 2 1];
R = cross(A, B)
R = cross(A, B, 2)]]></example_item_data>
            </example_item>
        </examples>
        <see_also>
            <see_also_item>
                <link linkend="${special_functions}dot">dot</link>
            </see_also_item>
        </see_also>
