    <language>en_US</language>
    <keyword>betainc</keyword>
    <short_description>Incomplete beta function</short_description>
    <syntax>
        <syntax_item>R = betainc(X, Z, W)</syntax_item>
        <syntax_item>R = betainc(X, Z, W, tail)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>X</param_name>
            <param_description
      >a real single or real double matrix. It must be in the closed interval [0, 1].</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>Z</param_name>
            <param_description
      >a real single or real double matrix. It must be nonnegative.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>W</param_name>
            <param_description
      >a real single or real double matrix. It must be nonnegative.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>tail</param_name>
            <param_description
      >a string 'upper' or 'lower' (default).</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>R</param_name>
            <param_description>result of betainc function.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>betainc</b> computes the Incomplete beta function.</p>
            <p
    >All arrays must be the same size or any of them can be scalar.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[R = betainc(0.5, 1:10, 3)]]></example_item_data>
            </example_item>
        </examples>
        <see_also>
            <see_also_item>
                <link linkend="${special_functions}gamma">gamma</link>
            </see_also_item>
        </see_also>
        <history>
