	<language>en_US</language>
	<keyword>nativecharset</keyword>
	<short_description
  >Find all charset matches that appear to be consistent with the input</short_description>

	<syntax>
		<syntax_item>ce = nativecharset(bytes)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>bytes</param_name>
			<param_description
      >a uint8 vector, or string or row characters array</param_description>
		</param_input_item>
	</param_input>

	<param_output>
		<param_output_item>
			<param_name>ce</param_name>
			<param_description>a cell of strings.</param_description>
		</param_output_item>
	</param_output>

	<description>
	<p><b
      >nativecharset</b> find all charset matches that appear to be consistent with the input, returning a cell of string with results.</p>
	<p>The results are ordered with the best quality match first.</p>
	<p
    >List of characters set: http://www.iana.org/assignments/character-sets/character-sets.xhtml</p>
	</description>
	<used_function />
	<bibliography>ICU library</bibliography>

<examples>
	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data
      ><![CDATA[C = uint8([194   232   240   242   243   224   235   252   237   224   255]);
nativecharset(R)]]>
		</example_item_data>
	</example_item>
</examples>

