    <language>fr_FR</language>
    <keyword>resume</keyword>
    <short_description>Reprend un objet audioplayer.</short_description>
    <syntax>
        <syntax_item>resume(playObj)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>playObj</param_name>
            <param_description>un objet audioplayer.</param_description>
        </param_input_item>
    </param_input>
    <description>
        <b>resume</b> reprend un objet audioplayer.</description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[signal = rand(2, 44100) - 0.5;
playObj = audioplayer(signal, 44100, 16)
play(playObj)
pause(playObj)
stop(playObj)
resume(playObj)
playObj]]></example_item_data>
            </example_item>
        </examples>
