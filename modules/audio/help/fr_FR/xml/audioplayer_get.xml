    <keyword>audioplayer_get</keyword>
    <short_description
  >Obtient la valeur de propriété de l'interface audioplayer.</short_description>
    <syntax>
        <syntax_item>v = get(h, propertyname)</syntax_item>
        <syntax_item>v = audioplayer_get(h, propertyname)</syntax_item>
        <syntax_item>v = h.propertyname</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>h</param_name>
            <param_description>un objet audioplayer.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>propertyname</param_name>
            <param_description
      >une chaîne : le nom de la propriété de l'objet audioplayer.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>v</param_name>
            <param_description>une variable Nelson.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p
    >La fonction retourne la valeur de la propriété spécifiée dans la chaîne, propertyname.</p>
    </description>
    <used_function />
    <bibliography />
    <examples>
        <example_item>
            <example_item_type>nelson</example_item_type>
            <example_item_description />
            <example_item_data
        runnable="cli"
      ><![CDATA[signal = rand(2, 44100) - 0.5;
playObj = audioplayer(signal, 44100, 16)
playObj.Running
]]></example_item_data>
        </example_item>
    </examples>
