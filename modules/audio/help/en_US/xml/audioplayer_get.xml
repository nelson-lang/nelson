  <language>en_US</language>
  <keyword>audioplayer_get</keyword>
  <short_description>Get property value from audioplayer interface.</short_description>

  <syntax>
    <syntax_item>v = get(h, propertyname)</syntax_item>
    <syntax_item>v = audioplayer_get(h, propertyname)</syntax_item>
    <syntax_item>v = h.propertyname</syntax_item>

  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>h</param_name>
      <param_description>an audioplayer object.</param_description>
    </param_input_item>

    <param_input_item>
      <param_name>propertyname</param_name>
      <param_description>a string: the property's name of audioplayer object.</param_description>
    </param_input_item>
  </param_input>

  <param_output>
    <param_output_item>
      <param_name>v</param_name>
      <param_description>a nelson variable.</param_description>
    </param_output_item>
  </param_output>


  <description>
    <p>The function returns the value of the property specified in the string, propertyname.</p>
  </description>

  <used_function></used_function>
  <bibliography></bibliography>
  

<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description></example_item_description>
    <example_item_data><![CDATA[signal = rand(2, 44100) - 0.5;
playObj = audioplayer(signal, 44100, 16)
playObj.Running
]]>
    </example_item_data>
  </example_item>

</examples>

