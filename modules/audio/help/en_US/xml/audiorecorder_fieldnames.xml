  <keyword>audiorecorder_fieldnames</keyword>
  <short_description
  >Returns the properties name of an audiorecorder object.</short_description>
  <syntax>
    <syntax_item>l = audiorecorder_fieldnames(h)</syntax_item>
    <syntax_item>l = fieldnames(h)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>h</param_name>
      <param_description>a audiorecorder object.</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>l</param_name>
      <param_description>a cell of strings.</param_description>
    </param_output_item>
  </param_output>
  <description><b
    >fieldnames</b> returns a cell of strings with properties name.</description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[recObj = audiorecorder()
fieldnames(recObj)
delete(recObj)
clear recObj]]></example_item_data>
    </example_item>
  </examples>
