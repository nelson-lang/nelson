  <syntax>
    <syntax_item>lang = getdefaultlanguage()</syntax_item>
  </syntax>

  <param_output>

    <param_output_item>
      <param_name>lang</param_name>
      <param_description>a string: 'en_US' by default.</param_description>
    </param_output_item>

