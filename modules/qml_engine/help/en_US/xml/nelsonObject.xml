  <language>en_US</language>
  <keyword>nelsonObject</keyword>
  <short_description>nelson object callable from QML.</short_description>

  <syntax>
  <syntax_item>nelson.disp(msg)</syntax_item>
  <syntax_item>nelson.evaluate(cmd)</syntax_item>
  <syntax_item>nelson.processevent()</syntax_item>
  <syntax_item>nelson.call(function_name)</syntax_item>
  <syntax_item>nelson.call(function_name, arg1, ..., arg5)</syntax_item>

  </syntax>

  <param_input>
    <param_input_item>
      <param_name>msg</param_name>
      <param_description>a string.</param_description>
     </param_input_item>
    <param_input_item>
      <param_name>cmd</param_name>
      <param_description>a string.</param_description>
     </param_input_item>
    <param_input_item>
      <param_name>function_name</param_name>
      <param_description
      >a string: nelson function name to call</param_description>
     </param_input_item>
    <param_input_item>
      <param_name>arg1, ..., arg5</param_name>
      <param_description>javascript variables</param_description>
     </param_input_item>

  </param_input>


  <description>
  <p><b
      >nelson</b> object contains some methods used as callback to call nelson from QML</p>
  </description>

  <used_function />
