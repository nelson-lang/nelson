  <language>en_US</language>
  <keyword>qml_evaluatestring</keyword>
  <short_description>Evaluates a js string.</short_description>

  <syntax>
    <syntax_item>r = qml_evaluatestring(string_to_eval)</syntax_item>
  </syntax>

  
  <param_input>
    <param_input_item>
      <param_name>string_to_eval</param_name>
      <param_description>a string: a js code.</param_description>
    </param_input_item>
 
  </param_input>

  <param_output>

    <param_output_item>
      <param_name>r</param_name>
      <param_description>a double, logical, int or string.</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p>Evaluates a js string.</p>
    <p
    >If returned value cannot be converted to a basic type, it will converted to string.</p>

</description>

  <used_function />
  <bibliography />

  <see_also>
    <see_also_item>
      <link linkend="${qml_engine}qml_evaluatefile">qml_evaluatefile</link>
    </see_also_item>
  </see_also>


<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data><![CDATA[qml_evaluatestring('a = 2 + 4')]]>
    </example_item_data>
  </example_item>

</examples>


