    <see_also_item>
      <link linkend="${qml_engine}QObject_get">QObject_get (get)</link>
    </see_also_item>

  </see_also>


<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data
      ><![CDATA[h1 = errordlg()
h2 = errordlg()
hr = QObject_findchildren(QObject_root(), 'errordlg', true)]]>
    </example_item_data>
  </example_item>

</examples>


