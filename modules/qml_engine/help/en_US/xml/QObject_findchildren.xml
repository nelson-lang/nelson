  <language>en_US</language>
  <keyword>QObject_findchildren</keyword>
  <short_description
  >Returns all children of this object with the given name.</short_description>

  <syntax>
    <syntax_item
    >hr = QObject_findchildren(h, objectName, recursive)</syntax_item>
  </syntax>

  
  <param_input>
    <param_input_item>
      <param_name>h</param_name>
      <param_description>an QObject handle.</param_description>
    </param_input_item>

    <param_input_item>
      <param_name>objectName</param_name>
      <param_description>a string.</param_description>
    </param_input_item>

    <param_input_item>
      <param_name>recursive</param_name>
      <param_description
      >a logical: true (The search is performed recursively).</param_description>
    </param_input_item>

  </param_input>

  <param_output>

    <param_output_item>
      <param_name>hr</param_name>
      <param_description>a vector of QObject handle.</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p>Returns all children of this object with the given name.</p>
</description>

  <used_function />
  <bibliography />

  <see_also>
    <see_also_item>
      <link linkend="${qml_engine}QObject_set">QObject_set (set)</link>
