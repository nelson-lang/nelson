    <language>fr_FR</language>
    <keyword>qml_evaluatestring</keyword>
    <short_description>Évalue une chaîne JS.</short_description>
    <syntax>
        <syntax_item>r = qml_evaluatestring(string_to_eval)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>string_to_eval</param_name>
            <param_description>une chaîne : code JS.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
        <param_name>r</param_name>
        <param_description>a double, logical, int ou string.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>Évalue une chaîne JS.</p>
        <p
    >Si la valeur retournée ne peut pas être convertie en type de base, elle sera convertie en chaîne.</p>
    </description>
    <used_function />
    <bibliography />
    <examples>
        <example_item>
            <example_item_type>nelson</example_item_type>
            <example_item_description />
                                <example_item_data
        runnable="adv-cli"
      ><![CDATA[qml_evaluatestring('a = 2 + 4')]]></example_item_data>
        </example_item>
    </examples>
