    <syntax_item>nelson.evaluate(cmd)</syntax_item>
    <syntax_item>nelson.processevent()</syntax_item>
    <syntax_item>nelson.call(function_name)</syntax_item>
    <syntax_item>nelson.call(function_name, arg1, ..., arg5)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>msg</param_name>
