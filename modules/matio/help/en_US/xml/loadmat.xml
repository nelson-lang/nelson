	<language>en_US</language>
	<keyword>loadmat</keyword>
	<short_description>load data from .mat file into Nelson's workspace.</short_description>

	<syntax>
		<syntax_item>loadmat(filename)</syntax_item>
		<syntax_item>st = loadmat(filename)</syntax_item>
		<syntax_item>loadmat(filename, var1, ..., varN)</syntax_item>
		<syntax_item>st = loadmat(filename, var1, ..., varN)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>filename</param_name>
			<param_description>a string: .mat filename.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>var1, ..., varN</param_name>
			<param_description>string: Names of variables to load into Nelson's workspace.</param_description>
		</param_input_item>
	</param_input>

	<param_output>
		<param_output_item>
			<param_name>st</param_name>
			<param_description>a structure with variables name as fieldnames.</param_description>
		</param_output_item>
	</param_output>

	<description>
		<p><b>loadmat</b> loads data from .mat file to Nelson's workspace.</p>
	</description>


	<used_function></used_function>
	<bibliography>Thanks to MATIO library (http://sourceforge.net/projects/matio/).</bibliography>

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[A = ones(3, 4);
B = 'hello for open mat users';
savemat([tempdir(), 'example_loadmat.mat'], 'A', 'B')
clear;
st = loadmat([tempdir(), 'example_loadmat.mat']);
who
st.A
st.B
clear
who
loadmat([tempdir(), 'example_loadmat.mat']);
who
A
B
]]>
		</example_item_data>
	</example_item>

</examples>

