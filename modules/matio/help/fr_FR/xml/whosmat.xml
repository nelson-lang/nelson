  <language>fr_FR</language>
  <keyword>whosmat</keyword>
  <short_description
  >Liste les variables d'un fichier .mat valide avec tailles et types.</short_description>
  <syntax>
    <syntax_item>whosmat(filename)</syntax_item>
    <syntax_item>st = whosmat(filename)</syntax_item>
    <syntax_item>whosmat(filename, var1, ..., varN)</syntax_item>
    <syntax_item>st = whosmat(filename, var1, ..., varN)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>filename</param_name>
      <param_description>une chaîne : nom de fichier .mat.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>var1, ..., varN</param_name>
      <param_description
      >une chaîne : noms des variables à inspecter.</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>st</param_name>
      <param_description
      >contient des informations sur les variables dans le tableau de structures st.</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p><b>whosmat</b> liste les variables d'un fichier .mat valide.</p>
  </description>
  <used_function />
  <bibliography
  >Remerciements à la bibliothèque MATIO (http://sourceforge.net/projects/matio/).</bibliography>
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[A = ones(3, 4);
B = 'Nelson';
C = sparse(true);
D = sparse(3i);		
savemat([tempdir(), 'example_whosmat-v7.3.mat'], 'A', 'B', 'C', 'D', '-v7.3')
whosmat([tempdir(), 'example_whosmat-v7.3.mat'])
st = whosmat([tempdir(), 'example_whosmat-v7.3.mat'])]]></example_item_data>
    </example_item>
  </examples>
