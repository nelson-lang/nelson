        <language>fr_FR</language>
        <keyword>loadmat</keyword>
        <short_description
  >charge des données depuis un fichier .mat dans l'espace de travail de Nelson.</short_description>
    <syntax>
        <syntax_item>loadmat(filename)</syntax_item>
        <syntax_item>st = loadmat(filename)</syntax_item>
        <syntax_item>loadmat(filename, var1, ..., varN)</syntax_item>
        <syntax_item>st = loadmat(filename, var1, ..., varN)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>filename</param_name>
            <param_description
      >une chaîne : nom de fichier .mat.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>var1, ..., varN</param_name>
            <param_description
      >une chaîne : noms des variables à charger dans l'espace de travail de Nelson.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>st</param_name>
            <param_description
      >une structure dont les noms de champs correspondent aux noms des variables.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b
      >loadmat</b> charge des données depuis un fichier .mat vers l'espace de travail de Nelson.</p>
        </description>
        <used_function />
      <bibliography
  >Remerciements à la bibliothèque MATIO (http://sourceforge.net/projects/matio/).</bibliography>
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[A = ones(3, 4);
B = 'hello for open mat users';
savemat([tempdir(), 'example_loadmat.mat'], 'A', 'B')
clear;
st = loadmat([tempdir(), 'example_loadmat.mat']);
who
st.A
st.B
clear
who
loadmat([tempdir(), 'example_loadmat.mat']);
who
A
B
]]></example_item_data>
            </example_item>
        </examples>
