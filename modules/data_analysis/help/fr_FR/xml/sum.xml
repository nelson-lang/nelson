    <language>fr_FR</language>
    <keyword>sum</keyword>
    <short_description>Somme des éléments d'un tableau.</short_description>
    <syntax>
        <syntax_item>R = sum(M)</syntax_item>
        <syntax_item>R = sum(M, d)</syntax_item>
        <syntax_item>R = sum(M, d)</syntax_item>
        <syntax_item>R = sum(M, d, t)</syntax_item>
        <syntax_item>R = sum(M, d, t, f)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
        <param_name>M</param_name>
        <param_description
      >un tableau de double, single, entiers, ...</param_description>
        </param_input_item>
        <param_input_item>
        <param_name>d</param_name>
        <param_description
      >dimension le long de laquelle opérer : entier positif scalaire.</param_description>
        </param_input_item>
        <param_input_item>
        <param_name>t</param_name>
        <param_description
      >une chaîne : 'default', 'double' ou 'native'.</param_description>
        </param_input_item>
        <param_input_item>
        <param_name>f</param_name>
        <param_description
      >une chaîne : 'includenan' ou 'omitnan'.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>R</param_name>
            <param_description
      >Somme des éléments du tableau.</param_description>
        </param_output_item>
    </param_output>
        <description>
        <p>
            <b>R = sum(M)</b> renvoie la somme des éléments du tableau M.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[M = uint8([10:30:70;20:30:80;30:30:90]);
R = sum(M, 'native')]]></example_item_data>
            </example_item>
        </examples>
