  <language>en_US</language>
  <keyword>cumsum</keyword>
  <short_description>Cumulative sum of array elements.</short_description>

  <syntax>
    <syntax_item>R = cumsum(M)</syntax_item>
    <syntax_item>R = cumsum(M, d)</syntax_item>
    <syntax_item>R = cumsum(M, d, direction)</syntax_item>
    <syntax_item>R = cumsum(M, d, direction, nanflag)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>M</param_name>
      <param_description
      >an array of double, single, integers, ...</param_description>
    </param_input_item>

    <param_input_item>
      <param_name>d</param_name>
      <param_description
      >dimension to operate along: positive integer scalar.</param_description>
    </param_input_item>

    <param_input_item>
      <param_name>direction</param_name>
      <param_description
      >a string: 'reverse', 'forward' (default).</param_description>
    </param_input_item>

    <param_input_item>
      <param_name>nanflag</param_name>
      <param_description
      >a string: 'includenan' (default) or 'omitnan'.</param_description>
    </param_input_item>
    
  </param_input>

  <param_output>
    <param_output_item>
      <param_name>R</param_name>
      <param_description>Cumulative Sum of array elements.</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p><b
      >R = cumsum(M)</b> returns the cumulative sum of the array elements of M.</p>
  </description>


  <used_function />
  <bibliography />

<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data>
      <![CDATA[M = uint8([10:30:70;20:30:80;30:30:90]);
R = cumsum(M)
R = cumsum(M, 'reverse')]]>
    </example_item_data>
  </example_item>


</examples>

