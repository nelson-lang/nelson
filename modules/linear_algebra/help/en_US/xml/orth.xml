  <language>en_US</language>
  <keyword>orth</keyword>
  <short_description>Range space of a matrix.</short_description>
  <syntax>
    <syntax_item>O = orth(A)</syntax_item>
    <syntax_item>O = orth(A, tol)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>A</param_name>
      <param_description>Input matrix</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>tol</param_name>
      <param_description
      >a numeric value: scalar, singular value tolerance</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>O</param_name>
      <param_description
      >real or complex number (double or single).</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p><b>O = orth(A)</b> returns an orthonormal basis for the range of<b
      >A</b>.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[M = [10 -20 40; -50 20 0; 10 0 30]
O = orth(M)
]]></example_item_data>
    </example_item>
  </examples>
