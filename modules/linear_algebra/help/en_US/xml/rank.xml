    <language>en_US</language>
    <keyword>rank</keyword>
    <short_description>Rank of matrix.</short_description>
    <syntax>
        <syntax_item>r = rank(A)</syntax_item>
        <syntax_item>r = rank(A, tol)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>A</param_name>
            <param_description>matrix: double or single</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>tol</param_name>
            <param_description>tolerance</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>r</param_name>
            <param_description>a numeric value: a scalar.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b
      >rank(A)</b> returns the number of linearly independent columns in a matrix (rank of the matrix).</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[X = rand(10, 10);
r = rank(X)]]></example_item_data>
            </example_item>
        </examples>
        <see_also>
            <see_also_item>
                <link linkend="${linear_algebra}svd">svd</link>
            </see_also_item>
        </see_also>
