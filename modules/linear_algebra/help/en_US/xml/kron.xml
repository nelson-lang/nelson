  <language>en_US</language>
  <keyword>kron</keyword>
  <short_description>Kronecker tensor product.</short_description>

  <syntax>
    <syntax_item>K = kron(A, B)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>A</param_name>
      <param_description
      >a matrix: scalars, vectors or matrices.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>B</param_name>
      <param_description
      >a matrix: scalars, vectors or matrices.</param_description>
    </param_input_item>
  </param_input>

  <param_output>
    <param_output_item>
      <param_name>K</param_name>
      <param_description>result: Kronecker Tensor Product.</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p><b
      >K = kron(A, B)</b> computes the Kronecker tensor product of matrices <b
      >A</b> and <b>B</b>.</p>
  </description>

  <bibliography>https://en.wikipedia.org/wiki/Kronecker_product</bibliography>

<examples>
  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data
      ><![CDATA[A = [1, 2; 3, 4];
B = [0, 5; 6, 7];
K = kron(A, B)
]]>
    </example_item_data>
  </example_item>

</examples>

