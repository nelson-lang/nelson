    <language>fr_FR</language>
    <keyword>orth</keyword>
    <short_description
  >Base orthonormée de l'espace image d'une matrice.</short_description>
    <syntax>
        <syntax_item>O = orth(A)</syntax_item>
        <syntax_item>O = orth(A, tol)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>A</param_name>
            <param_description>matrice d'entrée</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>tol</param_name>
        <param_description
      >une valeur numérique : scalaire, tolérance sur la valeur singulière</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>O</param_name>
        <param_description
      >nombre réel ou complexe (double ou simple précision).</param_description>
        </param_output_item>
    </param_output>
    <description>
                <p>
                        <b
      >O = orth(A)</b> retourne une base orthonormée de l'image (range) de <b
      >A</b>.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[M = [10 -20 40; -50 20 0; 10 0 30]
O = orth(M)
]]></example_item_data>
            </example_item>
        </examples>
