    <language>fr_FR</language>
    <keyword>kron</keyword>
    <short_description>Produit tensoriel de Kronecker.</short_description>
    <syntax>
        <syntax_item>K = kron(A, B)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>A</param_name>
        <param_description
      >une matrice : scalaires, vecteurs ou matrices.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>B</param_name>
        <param_description
      >une matrice : scalaires, vecteurs ou matrices.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>K</param_name>
        <param_description
      >r√©sultat : produit tensoriel de Kronecker.</param_description>
        </param_output_item>
    </param_output>
    <description>
                <p>
                        <b
      >K = kron(A, B)</b> calcule le produit tensoriel de Kronecker des matrices <b
      >A</b> et <b>B</b>.</p>
        </description>
        <bibliography
  >https://en.wikipedia.org/wiki/Kronecker_product</bibliography>
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[A = [1, 2; 3, 4];
B = [0, 5; 6, 7];
K = kron(A, B)
]]></example_item_data>
            </example_item>
        </examples>
