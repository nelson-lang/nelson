  <language>en_US</language>
  <keyword>FFTWwrapper</keyword>
  <short_description>load/free FFTW library dynamically.</short_description>

  <syntax>
    <syntax_item>r = FFTWwrapper('load')</syntax_item>
    <syntax_item
    >r = FFTWwrapper('load', fftwlibraryname, fftwflibraryname)</syntax_item>
    <syntax_item>r = FFTWwrapper('free')</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>'load'</param_name>
      <param_description>load FFTW library.</param_description>
    </param_input_item>

    <param_input_item>
      <param_name>'free'</param_name>
      <param_description>free FFTW library.</param_description>
    </param_input_item>

    <param_input_item>
      <param_name>fftwlibraryname</param_name>
      <param_description>a string: fftw library name.</param_description>
    </param_input_item>

    <param_input_item>
      <param_name>fftwflibraryname</param_name>
      <param_description>a string: fftw float library name.</param_description>
    </param_input_item>
  </param_input>

  <param_output>
    <param_output_item>
      <param_name>r</param_name>
      <param_description>a logical.</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p><b
      >FFTWwrapper</b> is an internal builtin used to load FFTW library dynamically.</p>
</description>


  <used_function />
