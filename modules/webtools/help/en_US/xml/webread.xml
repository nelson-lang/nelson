  <language>en_US</language>
  <keyword>webread</keyword>
  <short_description>Read data from RESTful web service to Nelson's variable</short_description>

  <syntax>
    <syntax_item>var = webread(url)</syntax_item>
    <syntax_item>var = webread(url, name1, value1, ... , nameN, valueN)</syntax_item>
    <syntax_item>var = webread(url, name1, value1, ... , nameN, valueN, options)</syntax_item>
  </syntax>
  

  <param_input>

    <param_input_item>
      <param_name>url</param_name>
      <param_description>a string: URL to a web service.</param_description>
    </param_input_item>

   <param_input_item>
      <param_name>name1, value1, ... , nameN, valueN</param_name>
      <param_description>Name-Value Pair Arguments.</param_description>
    </param_input_item>

   <param_input_item>
      <param_name>options</param_name>
      <param_description>a weboptions object.</param_description>
    </param_input_item>

  </param_input>

  <param_output>
    <param_output_item>
      <param_name>var</param_name>
      <param_description>a variable: content from web.</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p><b>webread()</b> reads content from the web to nelson's variable.</p>
  </description>
  <used_function></used_function>
  <bibliography></bibliography>

<examples>
  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description></example_item_description>
    <example_item_data><![CDATA[url = 'https://httpbin.org/get';
res = webread(url,weboptions('ContentType','json'));
]]>
    </example_item_data>
  </example_item>

 <example_item>
    <example_item_type>nelson</example_item_type>
