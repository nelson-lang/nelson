	<language>en_US</language>
	<keyword>assert_isfalse</keyword>
	<short_description>Check that condition is false.</short_description>

	<syntax>
		<syntax_item>assert_isfalse(x)</syntax_item>
		<syntax_item>r = assert_isfalse(x)</syntax_item>
		<syntax_item>[r, msg] = assert_isfalse(x)</syntax_item>
		<syntax_item>assert_isfalse(x, err_msg)</syntax_item>
		<syntax_item>r = assert_isfalse(x, err_msg)</syntax_item>
		<syntax_item>[r, msg] = assert_isfalse(x, err_msg)</syntax_item>
	
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>x</param_name>
			<param_description>a logical value</param_description>
		</param_input_item>

		<param_input_item>
			<param_name>err_msg</param_name>
			<param_description
      >a string, the error message to be printed in case of failure (optional).</param_description>
		</param_input_item>

	</param_input>

	<param_output>
		<param_output_item>
			<param_name>res</param_name>
			<param_description>a logical value</param_description>
		</param_output_item>
		<param_output_item>
			<param_name>msg</param_name>
			<param_description
      >a string value, the error message. If x == false, then msg == ''. If x == true, then msg contains the error message.</param_description>
		</param_output_item>
	</param_output>


	<description
  >Raises an error if x is true. Raises an error if x is not a logical.</description>

	<used_function />
	<bibliography />

<examples>
	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[assert_isfalse(3 ~= 4)]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[assert_isfalse(3 == 4)]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[r = assert_isfalse(false)]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[[r, msg] = assert_isfalse(false)]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data
      ><![CDATA[[r, msg] = assert_isfalse(3 == 3, 'your error message.')]]>
		</example_item_data>
	</example_item>


</examples>

