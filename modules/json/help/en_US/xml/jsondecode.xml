  <language>en_US</language>
  <keyword>jsondecode</keyword>
  <short_description>decodes a JSON string to Nelson object.</short_description>

  <syntax>
    <syntax_item>res = jsondecode(json_str)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>json_str</param_name>
      <param_description>a json string.</param_description>
    </param_input_item>
  </param_input>

  <param_output>
    <param_output_item>
      <param_name>res</param_name>
      <param_description>a nelson variable converted from JSON.</param_description>
    </param_output_item>
  </param_output>

  <description>
  <p><b>jsondecode</b> converts JSON object field names to Nelson structure field names</p>
  </description>
  <used_function></used_function>
  <bibliography>http://www.rfc-editor.org/rfc/rfc7159.txt</bibliography>

<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description></example_item_description>
    <example_item_data><![CDATA[field1 = 'f1';  value1 = zeros(1,10);
field2 = 'f2';  value2 = {'a', 'b'};
field3 = 'f3';  value3 = {pi, pi*pi};
field4 = 'f4';  value4 = {'fourth'};
s = struct(field1,value1,field2,value2,field3,value3,field4,value4)
r = jsonencode(s)
r2 = jsondecode(r)]]>
    </example_item_data>
  </example_item>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description></example_item_description>
    <example_item_data><![CDATA[
jsondecode(fileread([modulepath('json'), '/examples/patient.json']))
    ]]>
    </example_item_data>
  </example_item>


</examples>

