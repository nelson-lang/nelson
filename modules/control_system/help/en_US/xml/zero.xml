    <language>en_US</language>
    <keyword>zero</keyword>
    <short_description
  >Zeros and gain of SISO dynamic system.</short_description>
    <syntax>
        <syntax_item>Z = zero(sys)</syntax_item>
        <syntax_item>[Z, gain] = zero(sys)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>sys</param_name>
            <param_description>a LTI model.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>Z</param_name>
            <param_description>Zeros of the dynamic system.</param_description>
        </param_output_item>
        <param_output_item>
            <param_name>gain</param_name>
            <param_description
      >Zero-pole-gain of the dynamic system.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>[Z, gain] = zero(sys)</b> returns the zero-pole-gain of <b
      >sys</b>.</p>
        </description>
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[sys = tf([4.2,0.25,-0.004],[1,9.6,17]);
[Z, gain] = zero(sys)]]></example_item_data>
            </example_item>
        </examples>
        <see_also>
            <see_also_item>
                <link linkend="${control_system}pole">pole</link>
            </see_also_item>
        </see_also>
