    <language>en_US</language>
    <keyword>islti</keyword>
    <short_description
  >Checks if variable is an linear model tf, ss or zpk.</short_description>
    <syntax>
        <syntax_item>res = islti(sys)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>A</param_name>
            <param_description>variable.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>res</param_name>
            <param_description
      >a logical: true if it is an linear model.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>Checks if variable is an linear model tf, ss or zpk.</p>
    </description>
    <examples>
        <example_item>
            <example_item_type>nelson</example_item_type>
            <example_item_description />
            <example_item_data
        runnable="cli"
      ><![CDATA[A = [-15,-20; 10, 0];
B = [5; 0];
C = [0, 1];
D = 0;
sys = ss(A, B, C, D);
islti(sys)
islti(A)]]></example_item_data>
        </example_item>
    </examples>
