	<language>en_US</language>
	<keyword>augstate</keyword>
  <short_description>Append state vector to output vector.</short_description>

  <syntax>
    <syntax_item>sysa = augstate(sys)</syntax_item>
    <syntax_item>[Aa, Ba, Ca, Da] = augstate(A, B, C, D)</syntax_item>
  </syntax>

  <param_input>
    <param_input_item>
      <param_name>sys</param_name>
      <param_description>LTI model.</param_description>
    </param_input_item>

  </param_input>

  <param_output>
     <param_output_item>
      <param_name>sysa</param_name>
      <param_description
      >State-space model with states appended to the outputs.</param_description>
    </param_output_item>
  </param_output>

  <description>
  <p>The function <b
      >sysa = augstate(sys)</b> adds the state vector to the outputs of a state-space model.</p>
  </description>

<examples>
  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data><![CDATA[sys = ss(10, 10, 20, 0);
sysa = augstate(sys)]]>
    </example_item_data>
  </example_item>
</examples>

