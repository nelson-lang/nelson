    <language>fr_FR</language>
    <keyword>tf</keyword>
    <short_description
  >Construit un modèle de fonction de transfert.</short_description>
    <syntax>
        <syntax_item>sys = tf()</syntax_item>
        <syntax_item>sys = tf('s')</syntax_item>
        <syntax_item>sys = tf(numerator, denominator)</syntax_item>
        <syntax_item>sys = tf(numerator, denominator, Ts)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>numerator</param_name>
            <param_description
      >coefficients polynomiaux : un vecteur ligne ou un tableau de cellules de vecteurs lignes.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>denominator</param_name>
            <param_description
      >coefficients polynomiaux : un vecteur ligne ou un tableau de cellules de vecteurs lignes.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>Ts</param_name>
            <param_description
      >Temps d'échantillonnage Ts, par défaut : en secondes</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>sysIn</param_name>
            <param_description>Modèle LTI.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>sys</param_name>
            <param_description
      >Modèle de système de fonction de transfert en sortie.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p
    >Crée un modèle de fonction de transfert continu ou discret à partir des coefficients du numérateur et du dénominateur, et d'un temps d'échantillonnage optionnel.</p>
    </description>
    <examples>
        <example_item>
            <example_item_type>nelson</example_item_type>
            <example_item_description />
            <example_item_data
        runnable="cli"
      ><![CDATA[numerator = 10;
denominator = [20, 33, 44];
sys = tf(numerator, denominator)]]></example_item_data>
        </example_item>
        <example_item>
            <example_item_type>nelson</example_item_type>
            <example_item_description />
            <example_item_data
        runnable="cli"
      ><![CDATA[numerator = 10;
denominator = [20, 33, 44];
Ts = 1.5;
sys = tf(numerator, denominator, Ts)]]></example_item_data>
        </example_item>
    </examples>
