    <language>fr_FR</language>
    <keyword>tzero</keyword>
    <short_description
  >Zéros invariants d'un système linéaire.</short_description>
    <syntax>
        <syntax_item>z = tzero(sys)</syntax_item>
        <syntax_item>z = tzero(A, B, C, D)</syntax_item>
        <syntax_item>z = tzero(A, B, C, D, E)</syntax_item>
        <syntax_item>[z, nrank] = tzero(sys)</syntax_item>
        <syntax_item>[z, nrank] = tzero(A, B, C, D)</syntax_item>
        <syntax_item>[z, nrank] = tzero(A, B, C, D, E)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>sys</param_name>
            <param_description>un modèle LTI.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>A</param_name>
            <param_description
      >Matrice d'état : matrice Nx par Nx.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>B</param_name>
            <param_description
      >Matrice entrée-état : matrice Nx par Nu.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>C</param_name>
            <param_description
      >Matrice état-sortie : matrice Ny par Nx.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>D</param_name>
            <param_description
      >Feedthrough matrix: Ny-by-Nu matrix.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>E</param_name>
            <param_description>Nx-by-Nx matrix.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>Z</param_name>
            <param_description
      >Invariant zeros: column vector.</param_description>
        </param_output_item>
        <param_output_item>
            <param_name>nrank</param_name>
            <param_description
      >Normal rank: positive integer.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p
    >Calcule les zéros invariants d'un système d'état ou renvoie également le rang numérique associé.</p>
    </description>
    <examples>
        <example_item>
            <example_item_type>nelson</example_item_type>
            <example_item_description />
            <example_item_data
        runnable="cli"
      ><![CDATA[A = [1 2; 3 4];
B = [1; 0];
C = [1 0];
D = 0;
sys = ss(A, B, C, D);
z = tzero(sys)
[z, nrank] = tzero(sys)]]></example_item_data>
        </example_item>
    </examples>
