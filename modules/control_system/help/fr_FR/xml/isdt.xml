    <language>fr_FR</language>
    <keyword>isdt</keyword>
    <short_description
  >Vérifie si le modèle dynamique est en temps discret.</short_description>
    <syntax>
        <syntax_item>res = isdt(sys)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>sys</param_name>
            <param_description>un modèle lti.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>res</param_name>
            <param_description
      >un logique : vrai si le modèle dynamique est en temps discret.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>Vérifie si le modèle dynamique est en temps discret.</p>
    </description>
    <examples>
        <example_item>
            <example_item_type>nelson</example_item_type>
            <example_item_description />
            <example_item_data
        runnable="cli"
      ><![CDATA[A = [-15,-20; 10, 0];
B = [5; 0];
C = [0, 1];
D = 0;
sys1 = ss(A, B, C, D);
isdt(sys1)
sys2 = ss(A, B, C, D, 0.2);
isdt(sys2)]]></example_item_data>
        </example_item>
    </examples>
