  <language>fr_FR</language>
  <keyword>help</keyword>
  <short_description
  >Aide pour les fonctions dans la fenêtre de commande.</short_description>
  <syntax>
    <syntax_item>help function_name</syntax_item>
    <syntax_item>help('function_name')</syntax_item>
    <syntax_item>txt = help('function_name')</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>function_name</param_name>
      <param_description
      >une chaîne de caractères : nom de la fonction</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>txt</param_name>
      <param_description
      >une chaîne de caractères : texte d'aide</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p><b
      >help('function_name')</b> affiche le texte d'aide pour la fonctionnalité spécifiée.</p>
  </description>
  <used_function />
