  <language>en_US</language>
  <keyword>buildhelpmd</keyword>
  <short_description
  >Build help of Nelson's modules for GitBook.</short_description>

  <syntax>
    <syntax_item>buildhelpmd(dirdest)</syntax_item>
    <syntax_item>buildhelpmd(dirdest, module_name)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>dirdest</param_name>
      <param_description>a string: a path destination.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>module_name</param_name>
      <param_description
      >a string: module name (module must be loaded).</param_description>
    </param_input_item>
  </param_input>

  <description><p><b
      >buildhelpmd</b> generates help files for GitBook (markdown).</p>

</description>
  <used_function />
  <bibliography />

<examples>
  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data
      ><![CDATA[buildhelpmd(tempdir());
buildhelpmd(tempdir(), 'core');]]>
    </example_item_data>

  </example_item>


</examples>

