    <language>en_US</language>
    <keyword>buildhelp</keyword>
    <short_description>Build help of Nelson's modules.</short_description>
    <syntax>
        <syntax_item>buildhelp()</syntax_item>
        <syntax_item>buildhelp(module_name)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>module_name</param_name>
            <param_description
      >a string: module name (module must be loaded).</param_description>
        </param_input_item>
    </param_input>
    <description>
        <p>
            <b>buildhelp</b> generates help files.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[buildhelp();
buildhelp('core');]]></example_item_data>
            </example_item>
        </examples>
