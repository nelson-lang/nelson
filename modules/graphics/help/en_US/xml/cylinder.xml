  <language>en_US</language>
  <keyword>cylinder</keyword>
  <short_description>Create cylinder.</short_description>

  <syntax>
    <syntax_item>[X, Y, Z] = cylinder()</syntax_item>
    <syntax_item>[X, Y, Z] = cylinder(r)</syntax_item>
    <syntax_item>[X, Y, Z] = cylinder(r, n)</syntax_item>
    <syntax_item>cylinder()</syntax_item>
    <syntax_item>cylinder(r)</syntax_item>
    <syntax_item>cylinder(r, n)</syntax_item>
    <syntax_item>cylinder(ax, ...)</syntax_item>
  </syntax>
	
	<param_input>
		<param_input_item>
			<param_name>r</param_name>
			<param_description>Profile curve: vector.</param_description>
		</param_input_item>
    		<param_input_item>
			<param_name>n</param_name>
			<param_description>Number of points: positive whole number.</param_description>
		</param_input_item>
    		<param_input_item>
			<param_name>ax</param_name>
			<param_description>Target axes: 'axes' object.</param_description>
		</param_input_item>
	</param_input>

  <param_output>
    <param_output_item>
      <param_name>X, Y, Z</param_name>
      <param_description
      > x-, y-, and z- coordinates of a cylinder without drawing it.</param_description>
    </param_output_item>
  </param_output>

<description>
<p><b>cylinder</b> creates cylinder and plots it.</p>
</description>


  <used_function />
  <bibliography />
  
<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data><![CDATA[f1 = figure();
colormap(spring)
cylinder()]]>
    </example_item_data>
    <example_item_img src="../images/cylinder_1.svg" align="middle" />
  </example_item>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data
      ><![CDATA[f2 = figure();
colormap(summer)
r = 4;
cylinder(r);]]>
    </example_item_data>
    <example_item_img src="../images/cylinder_2.svg" align="middle" />
  </example_item>


</examples>

