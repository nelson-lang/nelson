                                                                                                                                                                                    <examples
  >
                                                                                                                                                                                        <example_item
    >
                                                                                                                                                                                            <example_item_type
      >nelson</example_item_type>
                                                                                                                                                                                            <example_item_description
      />
                                                                                                                                                                                            <example_item_data
      ><![CDATA[f = figure();
n = 100;
x = randn(n,1);
y = randn(n,1);
z = randn(n,1);
c = z;
sz = 20 + 50 * sqrt(x.^2 + y.^2 + z.^2);
scatter3(x, y, z, sz, c, 'filled');
% Add labels and title
xlabel('X Axis');
ylabel('Y Axis');
zlabel('Z Axis');
title('3D Scatter Plot Demo');
grid on;
axis equal;
view(-66.5, 12);]]></example_item_data>
