  <language>en_US</language>
  <keyword>imshow</keyword>
  <short_description>Display image.</short_description>

  <syntax>
    <syntax_item>imshow(filename)</syntax_item>
    <syntax_item>imshow(img)</syntax_item>
    <syntax_item>imshow(RGB)</syntax_item>
    <syntax_item>imshow(img, [low high])</syntax_item>
    <syntax_item>imshow(img, [])</syntax_item>
    <syntax_item>imshow(img, map)</syntax_item>
    <syntax_item>imshow(..., propertyName, propertyValue)</syntax_item>
    <syntax_item>go = imshow(...)</syntax_item>
  </syntax>
	
	<param_input>
		<param_input_item>
			<param_name>filename</param_name>
			<param_description
      >row vector character: file name of the image to display.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>img</param_name>
			<param_description>grayscale image: matrix.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>RGB</param_name>
			<param_description>truecolor image: m-by-n-by-3 array.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>[low high]</param_name>
			<param_description>grayscale image display range.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>map</param_name>
			<param_description>colormap: c-by-3 matrix.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>propertyName</param_name>
			<param_description
      >a scalar string or row vector character (for compatibility).</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>propertyValue</param_name>
			<param_description>a value (for compatibility).</param_description>
		</param_input_item>
	</param_input>

  <param_output>
    <param_output_item>
      <param_name>go</param_name>
      <param_description>a graphics object: image type.</param_description>
    </param_output_item>
  </param_output>

<description>
<p><b>imshow(img)</b> displays the image <b>im</b>.</p>
</description>

  <used_function />
  <bibliography />
  
