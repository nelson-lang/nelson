<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data><![CDATA[ca = gca()
isgraphics(ax, 'axes')]]>
    </example_item_data>
  </example_item>

</examples>
