                                                                                            <examples
  >
                                                                                                <example_item
    >
                                                                                                    <example_item_type
      >nelson</example_item_type>
                                                                                                    <example_item_description
      />
                                                                                                    <example_item_data
      ><![CDATA[f = figure();
x = linspace(0,10)';
y1 = sin(x);
y2 = cos(x);
line(x, y1, 'Color', [0 1 0])
line(x, y2, 'Color', [1 0 0])
]]></example_item_data>
                                                                                                    <example_item_img
        src="../images/line_xy.svg"
        align="middle"
      />
                                                                                                </example_item>
                                                                                                <example_item
    >
                                                                                                    <example_item_type
      >nelson</example_item_type>
                                                                                                    <example_item_description
      />
                                                                                                    <example_item_data
      ><![CDATA[f = figure();
x = [1 9];
y = [2 12];
line(x,y,'Color','red','LineStyle','--')]]></example_item_data>
                                                                                                    <example_item_img
        src="../images/line_linestyle.svg"
        align="middle"
      />
                                                                                                </example_item>
                                                                                                <example_item
    >
                                                                                                    <example_item_type
      >nelson</example_item_type>
                                                                                                    <example_item_description
      />
                                                                                                    <example_item_data
      ><![CDATA[f = figure();
t = linspace(0,10*pi,400);
x = sin(t);
y = cos(t);
z = t;
line(x,y,z)
view(3)]]></example_item_data>
                                                                                                    <example_item_img
        src="../images/line_xyz.svg"
        align="middle"
      />
                                                                                                </example_item>
