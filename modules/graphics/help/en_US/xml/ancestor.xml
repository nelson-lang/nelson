    <language>en_US</language>
    <keyword>ancestor</keyword>
    <short_description>Ancestor of graphics object.</short_description>
    <syntax>
        <syntax_item>p = ancestor(h, type)</syntax_item>
        <syntax_item>p = ancestor(h, type, 'toplevel')</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>h</param_name>
            <param_description>graphics object</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>type</param_name>
            <param_description
      >a row vector character or cell of strings: </param_description>
        </param_input_item>
        <param_input_item>
            <param_name>'toplevel'</param_name>
            <param_description
      >a row vector character: return the highest parent in the object hierarchy that matches the condition.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>p</param_name>
            <param_description>a graphics object or []</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b
      >ancestor</b> returns the handle of the specified object's ancestor of a given type.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="adv-cli"
      ><![CDATA[f = figure();
ax = gca();
s = surf(peaks);
AX = ancestor(s, 'axes')
F = ancestor(s, 'figure')
R = ancestor(s, 'root')]]></example_item_data>
            </example_item>
        </examples>
        <see_also>
            <see_also_item>
                <link linkend="${graphics}gcf">gcf</link>
            </see_also_item>
        </see_also>
