  <language>en_US</language>
  <keyword>abyss</keyword>
  <short_description>Abyss colormap array.</short_description>

  <syntax>
  <syntax_item>c = abyss</syntax_item>
  <syntax_item>c = abyss(m)</syntax_item>
  </syntax>
	
	<param_input>
		<param_input_item>
			<param_name>m</param_name>
			<param_description
      >a scalar integer value: Number of colors (256 as default value).</param_description>
		</param_input_item>
	</param_input>

  <param_output>
    <param_output_item>
      <param_name>c</param_name>
      <param_description>Abyss colormap array.</param_description>
    </param_output_item>
  </param_output>

<description>
<p><b>abyss</b> returns the colormap with abyss colors.</p>
  
</description>

  <used_function />
  <bibliography />
  
<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data><![CDATA[f = figure();
surf(peaks);
colormap('abyss');]]>
    </example_item_data>
    <example_item_img src="../images/abyss.svg" align="middle" />

  </example_item>

</examples>

