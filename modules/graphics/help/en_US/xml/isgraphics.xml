    <language>en_US</language>
    <keyword>isgraphics</keyword>
    <short_description>Check for graphics object.</short_description>
    <syntax>
        <syntax_item>tf = isgraphics(GO)</syntax_item>
        <syntax_item>tf = isgraphics(GO, type)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>GO</param_name>
            <param_description>variable or graphics object.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>type</param_name>
            <param_description
      >a character vector or scalar string: 'axes', 'line', 'image', 'root', 'text', 'figure'.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>tf</param_name>
            <param_description>a scalar logical.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>isgraphics</b> checks is variable is an graphics object.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[f = figure()
tf = isgraphics(f)
tf = isgraphics(f, 'figure')
tf = isgraphics(f, 'text')
f = 3
tf = isgraphics(f)]]></example_item_data>
            </example_item>
        </examples>
        <see_also>
            <see_also_item>
                <link linkend="${handle}isprop">isprop</link>
            </see_also_item>
        </see_also>
        <history>
            <history_item>
                <history_version>1.0.0</history_version>
                <history_description>initial version</history_description>
            </history_item>
        </history>
        <authors>
            <author_item>Allan CORNET</author_item>
        </authors>
    </xmldoc>
