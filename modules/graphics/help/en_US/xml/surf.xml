                                                                                                                                                                    <examples
  >
                                                                                                                                                                        <example_item
    >
                                                                                                                                                                            <example_item_type
      >nelson</example_item_type>
                                                                                                                                                                            <example_item_description
      />
                                                                                                                                                                            <example_item_data
      ><![CDATA[f = figure();
[X, Y, Z] = peaks(35);
C(:, :, 1) = zeros(35);
C(:, :, 2) = ones(35) .* linspace(0.5, 0.6, 35);
C(:, :, 3) = ones(35) .* linspace(0, 1, 35);
S = surf(X, Y, Z, C)]]></example_item_data>
                                                                                                                                                                            <example_item_img
        src="../images/surf_1.svg"
        align="middle"
      />
                                                                                                                                                                        </example_item>
                                                                                                                                                                        <example_item
    >
                                                                                                                                                                            <example_item_type
      >nelson</example_item_type>
                                                                                                                                                                            <example_item_description
      />
                                                                                                                                                                            <example_item_data
      ><![CDATA[f = figure();
[X,Y] = meshgrid(-8:.5:8);
R = sqrt(X.^2 + Y.^2) + eps;
Z = sin(R)./R;
h = surf(X, Y, Z);
axis square]]></example_item_data>
                                                                                                                                                                            <example_item_img
        src="../images/surf_2.svg"
        align="middle"
      />
                                                                                                                                                                        </example_item>
