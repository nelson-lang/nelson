    <language>fr_FR</language>
    <keyword>nebula</keyword>
    <short_description>Palette de couleurs Nebula.</short_description>
    <syntax>
        <syntax_item>c = nebula</syntax_item>
        <syntax_item>c = nebula(m)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>m</param_name>
            <param_description
      >Valeur entière scalaire : nombre de couleurs (256 par défaut).</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>c</param_name>
            <param_description>Palette de couleurs Nebula.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>nebula</b> retourne la palette de couleurs Nebula.
        </p>
    </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="adv-cli"
      ><![CDATA[f = figure();
n = 256;
cmap = nebula(n);
colormap(cmap);
imagesc(peaks(100));
colorbar;
title(['Nebula Colormap with ', num2str(n), ' Colors']);]]></example_item_data>
                <example_item_img
        src="../images/nebula.svg"
        align="middle"
        generate="true"
      />
            </example_item>
        </examples>
        <see_also>
            <see_also_item>
                <link linkend="${graphics}colormap">colormap</link>
            </see_also_item>
        </see_also>
        <history>
            <history_item>
                <history_version>1.14.0</history_version>
