  <language>fr_FR</language>
  <keyword>ylabel</keyword>
  <short_description>Étiquette de l'axe des y.</short_description>
  <syntax>
    <syntax_item>ylabel(text)</syntax_item>
    <syntax_item>ylabel(ax, text)</syntax_item>
    <syntax_item>ylabel(..., propertyName, propertyValue)</syntax_item>
    <syntax_item>go = ylabel(...)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>text</param_name>
      <param_description
      >Texte à afficher : vecteur de caractères, scalaire de chaîne, tableau de chaînes ou tableau de cellules.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>ax</param_name>
      <param_description
      >une valeur scalaire d'objet graphique : conteneur parent, spécifié comme un axes.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>propertyName</param_name>
      <param_description
      >une chaîne scalaire ou un vecteur de caractères en ligne.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>propertyValue</param_name>
      <param_description>une valeur.</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>go</param_name>
      <param_description>un objet graphique : type texte.</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p><b>ylabel('text')</b> étiquette l'axe des y des axes actuels.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="adv-cli"
      ><![CDATA[f = figure();
x = linspace(-1, 1);
y = sin(2*pi*x);
plot(x, y);
ylabel('Étiquette de l’axe Y - Unicode ドラゴンボールY(ゼット)')]]></example_item_data>
      <example_item_img
        src="../images/ylabel.svg"
        align="middle"
        generate="true"
      />
    </example_item>
  </examples>
