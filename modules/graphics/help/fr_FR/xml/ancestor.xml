    <language>fr_FR</language>
    <keyword>ancestor</keyword>
    <short_description>Ancêtre d'un objet graphique.</short_description>
    <syntax>
        <syntax_item>p = ancestor(h, type)</syntax_item>
        <syntax_item>p = ancestor(h, type, 'toplevel')</syntax_item>
    </syntax>
    <param_input>
                <param_input_item>
                        <param_name>h</param_name>
                        <param_description>objet graphique</param_description>
                </param_input_item>
                <param_input_item>
                        <param_name>type</param_name>
                        <param_description
      >un vecteur ligne de caractères ou une cellule de chaînes :</param_description>
                </param_input_item>
                <param_input_item>
                        <param_name>'toplevel'</param_name>
                        <param_description
      >un vecteur ligne de caractères : retourne le parent le plus haut dans la hiérarchie d'objets qui correspond à la condition.</param_description>
                </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>p</param_name>
            <param_description>un objet graphique ou []</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b
      >ancestor</b> retourne le handle de l'ancêtre d'un objet spécifié d'un type donné.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="adv-cli"
      ><![CDATA[f = figure();
ax = gca();
s = surf(peaks);
AX = ancestor(s, 'axes')
F = ancestor(s, 'figure')
R = ancestor(s, 'root')]]></example_item_data>
            </example_item>
        </examples>
