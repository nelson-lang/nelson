    <language>fr_FR</language>
    <keyword>iskeyword</keyword>
    <short_description>Renvoie tous les mots-clés de Nelson.</short_description>
    <syntax>
        <syntax_item>state = iskeyword(name)</syntax_item>
        <syntax_item>ce = iskeyword()</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>name</param_name>
            <param_description>une chaîne.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>state</param_name>
            <param_description
      >un logique : true si c'est un mot-clé Nelson.</param_description>
        </param_output_item>
        <param_output_item>
            <param_name>ce</param_name>
            <param_description
      >une cellule de chaînes : liste des mots-clés de Nelson.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b
      >iskeyword</b> renvoie la liste de tous les mots-clés de Nelson.</p>
        </description>
