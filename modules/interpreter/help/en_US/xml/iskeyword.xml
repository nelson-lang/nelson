    <language>en_US</language>
    <keyword>iskeyword</keyword>
    <short_description>Returns all Nelson keywords.</short_description>
    <syntax>
        <syntax_item>state = iskeyword(name)</syntax_item>
        <syntax_item>ce = iskeyword()</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>name</param_name>
            <param_description>a string.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>state</param_name>
            <param_description
      >a logical: true if is an Nelson keyword.</param_description>
        </param_output_item>
        <param_output_item>
            <param_name>ce</param_name>
            <param_description
      >a cell of strings: list of Nelson's keywords.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>iskeyword</b> returns a list of all Nelson keywords.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[iskeyword('for')
ce = iskeyword()]]></example_item_data>
            </example_item>
        </examples>
        <history>
            <history_item>
