  <language>en_US</language>
  <keyword>parsefile</keyword>
  <short_description>Parse a Nelson file.</short_description>

  <syntax>
    <syntax_item>status = parsefile(filename)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>filename</param_name>
      <param_description>a string: a filename to parse.</param_description>
    </param_input_item>
  </param_input>

  <param_output>

    <param_output_item>
      <param_name>status</param_name>
      <param_description>a string: 'script', 'function', 'error'.</param_description>
    </param_output_item>

  </param_output>

  <description>
  <p><b>parsefile</b> parse a file and returns if it is a valid script, a valid function or an error.</p>
</description>

  <used_function></used_function>
  <bibliography></bibliography>


<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description></example_item_description>
    <example_item_data><![CDATA[parsefile([nelsonroot(), '/etc/startup.nls'])
parsefile([nelsonroot(), '/modules/data_structures/functions/cellstr.nlf'])]]>
    </example_item_data>
  </example_item>


</examples>

