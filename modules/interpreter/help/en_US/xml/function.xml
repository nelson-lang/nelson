  <language>en_US</language>
  <keyword>function</keyword>
  <keyword>endfunction</keyword>

  <short_description>function declaration.</short_description>

  <syntax>
    <syntax_item>function [out_1,...,out_M,varargout] = fname(in_1, ... , in_N, varargin)</syntax_item>
    <syntax_item>function fname(in_1, ... , in_N, varargin)</syntax_item>
    <syntax_item>function [out_1,...,out_M,varargout] = fname()</syntax_item>
    <syntax_item>function fname()</syntax_item>
   </syntax>
  

  <description>
  <p><b>function</b> opens a function definition.</p>
  <p><b>endfunction</b> closes a function definition (optional, but strongly recommended).</p>

</description>

  <used_function></used_function>
  <bibliography></bibliography>


