    <language>en_US</language>
    <keyword>parsestring</keyword>
    <short_description>Parse a string.</short_description>
    <syntax>
        <syntax_item>status = parsestring(str)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>str</param_name>
            <param_description>a string: a string to parse.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>status</param_name>
            <param_description
      >a string: 'script', 'function', 'error'.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b
      >parsestring</b> parse a string and returns if it is a valid script, a valid function or an error.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[parsestring('1 + 1')
parsestring('1 +++ 1')
parsestring('1 +*+ 1')]]></example_item_data>
            </example_item>
        </examples>
