// Fix predecence of Colon ":" operator.
//=============================================================================
assert_isequal(5:10 == 5:10, logical(ones(1, 6)));
assert_isequal([5:10] == [5:10], logical(ones(1, 6)));
assert_isequal((5:10) == (5:10), logical(ones(1, 6)));
assert_isequal(5:10 == [5:10], logical(ones(1, 6)));
assert_isequal([5:10] == 5:10, logical(ones(1, 6)));
assert_isequal([5:10] == (5:10), logical(ones(1, 6)));
assert_isequal((5:10) == [5:10], logical(ones(1, 6)));
