  <language>en_US</language>
  <keyword>polyint</keyword>
  <short_description>Polynomial integration.</short_description>

  <syntax>
    <syntax_item>q = polyint(p, k)</syntax_item>
    <syntax_item>q = polyint(p)</syntax_item>
  </syntax>

  <param_input>
    <param_input_item>
      <param_name>p</param_name>
      <param_description>vector: polynomial coefficients</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>k</param_name>
      <param_description>numeric scalr: constant of integration</param_description>
    </param_input_item>
  </param_input>

 <param_output>
    <param_output_item>
      <param_name>q</param_name>
      <param_description>row vector: integrated polynomial coefficients</param_description>
    </param_output_item>
  </param_output>



  <description>
    <p><b>polyint</b> returns the integral of the polynomial represented by the coefficients in <b>p</b> using a constant of integration <b>k</b> (0 by default).</p>
  </description>

<examples>
  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description></example_item_description>
    <example_item_data><![CDATA[
p = [10, 0, -10, 0, 0, 10];
v = [10, 0, 10];
k = 3;
q = polyint(conv(p,v),k)]]>
    </example_item_data>
  </example_item>

</examples>

