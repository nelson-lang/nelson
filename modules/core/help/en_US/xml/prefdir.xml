	<language>en_US</language>
	<keyword>prefdir</keyword>
	<short_description
  >Return the preferences directory used by Nelson.</short_description>

	<syntax>
		<syntax_item>pref_path = prefdir</syntax_item>
	</syntax>
	

	<param_output>

		<param_output_item>
			<param_name>pref_path</param_name>
			<param_description>a string: the preferences directory</param_description>
		</param_output_item>
	</param_output>

	<description>
		<p><b
      >pref_path = prefdir()</b> returns the preferences directory used by Nelson.</p>
</description>


