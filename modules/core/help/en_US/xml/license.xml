	<language>en_US</language>
	<keyword>license</keyword>
	<short_description>Get license information for Nelson.</short_description>

	<syntax>
		<syntax_item>license</syntax_item>
		<syntax_item>r = license</syntax_item>
		<syntax_item>[r, txt] = license</syntax_item>
	</syntax>

	<param_output>
		<param_output_item>
			<param_name>r</param_name>
			<param_description
      >a string: minimal string description about license</param_description>
		</param_output_item>
		<param_output_item>
			<param_name>txt</param_name>
			<param_description>a string: complete license text.</param_description>
		</param_output_item>

	</param_output>


	<description>
		<p><b>license</b> get license information for Nelson.</p>
	</description>

	<used_function />
	<bibliography />
	

