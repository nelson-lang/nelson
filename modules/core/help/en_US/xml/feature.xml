	<language>en_US</language>
	<keyword>feature</keyword>
	<short_description>undocumented features.</short_description>

	<syntax>
		<syntax_item>ret = feature(name)</syntax_item>
		<syntax_item>ret = feature(name, newValue)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>name</param_name>
			<param_description>a string: name of the feature.</param_description>
		</param_input_item>

		<param_input_item>
			<param_name>newValue</param_name>
			<param_description>a variable</param_description>
		</param_input_item>
		
	</param_input>

	<param_output>

		<param_output_item>
			<param_name>ret</param_name>
			<param_description>result: result returned</param_description>
		</param_output_item>
	</param_output>

	<description>
		<p><b
      >feature</b> is an entirely undocumented and unsupported Nelson function.</p>
		<p>It is a helper function for debugging Nelson.</p>
		<p><b
      >feature</b>can change without prior notice between Nelson releases, so be very careful when using this function in your code.</p>
	</description>


