	<language>en_US</language>
	<keyword>nelsonroot</keyword>
	<short_description>Returns Nelson's root folder.</short_description>

	<syntax>
		<syntax_item>nelson_path = nelsonroot</syntax_item>
	</syntax>

	<param_output>
		<param_output_item>
			<param_name>nelson_path</param_name>
			<param_description>a string: the root folder of Nelson.</param_description>
		</param_output_item>
	</param_output>

	<description>
		<p><b>nelsonroot</b> returns the root folder of Nelson.</p>
	</description>


	<used_function />
	<bibliography />

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[pwd
cd(nelsonroot)
pwd]]>
		</example_item_data>
	</example_item>

</examples>

