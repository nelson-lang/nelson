  <language>fr_FR</language>
  <keyword>evalc</keyword>
  <short_description
  >Évalue une expression et capture la sortie.</short_description>
  <syntax>
    <syntax_item>t = evalc(str)</syntax_item>
    <syntax_item>t = evalc(str)</syntax_item>
    <syntax_item>[t, r1, ... rn] = evalc(str)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>str</param_name>
      <param_description>chaîne : expression à évaluer</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>T</param_name>
      <param_description
      >texte de sortie capturé dans la variable t</param_description>
    </param_output_item>
    <param_output_item>
      <param_name>[r1, ... rn]</param_name>
      <param_description>résultats : variables de sortie</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p
    >Évalue une expression et renvoie la sortie standard générée par l'exécution sous forme de chaîne de caractères.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[evalc('B=4')]]></example_item_data>
    </example_item>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      >        &gt;<![CDATA[t = evalc('dir')]]></example_item_data>
    </example_item>
  </examples>
