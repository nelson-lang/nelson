  <language>fr_FR</language>
  <keyword>sha256</keyword>
  <short_description>Calcule le hash SHA-256.</short_description>
  <syntax>
    <syntax_item>hexa_hash = sha256(str)</syntax_item>
    <syntax_item>hexa_hash = sha256(filename)</syntax_item>
    <syntax_item>hexa_hash = sha256(str, '-file')</syntax_item>
    <syntax_item>hexa_hash = sha256(str, '-string')</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>str</param_name>
      <param_description
      >vecteur de caractères, cellule de chaînes ou tableau de chaînes : contenu de la chaîne à hacher</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>filename</param_name>
      <param_description
      >chaîne : nom de fichier existant dont le contenu sera haché</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>'-file' or '-string'</param_name>
      <param_description
      >force à traiter comme fichier ou contenu de chaîne</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>hexa_hash</param_name>
      <param_description
      >vecteur de caractères, cellule de chaînes ou tableau de chaînes : résultat haché (checksum)</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p>Calcule la valeur de hachage SHA-256 d'une chaîne ou d'un fichier.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[R = sha256('Nelson')]]></example_item_data>
    </example_item>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[R = sha256({'Hello', 'World'})]]></example_item_data>
    </example_item>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[R = sha256(["Hello"; "World"])]]></example_item_data>
    </example_item>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[R = sha256([modulepath('matio', 'tests'), '/mat/test_char_array_unicode_7.4_GLNX86.mat'])]]></example_item_data>
    </example_item>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[R = sha256([modulepath('matio', 'tests'), '/mat/test_char_array_unicode_7.4_GLNX86.mat'], '-file')]]></example_item_data>
    </example_item>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[R = sha256([modulepath('matio', 'tests'), '/mat/test_char_array_unicode_7.4_GLNX86.mat'], '-string')]]></example_item_data>
    </example_item>
  </examples>
