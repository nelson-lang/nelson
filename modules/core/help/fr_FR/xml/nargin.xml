  <language>fr_FR</language>
  <keyword>nargin</keyword>
  <short_description
  >Nombre d'arguments d'entrée d'une fonction.</short_description>
  <syntax>
    <syntax_item>R = nargin()</syntax_item>
    <syntax_item>R = nargin(function_name)</syntax_item>
    <syntax_item>R = nargin(function_handle)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>function_name</param_name>
      <param_description>une chaîne : nom de la fonction</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>function_handle</param_name>
      <param_description>un handle de fonction</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>R</param_name>
      <param_description
      >une valeur entière : nombre d'arguments d'entrée</param_description>
    </param_output_item>
  </param_output>
  <description>
    <p
    >Retourne le nombre d'arguments d'entrée fournis à la fonction appelée.</p>
  </description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description
      >With an macro function:</example_item_description>
      <example_item_data
        runnable="cli"
      ><![CDATA[nargin('getfield')]]></example_item_data>
    </example_item>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description
      >With an builtin function:</example_item_description>
      <example_item_data
        runnable="cli"
      ><![CDATA[nargin('cos')]]></example_item_data>
    </example_item>
  </examples>
