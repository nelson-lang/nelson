  <language>en_US</language>
  <keyword>rethrow</keyword>
  <short_description>rethrow error.</short_description>

  <syntax>
    <syntax_item>rethrow(MException)</syntax_item>
  </syntax>

  <param_input>
    <param_input_item>
      <param_name>MException</param_name>
      <param_description>MException object</param_description>
    </param_input_item>
  </param_input>

  <description>
    <p><b>rethrow(MException)</b> reissues the error specified by <b
      >MException</b>.</p>
  </description>


  <used_function />
  <bibliography />

<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data><![CDATA[
try 
  a
catch ME
  disp(ME)
  rethrow(ME)
end
]]>
    </example_item_data>
  </example_item>

</examples>

