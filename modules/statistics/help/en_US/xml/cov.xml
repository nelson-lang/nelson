    <keyword>cov</keyword>
    <short_description>Covariance</short_description>
    <syntax>
        <syntax_item>C = cov(M)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>M</param_name>
            <param_description>a vector or matrix</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>V</param_name>
            <param_description>Covariance of M.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>C = cov(M)</b> returns the covariance.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
      ><![CDATA[M = [4 -7 3; 1 4 -2; 10 7 9];
C = cov(M)]]></example_item_data>
            </example_item>
        </examples>
