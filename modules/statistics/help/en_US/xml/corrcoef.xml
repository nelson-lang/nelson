  <language>en_US</language>
  <keyword>corrcoef</keyword>
  <short_description>Correlation coefficients</short_description>

  <syntax>
    <syntax_item>R = corrcoef(M)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>M</param_name>
      <param_description>a vector or matrix</param_description>
    </param_input_item>
  </param_input>

  <param_output>

    <param_output_item>
      <param_name>R</param_name>
      <param_description>Correlation coefficients of M.</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p><b
      >R = corrcoef(M)</b> returns the matrix of correlation coefficients for <b
      >M</b>, where the columns of <b
      >M</b> represent random variables and the rows represent observations.</p>
  </description>

  <used_function />
  <bibliography />

<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data>
      <![CDATA[M = [4 -7 3; 1 4 -2; 10 7 9];
R = corrcoef(M)]]>
    </example_item_data>
  </example_item>
</examples>

