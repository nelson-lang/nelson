as_uint64 = 18446744073709551615U;
max_uint64 = intmax('uint64');
assert_isequal(as_uint64, max_uint64);
//=============================================================================
A = 1;
B = 1U;
C = uint64(A);
assert_isequal(class(B), class(C));
assert_isequal(class(B), 'uint64');
assert_isequal(B, C);
