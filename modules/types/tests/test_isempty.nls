//=============================================================================
assert_isequal(nargin('isempty'), 1);
assert_isequal(nargout('isempty'), 1);
//=============================================================================
A = rand(3, 3, 3);
res = isempty(A);
assert_isfalse(res);
//=============================================================================
A(:, :, :) = [];
res = isempty(A);
assert_istrue(res);
//=============================================================================
B = {};
res = isempty(B);
assert_istrue(res);
//=============================================================================
C = struct();
res = isempty(C);
assert_isfalse(res);
//=============================================================================
C = struct([]);
res = isempty(C);
assert_istrue(res);
//=============================================================================
