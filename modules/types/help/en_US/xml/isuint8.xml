	<language>en_US</language>
	<keyword>isuint8</keyword>
	<short_description
  >Return true if variable var is an unsigned 8-bit integer type array.</short_description>

	<syntax>
		<syntax_item>res = isuint8(var)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>var</param_name>
			<param_description>a variable</param_description>
		</param_input_item>
	</param_input>

	<param_output>
		<param_output_item>
			<param_name>res</param_name>
			<param_description>a logical: true or false</param_description>
		</param_output_item>
	</param_output>

	<description><b>isuint8</b> returns a logical <b>1</b>if the argument is an <b
    >unsigned 8-bit</b> integer array and a logical <b
    >0</b> otherwise.</description>

	<used_function />
	<bibliography />

<examples>
	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[A = 3;
res = isuint8(A)]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[B = uint8(3);
res = isuint8(B)]]>
		</example_item_data>
	</example_item>
</examples>

