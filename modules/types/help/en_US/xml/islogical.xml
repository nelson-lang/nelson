  <language>en_US</language>
  <keyword>islogical</keyword>
  <short_description
  >Return true if variable var is a logical.</short_description>
  <syntax>
    <syntax_item>res = islogical(var)</syntax_item>
  </syntax>
  <param_input>
    <param_input_item>
      <param_name>var</param_name>
      <param_description>a variable</param_description>
    </param_input_item>
  </param_input>
  <param_output>
    <param_output_item>
      <param_name>res</param_name>
      <param_description>a logical: true or false</param_description>
    </param_output_item>
  </param_output>
  <description><b
    >islogical</b> returns a logical 1 if the argument is a logical array and a logical 0 otherwise.</description>
  <used_function />
  <bibliography />
  <examples>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[A = 1;
res = islogical(A)]]></example_item_data>
    </example_item>
    <example_item>
      <example_item_type>nelson</example_item_type>
      <example_item_description />
      <example_item_data
        runnable="cli"
      ><![CDATA[B = logical(1);
res = islogical(B)]]></example_item_data>
    </example_item>
  </examples>
