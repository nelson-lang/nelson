  <language>en_US</language>
  <keyword>isobject</keyword>
  <short_description
  >Return true if variable var is an object.</short_description>

  <syntax>
    <syntax_item>res = isobject(var)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>var</param_name>
      <param_description>a variable</param_description>
    </param_input_item>
  </param_input>

  <param_output>
    <param_output_item>
      <param_name>res</param_name>
      <param_description>a logical: true or false</param_description>
    </param_output_item>
  </param_output>

  <description><b
    >ishandle</b> returns a logical 1 if the argument is an object and a logical 0 otherwise.</description>

  <used_function />
  <bibliography />

<examples>
  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data
      ><![CDATA[A = 3;
res = isobject(A)

addpath([modulepath('overload', 'root'), '/examples/complex']);
A = complexObj(1, 2);
res = isobject(A)
]]>
    </example_item_data>
  </example_item>

</examples>

