<examples>
  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description></example_item_description>
    <example_item_data><![CDATA[A = 3;
res = isa(A, 'double')]]>
    </example_item_data>
  </example_item>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description></example_item_description>
    <example_item_data><![CDATA[B = {'NelSon', 3, true};
res = isa(B, 'cell')]]>
    </example_item_data>
  </example_item>

