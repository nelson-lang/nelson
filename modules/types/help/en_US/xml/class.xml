  <language>en_US</language>
  <keyword>class</keyword>
  <short_description
  >Return classname of object or creates a named object.</short_description>

  <syntax>
    <syntax_item>name = class(var)</syntax_item>
    <syntax_item>obj = class(st, strname)</syntax_item>
  </syntax>
  
  <param_input>
    <param_input_item>
      <param_name>var</param_name>
      <param_description>a variable</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>st</param_name>
      <param_description>a struct</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>strname</param_name>
      <param_description>a string: classname desired</param_description>
    </param_input_item>
  </param_input>

  <param_output>

    <param_output_item>
      <param_name>name</param_name>
      <param_description>a string</param_description>
    </param_output_item>

    <param_output_item>
      <param_name>obj</param_name>
      <param_description
      >an object of type 'strname' based on struct 'st'</param_description>
    </param_output_item>

  </param_output>

  <description><p><b
      >name = class(var)</b> returns the class of var variable.</p>
<p>Standard classes are:</p>
<p>'cell'</p>
<p>'struct'</p>
<p>'single'</p>
<p>'double'</p>
<p>'logical'</p>
<p>'char'</p>
<p>'int8'</p>
<p>'int16'</p>
<p>'int32'</p>
<p>'int64'</p>
<p>'uint8'</p>
<p>'uint16'</p>
<p>'uint32'</p>
<p>'uint64'</p>
<p>'function_handle'</p>
</description>

  <used_function />
  <bibliography />

<examples>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data><![CDATA[A = 3;
res = class(A)]]>
    </example_item_data>
  </example_item>

  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data><![CDATA[C = [1 ; 3];
res = class(C)]]>
    </example_item_data>
  </example_item>


  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data
      ><![CDATA[addpath([nelsonroot(), '/modules/overload/examples/complex']);
c = complexObj(3,4);
class(c)]]>
    </example_item_data>
  </example_item>


</examples>

