    <language>fr_FR</language>
    <keyword>getfield</keyword>
    <short_description
  >Renvoie la valeur d'un champ dans un struct.</short_description>
    <syntax>
        <syntax_item>value = getfield(st, field)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>st</param_name>
            <param_description>une structure.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>field</param_name>
            <param_description>une chaîne.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
        <param_name>value</param_name>
        <param_description
      >la valeur d'un champ d'une structure.</param_description>
        </param_output_item>
    </param_output>
                <description>
                <p>
                        <b
      >value = getfield(st, field)</b> renvoie la valeur du champ nommé <b
      >field</b> d'une structure.</p>
                </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[example.a = 1
example.b = 'nelson'
example.c = []
getfield(example, 'b')]]></example_item_data>
            </example_item>
        </examples>
