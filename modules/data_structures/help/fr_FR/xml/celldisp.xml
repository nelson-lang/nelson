    <language>fr_FR</language>
    <keyword>celldisp</keyword>
    <short_description
  >Afficher le contenu d'un tableau cellulaire.</short_description>
    <syntax>
        <syntax_item>celldisp(C)</syntax_item>
        <syntax_item>celldisp(C, name)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>C</param_name>
            <param_description>tableau cellulaire.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>name</param_name>
            <param_description
      >nom affiché du tableau cellulaire.</param_description>
        </param_input_item>
    </param_input>
    <description>
        <p>
            <b
      >celldisp</b> affiche récursivement le contenu d'un tableau cellulaire.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[C = {2, 22, 'ff', {331, 332}};
celldisp(C)
celldisp(C, 'var_name')]]></example_item_data>
            </example_item>
        </examples>
