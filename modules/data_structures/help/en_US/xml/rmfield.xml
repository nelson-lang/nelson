	<language>en_US</language>
	<keyword>rmfield</keyword>
	<short_description>Remove fields from structure.</short_description>

	<syntax>
		<syntax_item>s = rmfield(st, field)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>st</param_name>
			<param_description>a structure.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>field</param_name>
			<param_description>a string, cell of strings, or char.</param_description>
		</param_input_item>
	</param_input>

	<param_output>

		<param_output_item>
			<param_name>s</param_name>
			<param_description>a structure without field.</param_description>
		</param_output_item>
	</param_output>

	<description>
		<p><b
      >s = rmfield(st, field)</b> removes the specified field from structure array.</p>
</description>


	<used_function />
	<bibliography />

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data
      ><![CDATA[example.a = 1
example.b = 'nelson'
example.c = []
rmfield(example, 'b')]]>
		</example_item_data>
	</example_item>

</examples>

