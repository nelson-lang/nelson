	<language>en_US</language>
	<keyword>cellstr</keyword>
	<short_description>Converts to cell of character array.</short_description>

	<syntax>
		<syntax_item>ce = cellstr(A)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>A</param_name>
			<param_description>a string, a string array, cell of character array.</param_description>
		</param_input_item>
	</param_input>

	<param_output>
		<param_output_item>
			<param_name>ce</param_name>
			<param_description>a cell of character array</param_description>
		</param_output_item>
	</param_output>

	<description
		><p><b>cellstr(A)</b> converts to cell of character array.</p>
	</description>


	<used_function></used_function>
	<bibliography></bibliography>

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[cellstr('Nelson')]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[cellstr({'Nelson'})]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[cellstr({})]]>
		</example_item_data>
	</example_item>

</examples>

