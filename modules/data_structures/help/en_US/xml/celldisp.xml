	<language>en_US</language>
	<keyword>celldisp</keyword>
	<short_description>Display cell array contents.</short_description>

	<syntax>
		<syntax_item>celldisp(C)</syntax_item>
		<syntax_item>celldisp(C, name)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>C</param_name>
			<param_description>cell array.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>name</param_name>
			<param_description>displayed name of cell array.</param_description>
		</param_input_item>
	</param_input>

	<description>
		<p><b>celldisp</b> recursively display the contents of a cell array.</p>
	</description>


	<used_function></used_function>
	<bibliography></bibliography>

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description></example_item_description>
		<example_item_data><![CDATA[C = {2, 22, 'ff', {331, 332}};
celldisp(C)
celldisp(C, 'var_name')]]>
		</example_item_data>
	</example_item>

</examples>

