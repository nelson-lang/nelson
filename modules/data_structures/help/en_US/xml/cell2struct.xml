	<language>en_US</language>
	<keyword>cell2struct</keyword>
	<short_description>Creates a struct from a cell.</short_description>

	<syntax>
		<syntax_item>st = cell2struct(ce, fields)</syntax_item>
		<syntax_item>st = cell2struct(ce, fields, dim)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>ce</param_name>
			<param_description>a cell.</param_description>
		</param_input_item>
		
		<param_input_item>
			<param_name>fields</param_name>
			<param_description>a cell of strings.</param_description>
		</param_input_item>

		<param_input_item>
			<param_name>dim</param_name>
			<param_description>dimension along cell is converted.</param_description>
		</param_input_item>

	</param_input>

	<param_output>

		<param_output_item>
			<param_name>st</param_name>
			<param_description>a struct array.</param_description>
		</param_output_item>
	</param_output>

	<description>
		<p><b>st = cell2struct(ce, fields)</b> creates a struct from a cell.</p>
	</description>


	<used_function />
	<bibliography />

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data
      ><![CDATA[ce = {85, 50, 68; 'Pierre', 'Anna', 'Roberto'}
fields = {'Height','Name'}
A = cell2struct (ce, fields, 1)]]>
		</example_item_data>
	
	</example_item>

</examples>

