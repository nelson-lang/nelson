	<language>en_US</language>
	<keyword>struct2cell</keyword>
	<short_description>Creates a cell from a structure.</short_description>

	<syntax>
		<syntax_item>ce = struct2cell(st)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>st</param_name>
			<param_description>a structure.</param_description>
		</param_input_item>
	</param_input>

	<param_output>

		<param_output_item>
			<param_name>ce</param_name>
			<param_description>a cell.</param_description>
		</param_output_item>
	</param_output>

	<description>
		<p><b>ce = struct2cell(st)</b> returns a new cell from the structure.</p>
</description>


	<used_function />
	<bibliography />

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data
      ><![CDATA[names = {'Pierre', 'Anna', 'Roberto'}
values =  {45, 42, 13}
st = struct ('name', names, 'age', values);
ce = struct2cell(st)]]>
		</example_item_data>
	</example_item>

</examples>

