    <examples>
        <example_item>
            <example_item_type>nelson</example_item_type>
            <example_item_description
      >Surcharger l'opérateur minus pour double</example_item_description>
            <example_item_data
        runnable="false"
      ><![CDATA[% save in @double directory, as minus.m
function r = minus(A, B)
  disp('minus was called')
  % to call minus builtin
  r = builtin('minus', A, B)
end
]]></example_item_data>
        </example_item>
    </examples>
