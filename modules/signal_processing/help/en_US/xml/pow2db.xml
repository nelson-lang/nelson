  <language>en_US</language>
  <keyword>pow2db</keyword>
  <short_description>Convert power to decibel.</short_description>

  <syntax>
    <syntax_item>db = pow2db(pow)</syntax_item>
  </syntax>

  <param_input>
    <param_input_item>
      <param_name>pow</param_name>
      <param_description
      >input array: scalar, vector or matrix.</param_description>
    </param_input_item>
  </param_input>

  <param_output>
    <param_output_item>
      <param_name>db</param_name>
      <param_description>corresponding values in decibels</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p><b>db = pow2db(pow)</b> returns corresponding values in decibels.</p>
  </description>

<examples>
  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data><![CDATA[DB = pow2db([1, 0.01])]]>
    </example_item_data>
  </example_item>
</examples>

