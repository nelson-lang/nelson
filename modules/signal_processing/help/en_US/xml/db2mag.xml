  <language>en_US</language>
  <keyword>db2mag</keyword>
  <short_description
  >Convert a gain in decibels (dB) to a magnitude.</short_description>

  <syntax>
    <syntax_item>mag = db2mag(db)</syntax_item>
  </syntax>

  <param_input>
    <param_input_item>
      <param_name>db</param_name>
      <param_description
      >input array: scalar, vector or matrix.</param_description>
    </param_input_item>
  </param_input>

  <param_output>
    <param_output_item>
      <param_name>mag</param_name>
      <param_description>corresponding magnitude</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p><b>mag = db2mag(db)</b> returns corresponding magnitude.</p>
  </description>

<examples>
  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data><![CDATA[mag = db2mag([0, -20])]]>
    </example_item_data>
  </example_item>
</examples>

