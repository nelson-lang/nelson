  <language>en_US</language>
  <keyword>bartlett</keyword>
  <short_description>Bartlett window.</short_description>

  <syntax>
    <syntax_item>c = bartlett(m)</syntax_item>
  </syntax>

  <param_input>
    <param_input_item>
      <param_name>m</param_name>
      <param_description>positive integer: window length</param_description>
    </param_input_item>
  </param_input>

  <param_output>
    <param_output_item>
      <param_name>c</param_name>
      <param_description>column vector</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p><b>c = bartlett(m)</b> an L-point symmetric Bartlett window.</p>
  </description>

<examples>
  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description></example_item_description>
    <example_item_data><![CDATA[c = bartlett(8)]]>
    </example_item_data>
  </example_item>
</examples>

