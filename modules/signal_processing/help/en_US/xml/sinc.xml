  <language>en_US</language>
  <keyword>sinc</keyword>
  <short_description>Sinc function.</short_description>

  <syntax>
    <syntax_item>c = sinc(m)</syntax_item>
  </syntax>

  <param_input>
    <param_input_item>
      <param_name>m</param_name>
      <param_description
      >input array: scalar, vector or matrix.</param_description>
    </param_input_item>
  </param_input>

  <param_output>
    <param_output_item>
      <param_name>c</param_name>
      <param_description>sinc of input</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p><b>c = sinc(m)</b> returns an array <b
      >c</b> whose elements are the sinc of the elements of the input: <b
      >m</b>.</p>
  </description>

<examples>
  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data><![CDATA[c = sinc(pi)]]>
    </example_item_data>
  </example_item>
</examples>

