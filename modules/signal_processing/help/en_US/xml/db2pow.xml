  <language>en_US</language>
  <keyword>db2pow</keyword>
  <short_description
  >Convert a gain in decibels (dB) to power.</short_description>

  <syntax>
    <syntax_item>pow = db2pow(db)</syntax_item>
  </syntax>

  <param_input>
    <param_input_item>
      <param_name>db</param_name>
      <param_description
      >input array: scalar, vector or matrix.</param_description>
    </param_input_item>
  </param_input>

  <param_output>
    <param_output_item>
      <param_name>pow</param_name>
      <param_description>corresponding power</param_description>
    </param_output_item>
  </param_output>

  <description>
    <p><b>pow = db2pow(db)</b> returns corresponding power.</p>
  </description>

<examples>
  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data><![CDATA[pow = db2pow([0, -20])]]>
    </example_item_data>
  </example_item>
</examples>

