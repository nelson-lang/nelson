    <language>fr_FR</language>
    <keyword>mag2db</keyword>
    <short_description
  >Convertit une magnitude en décibels (dB).</short_description>
    <syntax>
        <syntax_item>db = mag2db(mag)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
        <param_name>mag</param_name>
        <param_description
      >tableau d'entrée : scalaire, vecteur ou matrice.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
        <param_name>db</param_name>
        <param_description
      >valeurs correspondantes en décibels</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
        <b
      >db = mag2db(mag)</b> renvoie les valeurs correspondantes en décibels.</p>
        </description>
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                                <example_item_data
        runnable="cli"
      ><![CDATA[DB = mag2db([1, 0.01])]]></example_item_data>
            </example_item>
        </examples>
        <see_also>
            <see_also_item>
                <link linkend="${signal_processing}db2mag">db2mag</link>
            </see_also_item>
        </see_also>
