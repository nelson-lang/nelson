        <language>fr_FR</language>
        <keyword>db2mag</keyword>
        <short_description
  >Convertit un gain en décibels (dB) en magnitude.</short_description>
    <syntax>
        <syntax_item>mag = db2mag(db)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>db</param_name>
            <param_description
      >tableau d'entrée : scalaire, vecteur ou matrice.</param_description>
        </param_input_item>
    </param_input>
    <param_output>
        <param_output_item>
            <param_name>mag</param_name>
            <param_description>magnitude correspondante</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>mag = db2mag(db)</b> renvoie la magnitude correspondante.</p>
        </description>
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[mag = db2mag([0, -20])]]></example_item_data>
            </example_item>
        </examples>
