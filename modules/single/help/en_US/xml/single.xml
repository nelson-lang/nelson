	<language>en_US</language>
	<keyword>single</keyword>
	<short_description
  >Converts a variable to single precision type.</short_description>

	<syntax>
		<syntax_item>S = single(V)</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>V</param_name>
			<param_description>a variable.</param_description>
		</param_input_item>
	</param_input>

	<param_output>

		<param_output_item>
			<param_name>S</param_name>
			<param_description>a single.</param_description>
		</param_output_item>
	</param_output>

	<description>
		<p><b>single(V)</b> converts to the single-precision type.</p>
</description>


	<used_function />
	<bibliography />

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[single('Nelson')]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[A = single(pi)]]>
		</example_item_data>
	</example_item>


</examples>

