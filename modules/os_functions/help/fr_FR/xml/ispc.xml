    <language>fr_FR</language>
    <keyword>ispc</keyword>
    <short_description
  >Vérifie si la version est pour la plateforme Windows.</short_description>
    <syntax>
        <syntax_item>s = ispc()</syntax_item>
    </syntax>
    <param_output>
        <param_output_item>
            <param_name>s</param_name>
            <param_description
      >un booléen : vrai si la plateforme est Windows.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p><b>ispc</b> vérifie si la plateforme est Windows.</p>
    </description>
    <used_function />
    <bibliography />
    <examples>
        <example_item>
            <example_item_type>nelson</example_item_type>
            <example_item_description />
            <example_item_data
        runnable="cli"
      ><![CDATA[if ispc
  disp('Your platform is Windows')
else
  disp('Your platform is not Windows')
end]]></example_item_data>
        </example_item>
    </examples>
