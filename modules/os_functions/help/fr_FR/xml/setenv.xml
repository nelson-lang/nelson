        <language>fr_FR</language>
        <keyword>setenv</keyword>
        <short_description
  >Définir la valeur d'une variable d'environnement.</short_description>
    <syntax>
        <syntax_item>setenv(env_name, env_value)</syntax_item>
    </syntax>
    <param_input>
        <param_input_item>
            <param_name>env_name</param_name>
            <param_description
      >une chaîne : nom de la variable d'environnement.</param_description>
        </param_input_item>
        <param_input_item>
            <param_name>env_value</param_name>
            <param_description
      >une chaîne : valeur de la variable d'environnement.</param_description>
        </param_input_item>
    </param_input>
    <description>
        <p><b>setenv</b> définit la valeur d'une variable d'environnement.</p>
    </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[setenv('MY_ENV_VAR', 'funvalue')
getenv('MY_ENV_VAR')
setenv('MY_ENV_VAR', '')
getenv('MY_ENV_VAR')]]></example_item_data>
            </example_item>
        </examples>
