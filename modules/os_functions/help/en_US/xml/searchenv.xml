  <language>en_US</language>
  <keyword>searchenv</keyword>
  <short_description>Searches for a file using environment paths.</short_description>

  <syntax>
    <syntax_item>c = searchenv(filename, env_name)</syntax_item>
  </syntax>

  <param_input>
    <param_input_item>
      <param_name>env_name</param_name>
      <param_description>a string: environment variable name.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>filename</param_name>
      <param_description>a string: filename searched in environment variable.</param_description>
    </param_input_item>
  </param_input>


  <param_output>
    <param_output_item>
      <param_name>c</param_name>
      <param_description>a cell of strings: full paths found in environment variable.</param_description>
    </param_output_item>
  </param_output>


  <description><p><b>searchenv</b> Searches for a file using environment paths.</p>
  </description>

  <used_function></used_function>
  <bibliography></bibliography>

<examples>
  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description></example_item_description>
    <example_item_data><![CDATA[[modules, paths] = getmodules();
env_value = '';
for p = paths
 env_value = [env_value, pathsep, p];
end

setenv('MY_PATH_ENV', env_value);
c = searchenv('loader.m', 'MY_PATH_ENV')]]>
    </example_item_data>
  </example_item>

</examples>

