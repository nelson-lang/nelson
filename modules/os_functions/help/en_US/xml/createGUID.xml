  <language>en_US</language>
  <keyword>createGUID</keyword>
  <short_description>Creates a GUID.</short_description>

  <syntax>
    <syntax_item>s = createGUID()</syntax_item>
    <syntax_item>c = createGUID(numbers_of_GUID)</syntax_item>
  </syntax>

  <param_input>
    <param_input_item>
      <param_name>numbers_of_GUID</param_name>
      <param_description
      >an integer value: numbers of GUID to create.</param_description>
    </param_input_item>
  </param_input>

  <param_output>
  	
    <param_output_item>
      <param_name>s</param_name>
      <param_description>a string</param_description>
    </param_output_item>

    <param_output_item>
      <param_name>c</param_name>
      <param_description>a cell of strings.</param_description>
    </param_output_item>
    
  </param_output>


  <description><p><b
      >createGUID</b> creates a Globally Unique IDentifier (GUID), , a unique 128-bit integer used for CLSIDs and interface identifiers.</p>
  </description>

  <used_function />
  <bibliography />

