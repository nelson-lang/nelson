  <language>en_US</language>
  <keyword>setenv</keyword>
  <short_description
  >Set the value of an environment variable.</short_description>

  <syntax>
    <syntax_item>getenv(env_name, env_value)</syntax_item>
  </syntax>

  <param_input>
    <param_input_item>
      <param_name>env_name</param_name>
      <param_description
      >a string: environment variable name.</param_description>
    </param_input_item>
    <param_input_item>
      <param_name>env_value</param_name>
      <param_description
      >a string: environment variable value.</param_description>
    </param_input_item>
  </param_input>


  <description><p><b>setenv</b> set the value of an environment variable.</p>
  </description>

  <used_function />
  <bibliography />

<examples>
  <example_item>
    <example_item_type>nelson</example_item_type>
    <example_item_description />
    <example_item_data
      ><![CDATA[setenv('MY_ENV_VAR', 'funvalue')
getenv('MY_ENV_VAR')
setenv('MY_ENV_VAR', '')
getenv('MY_ENV_VAR')]]>
    </example_item_data>
  </example_item>

</examples>

