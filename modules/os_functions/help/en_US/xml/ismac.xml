    <language>en_US</language>
    <keyword>ismac</keyword>
    <short_description
  >Checks if version is for MacOS platform.</short_description>
    <syntax>
        <syntax_item>s = ismac()</syntax_item>
    </syntax>
    <param_output>
        <param_output_item>
            <param_name>s</param_name>
            <param_description
      >a logical: true if it is a MacOS platform.</param_description>
        </param_output_item>
    </param_output>
    <description>
        <p>
            <b>ismac</b> checks if it is a MacOs platform.</p>
        </description>
        <used_function />
        <bibliography />
        <examples>
            <example_item>
                <example_item_type>nelson</example_item_type>
                <example_item_description />
                <example_item_data
        runnable="cli"
      ><![CDATA[if ismac
  disp('Your platform is MacOs')
else
  disp('Your platform is not MacOs')
end]]></example_item_data>
            </example_item>
        </examples>
