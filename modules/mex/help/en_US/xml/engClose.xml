	<language>en_US</language>
	<keyword>engClose</keyword>
	<short_description>Close Nelson engine session</short_description>
	<syntax>
		<syntax_item>#include "engine.h"</syntax_item>
		<syntax_item>int engClose(Engine *ep);</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>Engine *ep</param_name>
			<param_description>handle to Nelson engine.</param_description>
		</param_input_item>
	</param_input>

  <param_output>
    <param_output_item>
      <param_name>int</param_name>
      <param_description>0 on success and 1 on failure.</param_description>
    </param_output_item>
  </param_output>

	<description>
		<p>engClose closes engine session and terminates the connection.</p>
	</description>

