	<language>en_US</language>
	<keyword>engPutVariable</keyword>
	<short_description>Put variable into Nelson engine workspace</short_description>
	<syntax>
		<syntax_item>#include "engine.h"</syntax_item>
		<syntax_item
    >int engPutVariable(Engine *ep, const char *name, const mxArray *pm);</syntax_item>
	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>Engine *ep</param_name>
			<param_description>handle to Nelson engine.</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>const char *name</param_name>
			<param_description
      >name of mxArray in the Nelson workspace (base scope).</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>const mxArray *pm</param_name>
			<param_description>Pointer to mxArray.</param_description>
		</param_input_item>
	</param_input>

  <param_output>
    <param_output_item>
      <param_name>int</param_name>
      <param_description
      >0 if successful or 1 if an error occurs.</param_description>
    </param_output_item>
  </param_output>

	<description>
		<p>Put variable into Nelson engine workspace.</p>
	</description>

