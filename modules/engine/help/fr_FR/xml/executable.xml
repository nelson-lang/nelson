      ><![CDATA[nelson-adv-cli -q -e "a = 1 + 2"]]></example_item_data>
                    </example_item>
                    <example_item>
                        <example_item_type>nelson</example_item_type>
                        <example_item_description />
                        <example_item_data
      ><![CDATA[nelson-gui -v]]></example_item_data>
                    </example_item>
                    <example_item>
                        <example_item_type>nelson</example_item_type>
                        <example_item_description />
                        <example_item_data
      ><![CDATA[nelson-gui --help]]></example_item_data>
                    </example_item>
                </examples>
