	<language>en_US</language>
	<keyword>zeros</keyword>
	<short_description>Creates an matrix made of zeros.</short_description>

	<syntax>
		<syntax_item>R = zeros</syntax_item>
		<syntax_item>R = zeros(n)</syntax_item>
		<syntax_item>R = zeros(n, m)</syntax_item>
		<syntax_item>R = zeros(n, m, ..., z)</syntax_item>
		<syntax_item>R = zeros(n, m, ..., z, 'like', V)</syntax_item>
		<syntax_item>R = zeros(n, m, ..., z, classname)</syntax_item>

	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>n</param_name>
			<param_description>a variable</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>m</param_name>
			<param_description>a variable</param_description>
		</param_input_item>
	</param_input>


	<description><p><b>zeros</b> returns a matrix made of zeros.</p>
</description>

	<used_function />
	<bibliography />
	

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[zeros(3, 2)]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[zeros(3, 1, 3, 'single')]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[A = single([3 3])
B = zeros(2, 4, 'like', A)]]>
		</example_item_data>
	</example_item>


	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data
      ><![CDATA[tic(); single(1) * zeros(1000); toc()
tic();zeros(1000, 'single'); toc()]]>
		</example_item_data>
	</example_item>


</examples>

