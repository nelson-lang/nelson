	<language>en_US</language>
	<keyword>ones</keyword>
	<short_description>Creates an matrix made of ones.</short_description>

	<syntax>
		<syntax_item>R = ones</syntax_item>
		<syntax_item>R = ones(n)</syntax_item>
		<syntax_item>R = ones(n, m)</syntax_item>
		<syntax_item>R = ones(n, m, ..., z)</syntax_item>
		<syntax_item>R = ones(n, m, ..., z, 'like', V)</syntax_item>
		<syntax_item>R = ones(n, m, ..., z, classname)</syntax_item>

	</syntax>
	
	<param_input>
		<param_input_item>
			<param_name>n</param_name>
			<param_description>a variable: n-by-n matrix</param_description>
		</param_input_item>
		<param_input_item>
			<param_name>m</param_name>
			<param_description>a variable: n-by-m matrix</param_description>
		</param_input_item>
	</param_input>


	<description><p><b>ones</b> returns a matrix made of ones.</p>
</description>

	<used_function />
	<bibliography />
	

<examples>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[ones(3,2)]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[ones(3,1,3,'single')]]>
		</example_item_data>
	</example_item>

	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data><![CDATA[A = single([3 3])
B = ones(2,4,'like', A)]]>
		</example_item_data>
	</example_item>


	<example_item>
		<example_item_type>nelson</example_item_type>
		<example_item_description />
		<example_item_data
      ><![CDATA[tic(); single(1) * ones(1000); toc()
tic();ones(1000,'single'); toc()]]>
		</example_item_data>
	</example_item>


</examples>

